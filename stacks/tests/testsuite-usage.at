m4_divert_push([DEFAULTS])
function test_usage() {
    export MIBDIRS="${at_top_srcdir}/src/snmp/mibs:/usr/share/snmp/mibs"
    export MIBS=+OPENSS7-CDR2-MIB
    snmpinform -v $1 -c "$2" localhost:13001 ${@+2}
}
function test_usage_root() {
    export MIBDIRS="${at_top_srcdir}/src/snmp/mibs:/usr/share/snmp/mibs"
    export MIBS=+OPENSS7-CDR2-MIB
    snmpinform -v $1 -c "$2" localhost:162 ${@+2}
}
m4_divert_pop([DEFAULTS])

AT_TESTED([usage snmpinform snmptrap])
AT_BANNER([USAGE CALL DETAIL RECORDING tests])

AT_SETUP([usage test 1.1])
AT_KEYWORDS([usage])
AT_CHECK([
], [], [ignore], [], [], [])
AT_CLEANUP

