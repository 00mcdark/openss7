	indent -nhnl sccpMIB_simple.[ch]

sccpOmMIB_simple.c sccpOmMIB_simple.h: MIBS+=OPENSS7-SCCP-OM-MIB:OPENSS7-SCCP-MIB:OPENSS7-TC:PerfHist-TC-MIB:HC-PerfHist-TC-MIB
sccpOmMIB_simple.c sccpOmMIB_simple.h: mib2c mib2c.conf mibs/OPENSS7-SCCP-OM-MIB.mib
	./mib2c -c ./mib2c.conf -f sccpOmMIB_simple sccpOmMIB
	indent -nhnl sccpOmMIB_simple.[ch]

tcapMIB_simple.c tcapMIB_simple.h: MIBS+=OPENSS7-TCAP-MIB:OPENSS7-TC
tcapMIB_simple.c tcapMIB_simple.h: mib2c mib2c.conf mibs/OPENSS7-TCAP-MIB.mib
	./mib2c -c ./mib2c.conf -f tcapMIB_simple tcapMIB
	indent -nhnl tcapMIB_simple.[ch]

tcapOmMIB_simple.c tcapOmMIB_simple.h: MIBS+=OPENSS7-TCAP-OM-MIB:OPENSS7-TC
tcapOmMIB_simple.c tcapOmMIB_simple.h: mib2c mib2c.conf mibs/OPENSS7-TCAP-OM-MIB.mib
	./mib2c -c ./mib2c.conf -f tcapOmMIB_simple tcapOmMIB
	indent -nhnl tcapOmMIB_simple.[ch]

storage: \
	x400pMIB_storage.c x400pMIB_storage.h \
	ss7OmMIB_storage.c ss7OmMIB_storage.h \
	slMIB_storage.c slMIB_storage.h \
	mtpMIB_storage.c mtpMIB_storage.h \
	mtpOmMIB_storage.c mtpOmMIB_storage.h \
	sccpMIB_storage.c sccpMIB_storage.h \
	sccpOmMIB_storage.c sccpOmMIB_storage.h \
	tcapOmMIB_storage.c tcapOmMIB_storage.h \
	tcapMIB_storage.c tcapMIB_storage.h

cdr2MIB_storage.c cdr2MIB_storage.h: MIBS+=OPENSS7-CDR2-MIB:OPENSS7-TC
cdr2MIB_storage.c cdr2MIB_storage.h: mib2c mib2c.storage.conf mibs/OPENSS7-CDR2-MIB.mib
	./mib2c -d -c ./mib2c.storage.conf -f cdr2MIB_storage cdr2MIB
	indent -nhnl cdr2MIB_storage.[ch]

x400pMIB_storage.c x400pMIB_storage.h: MIBS+=OPENSS7-X400P-MIB:OPENSS7-SMI-MIB:OPENSS7-TC
