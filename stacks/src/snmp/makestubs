MIBS=
MIBDIRS=./mibs:/usr/share/snmp/mibs
MIBFILES=

export MIBS
export MIBDIRS
export MIBFILES

all: basic storage openss7

basic: \
	cdr2MIB_simple.c cdr2MIB_simple.h

cdr2MIB_simple.c cdr2MIB_simple.h: MIBS=+OPENSS7-CDR2-MIB
cdr2MIB_simple.c cdr2MIB_simple.h: mib2c mib2c.conf mibs/
	./mib2c -c ./mib2c.conf -f cdr2MIB_simple cdr2MIB
	indent -nhnl cdr2MIB_simple.[ch]

storage: \
	cdr2MIB_storage.c cdr2MIB_storage.h

cdr2MIB_storage.c cdr2MIB_storage.h: MIBS+=OPENSS7-CDR2-MIB
cdr2MIB_storage.c cdr2MIB_storage.h: mib2c mib2c.storage.conf mibs/OPENSS7-CDR2-MIB.mib
	./mib2c -c ./mib2c.storage.conf -f cdr2MIB_storage cdr2MIB
	indent -nhnl cdr2MIB_storage.[ch]

openss7: \
	cdr2MIB_openss7.c cdr2MIB_openss7.h

cdr2MIB_openss7.c cdr2MIB_openss7.h: MIBS+=OPENSS7-CDR2-MIB
cdr2MIB_openss7.c cdr2MIB_openss7.h: mib2c mib2c.openss7.conf mibs/OPENSS7-CDR2-MIB.mib
	./mib2c -c ./mib2c.openss7.conf -f cdr2MIB_openss7 cdr2MIB
	indent -nhnl cdr2MIB_openss7.[ch]

tables:
	./mib2c -c ./mib2c.storage.conf x400pSyncTable
	./mib2c -c ./mib2c.storage.conf x400pCardTable
	./mib2c -c ./mib2c.storage.conf x400pSpanTable
	./mib2c -c ./mib2c.storage.conf x400pChanTable
	indent -nhnl str*Table.[ch]

clean:
	rm -f cdr2MIB_simple.[ch]
	rm -f cdr2MIB_storage.[ch]
	rm -f cdr2MIB_openss7.[ch]
	rm -f cdr2MIB_simple.[ch]~
	rm -f cdr2MIB_storage.[ch]~
	rm -f cdr2MIB_openss7.[ch]~
