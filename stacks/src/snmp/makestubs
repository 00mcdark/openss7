	indent -nhnl mtpOmMIB_simple.[ch]

sccpMIB_simple.c sccpMIB_simple.h: MIBS+=OPENSS7-SCCP-MIB:OPENSS7-TC
sccpMIB_simple.c sccpMIB_simple.h: mib2c mib2c.conf mibs/OPENSS7-SCCP-MIB.mib
