MIBS=
MIBDIRS=./mibs:/usr/share/snmp/mibs
MIBFILES=

export MIBS
export MIBDIRS
export MIBFILES

all: storage openss7

check: \
	cdr2MIB.tree \
	x400pMIB.tree \
	ss7MIB.tree \
	ss7OmMIB.tree \
	slMIB.tree \
	mtpMIB.tree \
	mtpOmMIB.tree \
	sccpMIB.tree \
	sccpOmMIB.tree \
	tcapMIB.tree \
	tcapOmMIB.tree

cdr2MIB.tree: MIBS+=OPENSS7-CDR2-MIB:OPENSS7-TC
cdr2MIB.tree: mibs/OPENSS7-CDR2-MIB.mib
	snmptranslate -Tp OPENSS7-CDR2-MIB::cdr2MIB >cdr2MIB.tree

x400pMIB.tree: MIBS+=OPENSS7-X400P-MIB:OPENSS7-SMI-MIB:OPENSS7-TC
x400pMIB.tree: mibs/OPENSS7-X400P-MIB.mib
	snmptranslate -Tp OPENSS7-X400P-MIB::x400pMIB >x400pMIB.tree

ss7MIB.tree: MIBS+=OPENSS7-SS7-MIB:OPENSS7-TC
ss7MIB.tree: mibs/OPENSS7-SS7-MIB.mib
	snmptranslate -Tp OPENSS7-SS7-MIB::ss7MIB >ss7MIB.tree

ss7OmMIB.tree: MIBS+=OPENSS7-SS7-OM-MIB:OPENSS7-TC:PerfHist-TC-MIB:HC-PerfHist-TC-MIB
ss7OmMIB.tree: mibs/OPENSS7-SS7-OM-MIB.mib
	snmptranslate -Tp OPENSS7-SS7-OM-MIB::ss7OmMIB >ss7OmMIB.tree

slMIB.tree: MIBS+=OPENSS7-SL-MIB:OPENSS7-TC:PerfHist-TC-MIB:HC-PerfHist-TC-MIB
slMIB.tree: mibs/OPENSS7-SL-MIB.mib
	snmptranslate -Tp OPENSS7-SL-MIB::slMIB >slMIB.tree

mtpMIB.tree: MIBS+=OPENSS7-MTP-MIB:OPENSS7-TC:PerfHist-TC-MIB:HC-PerfHist-TC-MIB
mtpMIB.tree: mibs/OPENSS7-MTP-MIB.mib
	snmptranslate -Tp OPENSS7-MTP-MIB::mtpMIB >mtpMIB.tree

mtpOmMIB.tree: MIBS+=OPENSS7-MTP-OM-MIB2:OPENSS7-TC:PerfHist-TC-MIB:HC-PerfHist-TC-MIB
mtpOmMIB.tree: mibs/OPENSS7-MTP-OM-MIB2.mib
	snmptranslate -Tp OPENSS7-MTP-OM-MIB2::mtpOmMIB >mtpOmMIB.tree

sccpMIB.tree: MIBS+=OPENSS7-SCCP-MIB:OPENSS7-TC
sccpMIB.tree: mibs/OPENSS7-SCCP-MIB.mib
	snmptranslate -Tp OPENSS7-SCCP-MIB::sccpMIB >sccpMIB.tree

sccpOmMIB.tree: MIBS+=OPENSS7-SCCP-OM-MIB:OPENSS7-SCCP-MIB:OPENSS7-TC:PerfHist-TC-MIB:HC-PerfHist-TC-MIB
sccpOmMIB.tree: mibs/OPENSS7-SCCP-OM-MIB.mib
	snmptranslate -Tp OPENSS7-SCCP-OM-MIB::sccpOmMIB >sccpOmMIB.tree

tcapMIB.tree: MIBS+=OPENSS7-TCAP-MIB:OPENSS7-TC
tcapMIB.tree: mibs/OPENSS7-TCAP-MIB.mib
	snmptranslate -Tp OPENSS7-TCAP-MIB::tcapMIB >tcapMIB.tree

tcapOmMIB.tree: MIBS+=OPENSS7-TCAP-OM-MIB:OPENSS7-TC
tcapOmMIB.tree: mibs/OPENSS7-TCAP-OM-MIB.mib
	snmptranslate -Tp OPENSS7-TCAP-OM-MIB::tcapOmMIB >tcapOmMIB.tree

basic: \
	cdr2MIB_simple.c cdr2MIB_simple.h \
	x400pMIB_simple.c x400pMIB_simple.h \
	ss7MIB_simple.c ss7MIB_simple.h \
	ss7OmMIB_simple.c ss7OmMIB_simple.h \
	slMIB_simple.c slMIB_simple.h \
	mtpMIB_simple.c mtpMIB_simple.h \
	mtpOmMIB_simple.c mtpOmMIB_simple.h \
	sccpMIB_simple.c sccpMIB_simple.h \
	sccpOmMIB_simple.c sccpOmMIB_simple.h \
	tcapMIB_simple.c tcapMIB_simple.h \
	tcapOmMIB_simple.c tcapOmMIB_simple.h

cdr2MIB_simple.c cdr2MIB_simple.h: MIBS+=OPENSS7-CDR2-MIB:OPENSS7-TC
cdr2MIB_simple.c cdr2MIB_simple.h: mib2c mib2c.conf mibs/OPENSS7-CDR2-MIB.mib
	./mib2c -c ./mib2c.conf -f cdr2MIB_simple cdr2MIB
	indent -nhnl cdr2MIB_simple.[ch]

x400pMIB_simple.c x400pMIB_simple.h: MIBS+=OPENSS7-X400P-MIB:OPENSS7-SMI-MIB:OPENSS7-TC
x400pMIB_simple.c x400pMIB_simple.h: mib2c mib2c.conf mibs/OPENSS7-X400P-MIB.mib
	./mib2c -c ./mib2c.conf -f x400pMIB_simple x400pMIB
	indent -nhnl x400pMIB_simple.[ch]

ss7MIB_simple.c ss7MIB_simple.h: MIBS+=OPENSS7-SS7-MIB:OPENSS7-TC
ss7MIB_simple.c ss7MIB_simple.h: mib2c mib2c.conf mibs/OPENSS7-SS7-MIB.mib
	./mib2c -c ./mib2c.conf -f ss7MIB_simple ss7MIB
	indent -nhnl ss7MIB_simple.[ch]

ss7OmMIB_simple.c ss7OmMIB_simple.h: MIBS+=OPENSS7-SS7-OM-MIB:OPENSS7-TC:PerfHist-TC-MIB:HC-PerfHist-TC-MIB
ss7OmMIB_simple.c ss7OmMIB_simple.h: mib2c mib2c.conf mibs/OPENSS7-SS7-OM-MIB.mib
	./mib2c -c ./mib2c.conf -f ss7OmMIB_simple ss7OmMIB
	indent -nhnl ss7OmMIB_simple.[ch]

slMIB_simple.c slMIB_simple.h: MIBS+=OPENSS7-SL-MIB:OPENSS7-TC:PerfHist-TC-MIB:HC-PerfHist-TC-MIB
slMIB_simple.c slMIB_simple.h: mib2c mib2c.conf mibs/OPENSS7-SL-MIB.mib
	./mib2c -c ./mib2c.conf -f slMIB_simple slMIB
	indent -nhnl slMIB_simple.[ch]

mtpMIB_simple.c mtpMIB_simple.h: MIBS+=OPENSS7-MTP-MIB:OPENSS7-TC:PerfHist-TC-MIB:HC-PerfHist-TC-MIB
mtpMIB_simple.c mtpMIB_simple.h: mib2c mib2c.conf mibs/OPENSS7-MTP-MIB.mib
	./mib2c -c ./mib2c.conf -f mtpMIB_simple mtpMIB
	indent -nhnl mtpMIB_simple.[ch]

mtpOmMIB_simple.c mtpOmMIB_simple.h: MIBS+=OPENSS7-MTP-OM-MIB2:OPENSS7-TC:PerfHist-TC-MIB:HC-PerfHist-TC-MIB
mtpOmMIB_simple.c mtpOmMIB_simple.h: mib2c mib2c.conf mibs/OPENSS7-MTP-OM-MIB2.mib
