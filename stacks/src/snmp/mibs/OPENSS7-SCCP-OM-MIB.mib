-- SCCP OM 5-minute SSN indexed History Table

sccpOm5MinSsnHistoryTable OBJECT-TYPE
    SYNTAX SEQUENCE OF SccpOm5MinSsnHistoryEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
	""
    REFERENCE
	""
    ::= { sccpOmTables 10 }

sccpOm5MinSsnHistoryEntry OBJECT-TYPE
    SYNTAX SccpOm5MinSsnHistoryEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
	""
    REFERENCE
	""
    INDEX   {
		sccpNetworkEntityId,	-- shared index
		sccpSsn,		-- shared index
		sccpOm5MinSsnInterval
	    }
    ::= { sccpOm5MinSsnHistoryTable 1 }

SccpOm5MinSsnHistoryEntry ::=
    SEQUENCE {
	sccpOm5MinSsnInterval				Integer32,
	sccpOm5MinSsnIntervalValid			BITS,
	sccpOm5MinDT1MessagesReceived			HCPerfIntervalCount,
	sccpOm5MinDT1MessagesSent			HCPerfIntervalCount,
	sccpOm5MinDT2MessagesReceived			HCPerfIntervalCount,
	sccpOm5MinDT2MessagesSent			HCPerfIntervalCount,
	sccpOm5MinEDMessagesSent			HCPerfIntervalCount,
	sccpOm5MinEDMessagesReceived			HCPerfIntervalCount
    }

sccpOm5MinSsnInterval OBJECT-TYPE
    SYNTAX Integer32 (1..288)
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
	"Provides the least-significant index component that specifies 
	 the historical collection interval for the SCCP network entity 
	 and subsystem number (SSN) for which historical data is 
	 retained.  Interval '1' is the most recent complete and 
	 historic 5-minute collection interval; interval '288' is the 
	 oldest historic 5-minute collection interval.  Sufficient 
	 intervals are provided to retain one day of historical 5-minute 
	 collection intervals."
    ::= { sccpOm5MinSsnHistoryEntry 1 }

sccpOm5MinSsnIntervalValid OBJECT-TYPE
    SYNTAX BITS {
		invalid(0),
		dT1MessagesReceived(1),
		dT1MessagesSent(2),
		dT2MessagesReceived(3),
		dT2MessagesSent(4),
		eDMessagesSent(5),
		eDMessagesReceived(6)
	    }
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
	"This set-valued object indicates the validity of the conceptual 
	 row as well each field in the conceptual row.  When invalid(0) 
	 is included, it indicates that the entire row contains no valid 
	 information.  When invalid(0) is not included, the remaining 
	 set values indicate the validity of each field in the 
	 conceptual row.  For example, when dT1MessagesReceived(1) is 
	 included in the set, it indicates that the 
	 sccpOm5MinDT1MessagesReceived columnar object in the conceptual 
	 row is valid."
    ::= { sccpOm5MinSsnHistoryEntry 2 }

sccpOm5MinDT1MessagesReceived OBJECT-TYPE
    SYNTAX HCPerfIntervalCount
    UNITS "Messages/SCCP/SSN/30min"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
	"Provides a historical count of the number of data type 1 (DT1) 
	 messages received by the indexed SCCP and SSN during the 
	 indexed 5-minute interval."
    ::= { sccpOm5MinSsnHistoryEntry 3 }

sccpOm5MinDT1MessagesSent OBJECT-TYPE
