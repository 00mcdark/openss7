    REFERENCE
	"ITU-T Q.752 (1997) Table 7 (7.17).
	 ITU-T Q.751.2 (06/1997) ss7OnOccurEvent."
    ::= { sccpOmOblMessageFailuresEntry 21 }

sccpOmProviderInitiatedRelease OBJECT-TYPE
    SYNTAX PerfCurrentCount
    UNITS "Events/SCCP"
