mtpSap2AddressTable OBJECT-TYPE
    SYNTAX SEQUENCE OF MtpSap2AddressEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
	"Provides a side table for the mtpAccessPointTable that contains 
	 the SAP addresses associated with an mtpAccessPoint."
    REFERENCE
	"ITU-T Rec. Q.751.1 (1997), ANSI T1.116.1 (2000), mtpAccessPoint 
	 MOC, sap2Address attribute."
    ::= { mtpSap 2 }

mtpSap2AddressEntry OBJECT-TYPE
    SYNTAX MtpSap2AddressEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
	"Each entry in the table corresponds to one sap2Address 
	 component for a given mtpAccessPoint.  The associated 
	 mtpAccessPoint is indexed by the sapId index.  The components 
	 are indexed by the mtpSap2AddressId index, which is a simple 
	 ordinal number from 1 through the number of sap2Address 
	 components."
    REFERENCE
	"ITU-T Rec. Q.751.1 (1997), ANSI T1.116.1 (2000), mtpAccessPoint 
	 MO, sap2Address attribute."
    INDEX {
	mtpL3Id,
	mtpSapId,
	mtpSap2AddressId
    }
    ::= { mtpSap2AddressTable 1 }

MtpSap2AddressEntry ::= SEQUENCE {
    mtpSap2AddressId				Unsigned32,
    mtpSap2AddressComponent			OCTET STRING
}

mtpSap2AddressId OBJECT-TYPE
    SYNTAX Unsigned32
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
	"Provides a simple integer index containing the ordinal number 
	 of the sap2 address component."
    ::= { mtpSap2AddressEntry 1 }

mtpSap2AddressComponent OBJECT-TYPE
    SYNTAX OCTET STRING
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
	"Provides one SAP address component."
    REFERENCE
	"ITU-T Rec. Q.751.1 (1997), ANSI T1.116.1 (2000), sap2Address 
	 attribute."
    ::= { mtpSap2AddressEntry 2 }

-- ====================================================================

mtpSapProviderEntityNamesTable OBJECT-TYPE
    SYNTAX SEQUENCE OF MtpSapProviderEntityNamesEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
	"Provides a side table for the mtpAccessPointTable that contains 
	 the provider entity names associated with an mtpAccessPoint."
    ::= { mtpSap 4 }

mtpSapProviderEntityNamesEntry OBJECT-TYPE
    SYNTAX MtpSapProviderEntityNamesEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
	"Each entry in this table corresponds to one 
	 mtpSapProviderEntityName for a given mtpAccessPoint.  The 
	 associated mtpAccessPoint is that indexed by the sapId index.  
	 The name is indexed by the mtpSapProviderEntityNameId index, 
	 which is a simple ordinal number from 1 through the number of 
	 providerEntityNames for the mtpAccessPoint."
    INDEX {
	mtpL3Id,
	mtpSapId,
	mtpSapProviderEntityNameId
    }
    ::= { mtpSapProviderEntityNamesTable 1 }

MtpSapProviderEntityNamesEntry ::= SEQUENCE {
    mtpSapProviderEntityNameId			Unsigned32,
    mtpSapProviderEntityName			RowPointer
}

mtpSapProviderEntityNameId OBJECT-TYPE
    SYNTAX Unsigned32
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
	"Provides a simple integer index containing the cardinal number 
	 of the provider entity name."
    ::= { mtpSapProviderEntityNamesEntry 1 }

mtpSapProviderEntityName OBJECT-TYPE
    SYNTAX RowPointer
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
	"Provides a reference to a row in another table that represents 
	 the user entity."
    ::= { mtpSapProviderEntityNamesEntry 2 }


