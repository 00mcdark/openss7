    mtpSpSiReceivedUPUnavailable		PerfCurrentCount, -- Q.752, 5.7
    mtpSpSiHandledOctetsSIO			PerfCurrentCount, -- Q.752, 6.3
    mtpSpSiStatus				RowStatus
}

