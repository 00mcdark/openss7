mtpCompliances		OBJECT IDENTIFIER ::= { mtpConformance	 1 }
mtpGroups		OBJECT IDENTIFIER ::= { mtpConformance	 2 }

mtpL2Profile		OBJECT IDENTIFIER ::= { mtpTables	 1 }
mtpLkProfile		OBJECT IDENTIFIER ::= { mtpTables	 2 }
mtpLsProfile		OBJECT IDENTIFIER ::= { mtpTables	 3 }
mtpRtProfile		OBJECT IDENTIFIER ::= { mtpTables	 4 }
mtpRsProfile		OBJECT IDENTIFIER ::= { mtpTables	 5 }
mtpSpProfile		OBJECT IDENTIFIER ::= { mtpTables	 6 }
