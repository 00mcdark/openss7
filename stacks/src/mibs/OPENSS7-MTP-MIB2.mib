OPENSS7-MTP-MIB2 DEFINITIONS ::= BEGIN

IMPORTS
	MODULE-IDENTITY, OBJECT-IDENTITY, OBJECT-TYPE
		FROM SNMPv2-SMI

	RowStatus, TimeInterval
		FROM SNMPv2-TC

	SnmpAdminString
		FROM SNMP-FRAMEWORK-MIB

	MtpTransmissionRate
		FROM OPENSS7-MTP-TC

	openss7Modules
		FROM OPENSS7-SMI;

mtpMIB2 MODULE-IDENTITY
	LAST-UPDATED	"200709301118Z"
	ORGANIZATION	"The OpenSS7 Project"
	CONTACT-INFO	"Brian Bidulock
			 OpenSS7 Corporation
			 1469 Jefferys Crescent
			 Edmonton, Alberta
			 Canada T6L 6T1
			 Tel: +1 780 490 1141
			 Fax: +1 780 490 1241
			 Mob: +1 780 953 0557
			 mailto:bidulock@openss7.org"
	DESCRIPTION
			"Second crack at SS7 MTP MIB."
	::= { openss7Modules 3 }

-- top level organization of the MIB

