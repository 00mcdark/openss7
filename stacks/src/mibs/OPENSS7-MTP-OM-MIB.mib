	 ANSI T1.116.1 mtpSignPoint."
    INDEX   {
	mtpSignPointId,	-- shared index
	mtpOmOpc,	-- shared index
	mtpOmDpc,	-- shared index
	mtpOmSio	-- shared index
    }
    ::= { mtpOmHandledOctetsOpcDpcSioTable 1 }

MtpOmHandledOctetsOpcDpcSioEntry ::=
    SEQUENCE {
	mtpOmHandledOctetsOpcDpcSioInterval	TimeInterval,
	mtpOmHandledOctetsOpcDpcSio		PerfCurrentCount, -- handledOctetsOpcDpcSio
	mtpOmHandledOctetsOpcDpcSioRowStatus	RowStatus
    }

mtpOmHandledOctetsOpcDpcSioInterval OBJECT-TYPE
    SYNTAX TimeInterval (30000|180000)
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
	"Specifies the time interval over which measurement in this 
	 conceptual row are collected.  Intervals of 5 and 30 minutes 
	 must be supported.  Other intervals are optional."
    DEFVAL { 180000 } -- 30 minutes
    ::= { mtpOmHandledOctetsOpcDpcSioEntry 1 }

mtpOmHandledOctetsOpcDpcSio OBJECT-TYPE
    SYNTAX PerfCurrentCount
    UNITS "Octets/OPC/DPC/SIO"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
	"This attribute represents measurement T1.116.2/6.6.
	 
	 Number of SIF and SIO octets handled with given OPC, DPC and 
	 SIO in units of number of MSUs per OPC, DPC and SIO combination 
	 collected over a 5- or 30-minute interval.  This operational 
	 measurement is non-obligatory.  This measurement may be used 
	 for performance managment, accounting management, and network 
	 administration and planning."
    REFERENCE
	"ANSI T1.116.1D handledOctetsOpcDpcSio.
	 ANSI T1.116.2/2000 Table 6 (6.6)."
    ::= { mtpOmHandledOctetsOpcDpcSioEntry 2 }

