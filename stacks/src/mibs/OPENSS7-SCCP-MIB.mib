sccpGtTranslatorTable OBJECT-TYPE
    SYNTAX SEQUENCE OF SccpGtTranslatorEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
	""
    REFERENCE
	"ITU-T Rec. Q.751.2 gtTranslator MO."
    ::= { sccpGtTranslator 1 }

sccpGtTranslatorEntry OBJECT-TYPE
    SYNTAX SccpGtTranslatorEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
	"The Global Title Translator is selected on a combination of the 
	 values of the Nature of Address, the Translation Type, and the 
	 Numbering Plan (if available).  As a consequence, the Global 
	 Title Translator is modelled as a class with three 
	 characteristic attributes: Nature of Address, TranslationType, 
	 and Numbering Plan.  If a Global Title address field is not 
	 used, it has the value NULL.  The gtIndicator attribute is a 
	 read-only and is derived from the combination of Nature of 
	 Address, the Translation Type, and Numbering Plan used.  If for 
	 a given combination no Global Title Translator exists, a 
	 Routing Failure notification with the reason 'no translation 
	 for address of this nature' is emitted by the SCRC.  The 
	 administrativeState attribute can have the following states: 
	 locked and unlocked.
	 
	 The sccpNetworkEntityId identifies the SCCP network entity and 
	 the contained SCRC (SCCP routing control) protocol module to 
	 which the GT translator belongs."
    REFERENCE
	"ITU-T Rec. Q.751.2 gtTranslator MO."
    INDEX   {
	    mtpMsId,		 -- shared index
	    sccpNetworkEntityId, -- shared index
	    sccpGtTranslatorId   -- index
	}
    ::= { sccpGtTranslatorTable 1 }

SccpGtTranslatorEntry ::=
    SEQUENCE {
	sccpGtTranslatorId			SimpleNameType, -- index
	sccpGtTranslatorGtIndicator		GtIndicator,
	sccpGtTranslatorNatureOfAddress		GtNatureOfAddress,
	sccpGtTranslatorNumberingPlan		GtNumberingPlan,
	sccpGtTranslatorTranslationType		GtTranslationType,
	sccpGtTranslatorAdministrativeState	AdministrativeState,
	sccpGtTranslatorName			CommonLanguageName,
	sccpGtTranslatorRowStatus		RowStatus
    }

sccpGtTranslatorId OBJECT-TYPE
    SYNTAX SimpleNameType
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
	""
    REFERENCE
	"ITU-T Rec. Q.751.2 gtTranslatorId."
    ::= { sccpGtTranslatorEntry 1 }

sccpGtTranslatorGtIndicator OBJECT-TYPE
    SYNTAX GtIndicator
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
	""
    REFERENCE
	"ITU-T Rec. Q.751.2 gtIndicator."
    ::= { sccpGtTranslatorEntry 2 }

sccpGtTranslatorNatureOfAddress OBJECT-TYPE
