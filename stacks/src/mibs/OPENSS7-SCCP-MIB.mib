    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
	""
    REFERENCE
	"ITU-T Rec. Q.751.2 gtNumberingPlan."
    ::= { sccpGtTranslatorEntry 4 }

sccpGtTranslatorTranslationType OBJECT-TYPE
    SYNTAX GtTranslationType
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
	""
    REFERENCE
	"ITU-T Rec. Q.751.2 gtTranslationType."
    ::= { sccpGtTranslatorEntry 5 }

sccpGtTranslatorAdministrativeState OBJECT-TYPE
    SYNTAX AdministrativeState
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
	""
    REFERENCE
	"ITU-T Rec. X.721 (1992) | ISO/IEC 10165-2 : 1992, 
	     administrativeState attribute."
