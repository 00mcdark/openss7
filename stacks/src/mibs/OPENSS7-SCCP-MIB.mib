sccpConcernedAreaTable OBJECT-TYPE
	SYNTAX		SEQUENCE OF SccpConcernedAreaEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"A concernedArea contains a list of remote SCCP's 
		 (mtpAccessPoints) to be informed of local (primary 
		 broadcast) or remote (secondary broadcast) SCCP 
		 subsystem changes, or to informed of the SCCP status 
		 after completion of SCCP Restart."
	REFERENCE
		"ITU-T Rec. Q.751.2, ANSI T1.116.2 concernedArea."
	::= { sccpConcerneArea 1 }

sccpConcernedAreaEntry OBJECT-TYPE
	SYNTAX		SccpConcernedAreaEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"Rows may be added to this table.  Rows need to be added 
		 to the sccpRemoteSCCPTable to add remote SCCP to the 
		 concerned area.  Rows may not be removed from this 
		 table unless all of the rows corresponding to this 
		 index have also been removed from the corresponding 
		 index entries in the sccpRemoteSCCPTable; that is, the 
		 concerned are must be empty before it can be deleted.  
		 Concerned areas are specific to a SCRC protocol module 
		 and its associated SCCP network entity."
	REFERENCE
		"ITU-T Rec. Q.751.2, ANSI T1.116.2 concernedArea."
	INDEX	{
			sccpNetworkEntityId, -- shared index
			sccpConcernedAreaId -- index
		}
	::= { sccpConcernedAreaTable 1 }

SccpConcernedAreaEntry ::=
	SEQUENCE {
		sccpConcernedAreaId
			SimpleNameType, -- index
		sccpConcernedAreaRowStatus
			RowStatus
	}

sccpConcernedAreaId OBJECT-TYPE
	SYNTAX		SimpleNameType
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"Provides the name of the concerned area following the 
		 SimpleNameType textual convention.  This is also a 
		 secondary index into the table."
	REFERENCE
		"ITU-T Rec. Q.751.2 concernedAreaId."
	::= { sccpConcernedAreaEntry 1 }

sccpConcernedAreaRowStatus OBJECT-TYPE
	SYNTAX		RowStatus
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"Provides a mechanism according to the RowStatus textual  
		 convention by which management stations may create and 
		 destroy entries in this table.  New entires are created 
		 empty: that is, they have no remote SCCP MTP-SAPs 
		 associated with them.  Before entries can be deleted, 
		 they must have all remote MTP-SAPs removed by removing 
		 the corresponding entries in the sccpRemoteSCCPTable 
		 for the corresponding sccpNetworkEntityId and 
		 sccpConcernedAreaId indices."
	REFERENCE
		""
	::= { sccpConcernedAreaEntry 2 }

-- remoteSCCPList side-table

sccpRemoteSCCPTable OBJECT-TYPE
	SYNTAX		SEQUENCE OF SccpRemoteSCCPEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"This table is a side-table that provides a list of 
		 remote SCCP entitites that belong to a concerned 
		 area."
	REFERENCE
		"ITU-T Rec. Q.751.2 remoteSCCPList."
	::= { sccpRemoteSCCP 1 }

sccpRemoteSCCPEntry OBJECT-TYPE
	SYNTAX		SccpRemoteSCCPEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	INDEX	{
			sccpNetworkEntityId,	-- shared index
			sccpConcernedAreaId,	-- shared index
			sccpRemoteSCCPId	-- index
		}
	::= { sccpRemoteSCCPTable 1 }

SccpRemoteSCCPEntry ::=
