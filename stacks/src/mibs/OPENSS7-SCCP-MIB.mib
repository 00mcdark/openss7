OPENSS7-SCCP-MIB DEFINITIONS ::= BEGIN

IMPORTS
    MODULE-IDENTITY, OBJECT-TYPE, TEXTUAL-CONVENTION, enterprises
	FROM SNMPv2-SMI
    MODULE-COMPLIANCE, RowStatus, RowPointer, AutonomousType, 
    TimeInterval, DisplayString, TruthValue
	FROM SNMPv2-TC
    CommonLanguageName
	FROM OPENSS7-TC
    AdministrativeState, OperationalState, ProceduralStatus, 
    AvailabilityStatus, AlarmStatus, SimpleNameType, UsageState
	FROM OPENSS7-SMI-MIB
    NEntitySystemTypes, NEntitySystemType
	FROM OPENSS7-CLNS-MIB
    Ss7ProtocolVariant
	FROM OPENSS7-SS7-MIB
    mtpMsId, MtpSapAddress, MtpUserPart, MtpUserPartStatus, 
    MtpCongestedState, MtpCongestionLevel
	FROM OPENSS7-MTP-MIB
    openss7, openss7Q, openss7Q751
	FROM OPENSS7-SMI;

sccpMIB MODULE-IDENTITY
    LAST-UPDATED "200812201153Z"
    ORGANIZATION "The OpenSS7 Project"
    CONTACT-INFO
	"Brian Bidulock
	 OpenSS7 Corporation
	 1469 Jefferys Crescent
	 Edmonton, Alberta  T6L 6T1
	 CANADA
	 Tel: +1 780 490 1141
	 Fax: +1 780 490 1241
	 Mob: +1 780 953 0557
	 mailto:bidulock@openss7.org"
    DESCRIPTION
	"This MIB provides configuration and status information of an 
	 SCCP subsystem, where SCCP is in accordance with ITU-T Rec.  
	 Q.711 and managed according to ITU-T Rec.  Q.751.2 or ANSI 
	 T1.116.2."
    REVISION "200812201153Z"
    DESCRIPTION
	"Additional workup."
    REVISION "200812082322Z"
    DESCRIPTION
	"This is an update to the MIB that includes changes mostly to 
	 the descriptions of the managed objects.  Added missing 
	 TruthValue import.  Type name corrections to 
	 AdminstrativeState, SystemTypes and GtNumeringPlan."
    REVISION "200711281740Z"
    DESCRIPTION
	"This is the initial revision of this MIB that was released with 
	 the strss7-0.9a.8 release package."
    ::= { openss7Q751 2 }

 -- top level organization of the MIB

sccpNotifications	OBJECT IDENTIFIER ::= { sccpMIB      0 }
sccpObjects		OBJECT IDENTIFIER ::= { sccpMIB      1 }
sccpConformance		OBJECT IDENTIFIER ::= { sccpMIB      2 }

sccpTables		OBJECT IDENTIFIER ::= { sccpObjects  1 }
sccpScalars		OBJECT IDENTIFIER ::= { sccpObjects  2 }
