	REFERENCE
		"ITU-T Recommendation Q.754 (1997) ASN.1 Priority."
	SYNTAX	INTEGER {
			unknown(0),
			firstChoice(1),
			secondChoice(2),
			thirdChoice(3),
			...
		} (0..255)

RoutePriorityList TEXTUAL-CONVENTION
	DISPLAY-HINT	"1d,"
	STATUS		current
	DESCRIPTION
		""
	REFERENCE
		"ITU-T Recommdendation Q.754 (1997) ASN.1 
		 RoutePriorityList."
	SYNTAX		OCTET STRING

GlobalTitle TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"Provides a specification of a global title.  The global 
		 title is represented as an octet string containing the 
		 same bit encoding as the global title that appears in 
		 an SCCP message as described in ITU-T Recommendation 
		 Q.713, with the exception that the first octet contains 
		 only the Global Title Indicator as illustrated in 
		 3.4.1/Q.713, and the remaining octet contain only the 
		 Global Title as in 3.4.2.1 through 3.4.2.4/Q.713 as 
		 appropriate for the GTI (and not the SubSystem Number 
		 or Signalling Point Code) as illustrated in Q.713 ."
	REFERENCE
		"ITU-T Recommendation Q.713 3.4.1, and 3.4.2.1 through 
		 3.4.2.4, ITU-T Recommendation Q.754 (1997) ASN.1 
		 GlobalTitle."
	SYNTAX		OCTET STRING

GtEncodingScheme TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"Provides a specification of the encoding scheme to be 
		 used in Global Titles."
	REFERENCE
		"ITU-T Rec. Q.751.2 ASN.1 GtEncodingScheme."
	SYNTAX	INTEGER {
			unknown(0),
			bCDODD(1),
			bCDEVEN(2),
			nationalSpecific(3),
			notUsedOrNoOverwite(256)
		}

GtNatureOfAddress TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"Provides a specification of the nature of address to be 
		 used in or matched against Global Titles."
	REFERENCE
		"ITU-T Rec. Q.751.2 ASN.1 GtNatureOfAddress."
	SYNTAX	INTEGER {
			unknown(0),
			iSDNTNP(1),
			genericNumberingPlan(2),
			dNP(3),
			tNP(4),
			mMNP(5),
			lMNP(6),
			iSDNMNP(7),
			privateNumberingPlan(14),
			notUsedOrNoOverwite(256)
		}

GtNumberingPlan TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"Provides a specification of the numbering plan to be 
		 used in or matched against Global Titles."
	REFERENCE
		"ITU-T Rec. Q.751.2 ASN.1 GtNumberingPlan."
	SYNTAX	INTEGER {
			unknown(0),
			subscriber(1),
			national(3),
			international(4),
			notUsedOrNoOverwrite(256)
		}

GtTranslationType TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"Provides a specification of the translation type to be 
		 used in or matched against Global Titles."
	REFERENCE
		"ITU-T Q.751.2 ASN.1 GtTranslationType."
	SYNTAX	INTEGER {
			unknown(0),
			iTCC(1),
			genericNumberingPlan(14),
			iEESS(17),
			notUsedOrNoOverwrite(256)
		}

GtIndicator TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"Provides a specification of the Global Title Indicator 
		 to be used in or matched against Global Titles."
	REFERENCE
		"ITU-T Rec. Q.751.2 ASN.1 GtIndicator."
	SYNTAX	INTEGER {
			noGlobalTitle(0),
			nOAonly(1),
			tTonly(2),
			tT-NP-ES(3),
			tT-NP-ES-NOA(4)
		}

GtAddressInformation TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"Provides a specification of of the Global Title Address 
		 Information to be used in or matched against Global 
		 Titles."
	REFERENCE
		"ITU-T Rec. Q.751.2 ASN.1 GtAddressInformation, 
		 3.4.2/Q.713."
	SYNTAX		OCTET STRING

SubSystemNumber TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"Provides a specification of an SCCP subsystem number 
		 (SSN) to be used in or matched against Global Titles."
	REFERENCE
		"ITu-T Rec. Q.751.2 sap2Address, 3.4/Q.713."
	SYNTAX		OCTET STRING(SIZE(0..1))

ImportanceLevel TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"Provides a set values specification of the importance 
		 level.  The first octet in the two-octet string is the 
		 default importance level, the second octet in the 
		 two-octet string is the maximum importance level.  
		 Importance levels are only allowed to range from 0 to 
		 255."
	REFERENCE
		"ITU-T Rec. Q.751.2 ASN.1 ImportanceLevel, 2.6.2/Q.714."
	SYNTAX		OCTET STRING(SIZE(2))

OperationalProtocols TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"Specifies the OperationalProtocols that are supported.  
		 When class0(0) is set, it specifies that the basic 
		 connectionless class, protocol class 0, is supported.  
		 When class1(1) is set, it specifies that the sequenced 
		 connectionless class, protocol class 1, is supported.  
		 When class2(2) is set, it specifies that the basic 
		 connection-oriented calss, protocol class 2, is 
		 supported.  WHen class3(3) is set, it specifies that 
		 the flow control connection oriented calss, protocol 
		 class 3, iss supported.  This is a set-valued 
		 attribute."
	REFERENCE
		"ITU-T Rec. Q.751.2 ASN.1 OperationalProtols."
	SYNTAX		BITS { class0(0), class1(1), class2(2), class3(3) }

SccpSyntaxErrorList TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"Provides a means for specifying SCCP syntax errors.  
		 Typically used with an accessible-for-notify scalar to 
		 be used in notifications to indicate the syntax error 
		 experienced."
	REFERENCE
		"ITU-T Rec. Q.751.2 ASN.1 SccpSyntaxErrorList."
	SYNTAX	INTEGER {
			unknownMessageType(0),
			invalidValueOfProtocolClass(1),
			invalidValueOfGTI(2),
			invalidValueForEncodingScheme(3),
			invalidParameterLength(4),
			invalidPointerToOptionalParameter(5),
			optionalParameterTooLong(6),
			pointerInconsistentWithLengths(7),
			incompatibleAddressLength(8),
			expectedSSNotFound(9)
		}

PrimaryOrBackup TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"Provides a means for specifying whether an entity in an 
		 entity set is an equal(0), primary(1) or backup(2) 
		 member of the entity set.  When all memebrs of the 
		 entity set are equal(0), loadsharing will occur accross 
		 members of the entity set.  When one member is 
		 primary(1) and the other members are backup(2), the 
		 traffic load will be directed at the primary(1)."
	REFERENCE	
		"ITU-T Rec. Q.751.2 ASN.1 PrimaryOrBackup."
	SYNTAX	INTEGER { equal(0), primary(1), backup(2) }

SharingMode TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"Provides a means for specifying the sharing mode 
		 between entities in an entity set.  Solitary means that 
		 there is only one entity in the entity set.  Duplicated 
		 dominant.  Duplicated replacement.  Duplicated load 
		 shared indicates that the traffic is load shared over 
		 the two entities in the entity set."
	REFERENCE
		"ITU-T Rec. Q.751.2 ASN.1 SharingMode."
	SYNTAX	INTEGER {
			solitary(0),
			dupliDominant(1),
			dupliReplacement(2),
			dupliLoadShared(3)
		}


SccpNAddress TEXTUAL-CONVENTION
	DISPLAY-HINT	"1d:1d-1d-1d[1d]"
	STATUS		current
	DESCRIPTION
		"Provides a means for specifying the SccpNAddress in the 
		 form of an NSAP Address for use with the 
		 networkEntityTitles that are associated with an SCCP 
		 network entity to uniquely identify it.  An SCCP Adress 
		 is the combination of a network references point code 
		 (NetworkPointCode textual convention from the 
		 OPENSS7-SS7-TC module) and a SubSystemNumber.  
		 Therefore, the first octet represents the larger 
		 national or International network, the second octet 
		 represents the value of the 'network' component of the 
		 SS7 signalling point code, the third octet, the 
		 'cluster' component, the fourth, the 'member' 
		 component, and the fifth, the SCCP subsystem number.  
		 This is displayed with a colon between the national or 
		 International network identifier, dashes between the 
		 components of the signalling point code, and square 
		 brackets around the subsystem number; e.g. 1:5-1-2[5]."
	REFERENCE
		"ITU-T Rec. X.283 (1992) | ISO/IEC 10733 : 1993 ASN.1 
		 NLM.NAddress."
	SYNTAX		OCTET STRING(SIZE(5))

SccpNAddresses TEXTUAL-CONVENTION
	DISPLAY-HINT	"1d:1d-1d-1d[1d],"
	STATUS		current
	DESCRIPTION
		"Provides a means for specifying a list of SCCP 
		 addresses.  Each SCCP address in the list is 
		 represented by 5 octets that appear in the form 
		 specified for the SccpNAddress.  Each successive 5 
		 octets represents another SccpNAddress.  The 
		 SccpNAddresses textual convention is, therefore, a 
		 multiple of 5 octets in length."
	REFERENCE
		"ITU-T Rec. X.283 (1992) | ISO/IEC 10733 : 1993 ASN.1 
		 NLM.NAddresses."
	SYNTAX		OCTET STRING(SIZE(0,5,10,...))

-- =====================================================================
-- 
-- NOTIFICATIONS defined by this module.
-- 

sccpRouteTestResult NOTIFICATION-TYPE
	OBJECTS {
		objectInstance
		-- pointCodeList,
		-- routePriorityList,
		-- copyData
		-- or --
		-- failureType,
		-- taceSent,
		-- copyData
	}
	STATUS	current
	DESCRIPTION
		"This is the sccpRouteTestResult notification.  When an 
		 SRVT (SCCP Route Verification test) is initated, and 
		 either succeeds or fails, the sccpRouteTestResult 
		 notification is issued.  This notification always 
		 contains the objectInstance, which is a RowPointer into 
		 the sccpSrvtTable table that indicates the specific 
		 SCCP Route Verification test that has succeeded or 
		 failed.

		 When the notification indicates success, the 
		 notification may include a pointCodeList, 
		 routePriorityList and copyData parameters, depending 
		 upon what was requested in the test (see the 
		 sccpTraceRequested and sccpInfoRequest columns in the 
		 sccpSrvtTable); or it could indicate failure, in which 
		 case the notifcation must include failureType and 
		 traceSent parameters and might optionally include the 
		 copyData parameter depending upon the failure and the 
		 requested information."
	REFERENCE
		"ITU-T Recommendation Q.751.2 (1992) 
		 sccpRouteTestResult."
	::= { sccpNotifications 1 }

