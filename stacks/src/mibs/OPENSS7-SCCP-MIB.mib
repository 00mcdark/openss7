    STATUS current
    DESCRIPTION
	"Provides the module compliance requirements for the SCCP MIB."

    MODULE -- this module

    MANDATORY-GROUPS	{
	    sccpNetworkEntityGroup, 
	    sccpAccessPointGroup, 
	    sccpLinkageGroup, 
	    sccpScrcGroup, 
	    sccpConcernedAreaGroup, 
	    sccpRemoteSCCPGroup
	}

