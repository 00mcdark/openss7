			CommonLanguageName,
		sccpRemoteSCCPRowStatus
			RowStatus
	}

sccpRemoteSCCPId OBJECT-TYPE
	SYNTAX		SimpleNameType
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		""
	REFERENCE
		""
	::= { sccpRemoteSCCPEntry xx }

sccpRemoteSCCPMTPAccessPoint OBJECT-TYPE
	SYNTAX		RowPointer
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"This attribute models a list of remote MTP Access 
		 Points all belonging to the same MTP network."
	REFERENCE
		""
	::= { sccpRemoteSCCPEntry xx }

sccpRemoteSCCPName OBJECT-TYPE
	SYNTAX		CommonLanguageName
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		""
	REFERENCE
		""
	::= { sccpRemoteSCCPEntry xx }

sccpRemoteSCCPRowStatus OBJECT-TYPE
	SYNTAX		RowStatus
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		""
	REFERENCE
		""
	::= { sccpRemoteSCCPEntry xx }

