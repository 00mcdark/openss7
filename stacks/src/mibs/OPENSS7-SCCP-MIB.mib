sccpScrcTable OBJECT-TYPE
	SYNTAX		SEQUENCE OF SccpScrcEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"The scrc managed object class contains the management 
		 information concerning the SCCP routing control (SCRC).  
		 One SCRC protocol machine managed object exists for 
		 each SCCP (network entity)."
	REFERENCE
		"ITU-T Rec. Q.751.2 scrc MO."
	::= { sccpScrc 1 }

sccpScrcEntry OBJECT-TYPE
	SYNTAX		SccpScrcEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"Entries in this table can generate 
		 qualityofSerivceAlarm notifications which are detailed 
		 under the sccpScrcAlarmStatus column, below.  Although 
		 an sccpScrcRowStatus column is provided, entries in 
		 this table are not normally created by management 
		 stations, and are instead created by system operation 
		 when SCCP network entities are created.  It is not 
		 necessary for implementations to permit management 
		 stations to create entries in this table."
 	REFERENCE
		"ITU-T Rec. Q.751.2 scrc MO."
	INDEX		{ sccpNetworkEntityId }	-- shared index
	::= { sccpScrcTable 1 }

SccpScrcEntry ::=
	SEQUENCE {
		sccpScrcId
			SimpleNameType,
		sccpScrcAlarmStatus
			AlarmStatus,
		sccpScrcName
			CommonLanguageName,
		sccpScrcRowStatus
			RowStatus
	}

sccpScrcId OBJECT-TYPE
	SYNTAX		SimpleNameType
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"This attribute is used for naming instances.  Because 
		 there is a one-to-one relationship between SCCP 
		 (network entity) and a contained SCRC (SCCP routing 
		 control protocol machine), this attribute is not an 
		 index."
	REFERENCE
		"ITU-T Rec. Q.751.2 scrcId."
	DEFVAL		{ 0 }
	::= { sccpScrcEntry xx }

sccpScrcAlarmStatus OBJECT-TYPE
	SYNTAX		AlarmStatus
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"Provides the alarm status, following the AlarmStatus 
		 textual convention of the OPENSS7-TMN-TC module, of the 
		 SCRC protocol machine corresponding to the conceptual 
		 row in which this status occurs.  This managed object 
		 generates the following alarms:

		 The qualityofServiceAlarm can have the following 
		 probably causes:

		 - ProbableCause = noTranslatorForAddress which 
		   represents measurement Q.752/7.1 (PerceivedSeverity = 
		   Major/Minor/Warning),

		 - ProbableCause = noRuleForAddress which represents 
		   measurement Q.752/7.2 (PerceivedSeverity = 
		   Major/Minor/Warning),

		 - ProbableCause = pointCodeNotAvailable which 
		   represents measurement Q.752/7.3 (PerceivedSeverity = 
		   Major/Minor/Warning),

		 - ProbableCause = pointCodeCongested which represents 
		   measurement Q.752/7.4 (PerceivedSeverity = 
		   Major/Minor/Warning),

		 - ProbableCause = subsystemUnavailable which represents 
		   measurement Q.752/7.5 (PerceivedSeverity = 
		   Major/Minor/Warning),

		 - ProbableCause = subsystemCongested which represents 
		   measurement Q.752/7.6 (PerceivedSeverity = 
		   Major/Minor/Warning),

		 - ProbableCause = unequippedSubsystem which represents 
		   measurement Q.752/7.7 (PerceivedSeverity = 
		   Major/Minor/Warning),

		 - ProbableCause = syntaxErrorDetected which represents 
		   measurement Q.752/7.8 (PerceivedSeverity = 
		   Major/Minor/Warning),

		 - ProbableCause = routingFailureNoReasonOrUnqualified 
		   which represents measurement Q.752/7.9 
		   (PerceivedSeverity = Major/Minor/Warning), and

		 - ProbableCause = hopCounterViolation which represents 
		   measurement Q.752/7.13 (PerceivedSeverity = 
		   Major/Minor/Warning)."
	REFERENCE
		"ITU-T Rec. X.731 (1993) : 8.1.2.1, ITU-T Rec. X.721 
		 (1882) : alarmStatus"
	DEFVAL		{ '000000'B }
	::= { sccpScrcEntry xx }

sccpScrcName OBJECT-TYPE
	SYNTAX		CommonLanguageName
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"Provides the optional ability to associate a name with 
		 the the conceptual row following the CommonLanguageName 
		 textual convention.  When unused, this attribute is a 
		 null string."
	REFERENCE
		"ITU-T Rec. Q.751.2 scrcName."
	DEFVAL		{ ''H }
	::= { sccpScrcEntry xx }

sccpScrcRowStatus OBJECT-TYPE
	SYNTAX		RowStatus
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"Provides a means, following the RowStatus textual 
		 convention, for management stations to create or 
		 destroy conceptul rows corresponding to SCRC protocol 
		 machine instances in this table."
	REFERENCE
		"STD 58, SNMPv2-TC RowStatus"
	::= { sccpScrcEntry xx }

-- =====================================================================

sccpEntitySetTable OBJECT-TYPE
	SYNTAX		SEQUENCE OF SccpEntitySetEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		""
	REFERENCE
		""
	::= { sccpEntitySet 1 }

sccpEntitySetEntry OBJECT-TYPE
	SYNTAX		SccpEntitySetEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"Note that an specific Entity Set belongs to an SCRC 
		 (protocol machine) which also belongs to a SCCP 
		 (network entity).  Because there is a one-to-one 
		 relationship between an SCCP (network entity) and an 
		 SCRC (protocol machine), the sccpNetworkEntityId is 
		 sufficient to identify both, and it is used as the 
		 shared index of this table."
	REFERENCE
		""
	INDEX	{
			sccpNetworkEntityId, -- shared index
			sccpEntitySetId -- index
		}
	::= { sccpEntitySetTable 1 }

SccpEntitySetEntry ::=
	SEQUENCE {
		sccpEntitySetId
			SimpleNameType,
		sccpSharingMode
			SharingMode,
		sccpLoadSharingAlgPointer
			AutonomousType,
		sccpEntitySetName
			CommonLanguageName,
		sccpEntitySetType
			INTEGER,
		sccpEntitySetSsn
			SubSystemNumber,
		sccpEntitySetRowStatus
			RowStatus
	}

sccpEntitySetId OBJECT-TYPE
	SYNTAX		SimpleNameType
	MAX-ACCESS	read-create
	STATUS		current
	DEFINITIONS
		"Provides an identifier for use in naming the conceptual 
		 row of the sccpEntitySetTable table."
	REFERENCE
		"ITU-T Rec. Q.751.2 entitySetId."
	::= { sccpEntitySetEntry xx }

sccpSharingMode OBJECT-TYPE
	SYNTAX		SharingMode
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"This attribute models the sharing mode of the SCCP 
		 Entity Set.  Possible values are: solitary, 
		 duplicated-replacement, duplicated-dominant, and 
		 duplicated-loadshared."
	REFERENCE
		"ITU-T Rec. Q.751.2 sharingMode."
	::= { sccpEntitySetEntry xx }

sccpLoadSharingAlgPointer OBJECT-TYPE
	SYNTAX		AutonomousType
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"Provides a specification of a load sharing algorithm 
		 using the AutonomousType texutual convention.  This can 
		 be a defined value.  Several load sharing aglortihsm 
		 are defined by this MIB: see sccpLoadSharingRoundRobin 
		 and sccpLoadSharingSlsSplitting object identities 
		 defined for this MIB (above).  THe 
		 sccpLoadSharingAlgPointer is only significant when the 
		 sccpSharingMode is set to duplicated-loadshared."
	REFERENCE
		"ITU-T Rec. Q.751.2 loadSharingAlgPointer."
	::= { sccpEntitySetEntry xx }

sccpEntitySetName OBJECT-TYPE
	SYNTAX		CommonLanguageName
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"Provides an optional name for the entity set 
		 represented by this conceptual row.  When unused (the 
		 default), this value should be set to a null string."
	REFERENCE
		"ITU-T Rec. Q.751.2 entitySetName."
	DEFVAL		{ ''X }
	::= { sccpEntitySetEntry xx }

sccpEntitySetType OBJECT-TYPE
	SYNTAX	INTEGER {
				endNodeEntitySetWithoutSSN(2),
				endNodeEntitySetWithSSN(3),
				relayNodeEntitySet(7)
			}
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"These type are represented by derivation in TNM, 
		 however, here they are respresented by this attribute 
		 and one other.  The values are as follows:

		 - endNodeEntitySetWithoutSSN(2): This maanged object 
		   class is an sccpEntitySet that indentifies SCCP end 
		   nodes.  Because of this, the entitySetSapPointer 
		   attribute is only allowed to refer to instances of 
		   the sccpLinkage class.  A subsystem number is not 
		   specified by this entity set, but routing is on SSN.

		 - endNodeEntitySetWithSSN(3): This managed object class 
		   is an sccpEntitySet that identifies SCCP Access 
		   Points.  Because of this, the SAP pointer attribute 
		   is only allowed to refer to instances of the 
		   sccpAccessPoint class.

		 - relayNodeEntitySet(7): This managed object class is a 
		   sccpEntitySet that identifies SCCP relay nodes, i.e. 
		   MTP Access Points.  Because of this, the 
		   entitySetSapPointer attribute is only allows to refer 
		   to instances of the sccpLinkage class.  The ssn 
		   attribtute allows to optionally set the subsystem 
		   number as a result of a Global Title Translation, if 
		   routing is done on Global Title and DPC to a relay 
		   node."
	REFERENCE
		"ITU-T Rec. Q.751"
	::= { sccpEntitySetEntry xx }

sccpEntitySetSsn OBJECT-TYPE
	SYNTAX		SubSystemNumber
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"Provides an optional subsystem number when the entity 
		 set type is relayNodeEntitySet(7).  Otherwise, the 
		 value is ignored."
	REFERENCE
		"ITU-T Rec. Q.751.2 ssn ATTRIBUTE."
	::= { sccpEntitySetEntry xx }


sccpEntitySetRowStatus OBJECT-TYPE
	SYNTAX		RowStatus
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"Provides a mechanism by what of which management 
		 stations, using the RowStatus textual convention can 
		 create or destroy conceptual rows in this table.  When 
		 entity sets are created in this fashion, they are 
		 created empty in that there are no entities contained 
		 in the sccpEnttySetSapTable corresndpoing to the same 
		 index as the sccpENtitySetId values for the created 
		 conceptual row.  Also, before entries can be 
		 destroyned, all of the entits within the entity set 
		 (those that appear in the sccpEntitySetSapTable with 
		 the same value of the sccpEntitySetId index) must be 
		 destroyed first."
	REFERENCE
		"STD 58, SNMPv2-TC RowStatus."
	::= { sccpEntitySetEntry xx }

-- SAP side-table for entity set

sccpEntitySetSapTable OBJECT-TYPE
	SYNTAX		SEQUENCE OF SccpEntitySetSapEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"This side-table provides a list of SAP types and 
		 pointers for each SAP contained in the entity set.  Up 
		 to two SAPs can occur per entity set.  Before entires 
		 are create in this subtable and entry for the 
		 corresponding sccpEntitySetId must already exist in the 
		 sccpEntitySetTable.  Before entries are destroyed in 
		 the sccpEntitySetTable, all entries for the 
		 coresponding sccpEntitySetId in this table must first 
		 be destroyed."
	REFERENCE
		""
	::= { sccpEntitySetSap 1 }

sccpEntitySetSapEntry OBJECT-TYPE
	SYNTAX		SccpEntitySetSapEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		""
	REFERENCE
		""
	INDEX	{
			sccpNetworkEntityId,	-- shared index
			sccpEntitySetId,	-- shared index
			sccpEntitySetSapId	-- index
		}
	::= { sccpEntitySetSapTable 1 }

SccpEntitySetSapEntry ::=
	SEQUENCE {
		sccpEntitySetSapId
			SimpleNameType, --index
		sccpEntitySetSapType
			PrimaryOrBackup,
		sccpEntitySetSapPointer
			RowPointer,
		sccpEntitySetSapRowStatus
			RowStatus
	}

sccpEntitySetSapId OBJECT-TYPE
	SYNTAX		SimpleNameType
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"Provides the index of the entity represented by the SAP 
		 within the entity set.  This is used as an index.  Not 
		 all entities or algorithms will support more than 2 
		 entities in the entity set (Q.751.2 limits the entity 
		 set membership to 2).  Not all implementations will 
		 support more than one entity in an entity set.  All 
		 implementations will support one entity in the entity 
		 set."
	REFERENCE
		"ITU-T Rec. Q.751.2 ANS.1 EntitySetSapPointer."
	::= { sccpEntitySetSapEntry xx }

sccpEntitySetSapType OBJECT-TYPE
	SYNTAX		PrimaryOrBackup
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"Specifies the role of the entity corresponding to the 
		 SAP within the Entity Set.  The role can be equal(0), 
		 primary(1) or backup(2)."
	REFERENCE
		"ITU-T Rec. Q.751.2 entitSetSapPointer, ASN.1 
		 PrimaryOrBackup."
	::= { sccpEntitySetSapEntry xx }

sccpEntitySetSapPointer OBJECT-TYPE
	--
	-- Because this pointer can point to an entry selected from one 
	-- of several tables, the index of any given table cannot be 
	-- used and the RowPointer textual convention is therefore 
	-- necessary.
	--
	SYNTAX		RowPointer
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"Provides a row pointer to an entry in the 
		 sccpAccesssPointTable table or in the sccpLinkageTable 
		 table that identifies the local or remote SAP (resp.) 
		 for the member of the entity set.  When the type of the 
		 entity set is endNodeEntitySetWithoutSSN or 
		 relayNodeEntitySet, this column must point to an row of 
		 the sccpLinkageTable table.  When the type of the 
		 entity set is endNodeEntitySetWithSSN, this column must 
		 point to a rows in the sccpAccessPointTable table."
	REFERENCE
		"ITU-T Rec. Q.751.2 entitSetSapPointer, ASN.1 
		 SAPPointer."
	::= { sccpEntitySetSapEntry xx }

sccpEntitySetSapRowStatus OBJECT-TYPE
	SYNTAX		RowStatus
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"Used by managements stations, according to the 
		 RowStatus textual convention, to create or destroy 
		 conceptual rows in this table.  Before a row may be 
		 created, the corresponding sccpEntitySetId must exist 
		 as a conceptual row"
	REFERENCE
		""
	::= { sccpEntitySetSapEntry xx }



