sccpNetworkEntity	OBJECT IDENTIFIER ::= { sccpTables   1 } -- SCCP_OBJ_TYPE_SP
sccpAccessPoint		OBJECT IDENTIFIER ::= { sccpTables   2 } -- SCCP_OBJ_TYPE_SS, SCCP_OBJ_TYPE_RS
sccpLinkage		OBJECT IDENTIFIER ::= { sccpTables   3 } -- SCCP_OBJ_TYPE_MT, SCCP_OBJ_TYPE_SR
sccpMtp			OBJECT IDENTIFIER ::= { sccpTables   4 } -- SCCP_OBJ_TYPE_SR
