sccpGtRuleTable OBJECT-TYPE
	SYNTAX		SEQUENCE OF SccpGtRuleEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		""
	REFERENCE
		"ITU-T Q.751.2 gtRule MO."
	::= { sccpGtRule 1 }
		

sccpGtRuleEntry OBJECT-TYPE
	SYNTAX		SccpGtRuleEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"A Global Title Rule is selected based on the (matching) 
		 Global Title Address Information and possibly the 
		 Encoding Scheme.  If there is no matching Translation 
		 Rule for a given Global Title, then a Routing Failure 
		 notification with the reason 'no translation for an 
		 address of this nature' is emitted by the SCRC.  The 
		 Global Title Rule points to the Entity Set resulting 
		 from the Global Title Transation.  The Global Title 
		 Conversion Rule pointer referes to an operation Global 
		 Title Conversion Rule that may modify the Global Title.  
		 The new Routing Indicator is implicitly set according 
		 to the sccpEntitySet pointer to.  The 
		 administrativeState attribute can have the following 
		 states: locked and unlocked."
	REFERENCE
		"ITU-T Q.751.2 gtRule MO."
	INDEX	{
			sccpNetworkEntityId,	-- shared index
			sccpGtTranslatorId,	-- shared index
			sccpGtRuleId		-- index
		}
	::= { sccpGtRuleTable 1 }

SccpGtRuleEntry ::=
	SEQUENCE {
		sccpGtRuleId
			SimpleNameType,
		sccpGtRuleAdministrativeState
			AdminstrativeState,
		sccpGtAddressInformation
			GtAddressInformation,
		sccpGtConvRulePointer
			SimpleNameType,
		sccpGtEncodingScheme
			GtEncodingScheme,
		sccpEntitySetPointer
			SimpleNameType,
		sccpGtRuleName
			CommonLanguageName,
		sccpGtRuleRowStatus
			RowStatus
	}

sccpGtRuleId OBJECT-TYPE
	SYNTAX		SimpleNameType
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		""
	REFERENCE
		""
	::=  { sccpGtRuleEntry xx }

sccpGtRuleAdministrativeState OBJECT-TYPE
	SYNTAX		AdminstrativeState
	MAX-ACCESS	read-create
	STATUS		current
	::=  { sccpGtRuleEntry xx }

sccpGtAddressInformation OBJECT-TYPE
	SYNTAX		GtAddressInformation
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"Provides the address information against which to match 
		 the Global Title for this GT Rule to be applicable.

		 The gtAddressInformation attribute contains the value 
		 of the Global Title address information, as indicated 
		 in 3.4.2/Q.713."
	REFERENCE
		"ITU-T Rec. Q.751.2 gtAddressInformation."
	::=  { sccpGtRuleEntry xx }

sccpGtConvRulePointer OBJECT-TYPE
	--
	-- Because this pointer only points to entries in one table, the 
	-- sccpGtConversionRuleTable, this pointer is formatted the same 
	-- as the secondary index to that table.  When finding the entry 
	-- in the table, the same value of the sccpNetworkEntityId as is 
	-- used to index this table is used as the primary index into 
	-- the sccpGtConversionRuleTable table as well.
	--
	SYNTAX		SimpleNameType	-- was RowPointer
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"Provides a row pointer that indicates the GT Conversion 
		 Rule to be applied for this GT Rule, using the 
		 RowPointer textual convention.  This is a pointer to an 
		 entry in the sccpGtConversionRuleTable.  The 
		 sccpNetworkEntityId of the row pointed to and the 
		 sccpNetworkEntityId of this GT Rule entry must be the 
		 same or an error will result."
	::=  { sccpGtRuleEntry xx }

sccpGtEncodingScheme OBJECT-TYPE
	SYNTAX		GtEncodingScheme
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"Provides the encoding scheme against which to match the 
		 Global Title for this GT Rule to be applicable."
	REFERENCE
		"ITU-T Rec. Q.751.2 gtEncodingScheme."
	::=  { sccpGtRuleEntry xx }

sccpEntitySetPointer OBJECT-TYPE
	--
	-- Beacuse this pointer only points to entries in one table, the 
	-- sccpEntitySetTable, this pointer is formatted the same as the 
	-- secondary index to that table.  When finding the entry in the 
	-- table, the same value of the sccpNetworkENtityId as is used 
	-- to index this table is used as the primary index into the 
	-- sccpEntitySetTable as well.
	--
	SYNTAX		SimpleNameType	-- was RowPointer
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"Proivdes a row pointer that indicates the SCCP Entity 
		 Set to which this translation results.  Using the 
		 RowPointer textual convention, this is a pointer to an 
		 entry in the sccpEntitySetTable.  The 
		 sccpNetworkEntityId of the row pointed to and the 
		 sccpNetworkEntityId of this GT Rule entry must be the 
		 same or an error will result."
	REFERENCE
		"ITU-T Rec. Q.751.2 entitySetPointer."
	::=  { sccpGtRuleEntry xx }

sccpGtRuleName OBJECT-TYPE
	SYNTAX		CommonLanguageName
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		""
	REFERENCE
		""
	DEFVAL		{ ''X }
	::=  { sccpGtRuleEntry xx }

sccpGtRuleRowStatus OBJECT-TYPE
	SYNTAX		RowStatus
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		""
	REFERENCE
		"SNMPv2-TC RowStatus"
	::=  { sccpGtRuleEntry xx }

-- =====================================================================

sccpSrvtTable OBJECT-TYPE
	SYNTAX		SEQUENCE OF SccpSrvtEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"This managed object represents the management 
		 information of the SCCP Routing Verification Test.  
		 This test is described in Recommendation Q.753.  The 
		 SCCP Routing Verification Test (srvt) is used to test 
		 the Global Title Translation service of the SCCP."
	REFERENCE
		""
	::= { sccpSrvt 1 }

sccpSrvtEntry OBJECT-TYPE
	SYNTAX		SccpSrvtEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"A svrt "
	REFERENCE
		""
	INDEX	{
			sccpNetworkEntityId, -- shared index
			sccpRouteTestId -- index
		}
	::= { sccpSrvtTable 1 }

SccpSrvtEntry ::=
	SEQUENCE {
		sccpRouteTestId
			SimpleNameType,
		sccpDSRVT
			TimeInterval,
		sccpNSRVT
			INTEGER,
		sccpSrvtName
			CommonLanguageName,
		sccpAdministrativeState
			AdministrativeState,
		sccpOperationalState
			OperationalState,
		sccpProceduralStatus
			ProceduralStatus,
		sccpTraceRequested
			TruthValue,
		sccpThreshold
			INTEGER,
		sccpMtpBackwardRoutingRequested
			TruthValue,
		sccpOriginalGT
			GlobalTitle,
		sccpInfoRequest
			BITS,
		sccpReturnUnknownParams
			BITS,
		sccpRouteTestRowStatus
			RowStatus
	}

sccpRouteTestId OBJECT-TYPE
	SYNTAX		SimpleNameType
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"This attribute is used for naming conceptual rows 
		 following the SimpleNameType textual convention."
	REFERENCE
		"ITU-T Rec. Q.751.2 routeTestId."
	::= { sccpSrvtEntry xx }

sccpDSRVT OBJECT-TYPE
	SYNTAX		TimeInterval
	UNITS		"seconds"
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"The estimated maximum delay between relay points DSRVT 
		 as described in 3.2.2.5/Q.753.  This value is needed 
		 for the calculation of the SRVT Guard Timer T2."
	REFERENCE
		"ITU-T Rec. Q.752.1 dSRVT ATTRIBUTE, ITU-T Rec. Q.753 
		 3.2.2.5."
	::= { sccpSrvtEntry xx }

sccpNSRVT OBJECT-TYPE
	SYNTAX		INTEGER
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"This attribute equals to the value of the NSRVT as 
		 described in 3.2.2.5/Q.753."
	REFERENCE
		"ITU-T Rec. Q.752.1 nSRVT ATTRIBUTE, ITU-T Rec. Q.753 
		 3.2.2.5."
	::= { sccpSrvtEntry xx }

sccpSrvtName OBJECT-TYPE
	SYNTAX		CommonLanguageName
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"Provides an optional COMMON LANGAUGE name for the SCCP 
		 Route Verification test module in accordance with the 
		 common language textual convention.  This attribute is 
		 optional and when unused (the defautl) is set to the 
		 null string."
	REFERENCE
		"ITU-T Rec. Q.752.1 srvtName ATTRIBUTE."
	DEFVAL		{ ''X }
	::= { sccpSrvtEntry xx }

sccpAdministrativeState OBJECT-TYPE
	SYNTAX		AdministrativeState
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"Unlike CMIP, SNMP has no way of providing actions nor 
		 parameters for actions: this column (and the 
		 perationalState and proceduralStatus below) appear in 
		 this table to model the startSRVT action and subsequent 
		 columns model the parameters of the startSVRT action.  
		 The sccpRouteTestResult notification and the 
		 operationalStatus and proceduralStatus columns model 
		 the result of the action.

		 Rows in this table are created by default in the 
		 locked(0) administrativeState unless the 
		 adminstrativeState is explicity set by management 
		 action when a row is created.  Setting the 
		 administrativeState to unlocked(1) results in the 
		 initiation of the startSRVT action with the parameters 
		 indicated in subsequent columns.  After being launched, 
		 the sccpOperationalState and sccpProceduralStatus 
		 columsn reflect the operational and procedural state of 
		 the SRVT test.  A sccpRouteTestResult notification will 
		 be issued when SRVT test either fails or succeeds (but 
		 not if it is cancelled).

		 Setting the administrative state to locked(1) when the 
		 operational state is enabled(1) will result in the 
		 cancellation of the test.  The results will not be 
		 reported.  Setting the adminstrative state to 
		 shuttingDown(2) has no effect: the test will completed 
		 and the results reported."
	REFERENCE
		"ITU-T Rec. Q.753 startSRVT ConfirmedAction,
		 ITU-T Rec. Q.751.2 startSccpRouteTest ACTION."
	DEFVAL		{ locked(0) }
	::= { sccpSrvtEntry xx }

sccpOperationalState OBJECT-TYPE
	SYNTAX		OperationalState
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
		"Provides the operational state of the SCCP Route 
		 Verification test in progress.  The operational state 
		 is disabled(0) when the test is not running, or when it 
		 has concluded.  The operational state is enabled(1) 
		 when the test is running and has not yet concluded.

		 This object uses the OperationalState textual 
		 convention from the OPENSS7-TMN-TC module."
	REFERENCE
		"ITU-T Rec. Q.753 (1997) startSRVT ConfirmedAction,
		 ITU-T Rec. Q.751.2 (1992) startSccpRouteTest ACTION,
		 ITU-T Rec. X.721 (1992) | ISO/IEC 10165-2 : 1992 
		 operationalState."
	::= { sccpSrvtEntry xx }

sccpProceduralStatus OBJECT-TYPE
	SYNTAX		ProceduralStatus
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
		"Provides the procedural statu of the SCCP Route 
		 Verification test in progress.

		 - initializationRequired(0): The procedural status 
		   contains initialization required when the test 
		   requires initialization to be invoked by the manager 
		   before it will run, and such initialization has not 
		   yet been invoked.  The manager may invoke 
		   intialization by setting the sccpAdministrativeState 
		   to unlocked(1).

		 - notInitialized(1): The procedural status contains not 
		   initialized when the test requires intialization 
		   before it can run and the test has not been 
		   initialized.  The test initializes itself 
		   autononomously, and the operational state is 
		   disabled.

		 - intializing(2): The procedural status contains 
		   initializing when the test required initialization 
		   before it can run, and this initialization has been 
		   initiated but is not yet complete.  When this 
		   condition is present, the initialization required 
		   condition is absent, since initialization has already 
		   begun.  The operational state is enabled.

		 - reporting(3): The procedural status contains 
		   reporting when the test has terminated succesfully or 
		   unsuccessfully, and the result is being reported.

		 - teminating(4): The procedural status contains 
		   terminating when the test is in a termination phase.  
		   The initializationRequired condition is not present 
		   and the operational state is disabled.

		 This object uses the ProceduralStatus textual 
		 convention from the OPENSS7-TMN-TC module."
	REFERENCE
		"ITU-T Rec. Q.753 (1997) startSRVT ConfirmedAction,
		 ITU-T Rec. Q.751.2 (1992) startSccpRouteTest ACTION,
		 ITU-T Rec. X.721 (1992) | ISO/IEC 10165-2 : 1992 
		 proceduralStatus."
	::= { sccpSrvtEntry xx }

sccpTraceRequested
	SYNTAX		TruthValue
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"Unlink CMIP, SNMP has no specific way of providing 
		 parameters for actions: this column appears in this 
		 table to model an action parameter.  This column models 
		 a mandatory parameter of the startSRVT action."
	REFERENCE
		"ITU-T Recommendation Q.753 (1997) ASN.1 startSVRT 
		 traceRequested."
	::= { sccpSrvtEntry xx }

sccpThreshold
	SYNTAX		INTEGER
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"Unlink CMIP, SNMP has no specific way of providing 
		 parameters for actions: this column appears in this 
		 table to model an action parameter.  This column models 
		 a mandatory parameter of the startSRVT action."
	REFERENCE
		"ITU-T Recommendation Q.753 (1997) ASN.1 startSVRT 
		 threshold."
	::= { sccpSrvtEntry xx }

sccpMtpBackwardRoutingRequested
	SYNTAX		TruthValue
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"Unlink CMIP, SNMP has no specific way of providing 
		 parameters for actions: this column appears in this 
		 table to model an action parameter.  This column models 
		 an optional parameter of the startSRVT action.  When 
		 not present, the value is false."
	REFERENCE
		"ITU-T Recommendation Q.753 (1997) ASN.1 startSVRT 
		 mtpBackwardRoutingRequested."
	DEFVAL		{ false(1) }
	::= { sccpSrvtEntry xx }

sccpOriginalGT
	SYNTAX		GlobalTitle
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"Unlink CMIP, SNMP has no specific way of providing 
		 parameters for actions: this column appears in this 
		 table to model an action parameter.  This column models 
		 an optional parameter of the startSRVT action.  When 
		 not present, the length of the octet string 
		 representing the GlobalTitle will be zero (null 
		 string)."
	REFERENCE
		"ITU-T Recommendation Q.753 (1997) ASN.1 startSVRT 
		 originalGT."
	DEFVAL		{ ''X }
	::= { sccpSrvtEntry xx }

sccpInfoRequest
	SYNTAX	BITS {
			pointCode(0),
			pointCodeList(1),
			routePriorityList(2)
		}
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"Unlink CMIP, SNMP has no specific way of providing 
		 parameters for actions: this column appears in this 
		 table to model an action parameter.  This column models 
		 an optional parameter of the startSRVT action.  When 
		 not present, the value is set to all zeros, '000'B.  
		 sccpInfoRequest is used to inidicate that the test 
		 initiator node can accept a routeTraceNew RVR message, 
		 and also asks for particular parameters to be returned 
		 in it, if it is sent.  This parameter can only be 
		 inserted at the initiator node, but it can be copied 
		 into regenerated MRVTs."
	REFERENCE
		"ITU-T Recommendation Q.753 (1997) ASN.1 startSVRT 
		 infoRequest."
	DEFVAL		{ '000'B }
	::= { sccpSrvtEntry xx }

sccpReturnUnknownParams
	SYNTAX	BITS {
			tag15(0),
			tag16(1)
		}
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"Unlink CMIP, SNMP has no specific way of providing 
		 parameters for actions: this column appears in this 
		 table to model an action parameter.  This column models 
		 an optional parameter of the startSRVT action.  When 
		 not present, the value is set to all zeros, '00'B.  The 
		 sccpReturnUnknownParams is used to indicate which 
		 parameters that a node does not understand should be 
		 returned in an RVR if one is sent (or in an RVA message 
		 in the copyData field if the test initiator is 
		 unknown).  Bit 0 represents an RVT parameter with tag 
		 value 15, bit 1 an RVT parameter with tage value 16, 
		 etc.  This parameter can only be present if 
		 sccpInfoRequest is present."
	REFERENCE
		"ITU-T Recommendation Q.753 (1997) ASN.1 startSVRT 
		 returnUnknownParams."
	DEFVAL		{ '00'B }
	::= { sccpSrvtEntry xx }

sccpRouteTestRowStatus OBJECT-TYPE
	SYNTAX		RowStatus
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		""
	REFERENCE
		""
	::= { sccpSrvtEntry xx }


END

