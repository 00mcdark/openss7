lsTimersProfile OBJECT IDENTIFIER
    ::= { mtpObjectClass 1 }

lsTimersProfileTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF LsTimersProfileEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "A signalling link set timer profile is a set of Q.704  
         or T1.111.4 timers which are related to the 
         characteristics of signalling links sets. The managed 
         object class lsTimersProfile is a support managed 
         object class which can be instantiated in order to 
         define a particular profile. At creation time of a 
         signLinkSetTp one of the slTimersProfiles is 
         referenced."
    REFERENCE   
        "ITU-T Rec. Q.751.1 (1997), ANSI T1.116.1 (200), 
         lsTimersProfile MOC."
    ::= { lsTimersProfile 1 }

lsTimersProfileEntry OBJECT-TYPE
    SYNTAX      LsTimersProfileEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        ""
    REFERENCE   
        "ITU-T Rec. Q.751.1 (1997), ANSI T1.116.1 (200), 
         lsTimersProfile MO."
    INDEX       { lsTimersProfileId }
    ::= { lsTimersProfileTable 1 }

LsTimersProfileEntry ::= SEQUENCE {
    lsTimersProfileId           SimpleNameType,
    lsTimersProfileMtpT6        TimeInterval,
    lsTimersProfileMtpT8        TimeInterval,
    lsTimersProfileMtpT10       TimeInterval,
    lsTimersProfileName         SimpleNameType,
    lsTimersProfileRowStatus    RowStatus
}

lsTimersProfileId OBJECT-TYPE
    SYNTAX      SimpleNameType
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Provides an index for the lsTimersProfileTable."
    REFERENCE   
        "ITU-T Rec. Q.751.1 (1997), ANSI T1.116.1 (2000), 
         lsTimersProfileId attribute."
    ::= { lsTimersProfileEntry 1 }

lsTimersProfileMtpT6 OBJECT-TYPE
    SYNTAX      TimeInterval
    UNITS       "centiseconds"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "This attribute defines the initial timer value of timer 
         T6 'Delay to avoid message mis-sequencing on controlled 
         rerouting' defined in ITU-T Recommendation Q.704, ANSI 
         T1.111.4.  The value range is from 500 to 1200 ms. For 
         routes with long propagation delays a minimum value of 
         800 ms is used."
    REFERENCE   
        "ITU-T Rec. Q.751.1 (1997), ANSI T1.116.1 (2000), 
         lsTimersProfileId attribute."
    DEFVAL      { 80 } -- 800 milliseconds
    ::= { lsTimersProfileEntry 2 }

lsTimersProfileMtpT8 OBJECT-TYPE
    SYNTAX      TimeInterval
    UNITS       "centiseconds"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "This attribute defines the initial timer value of timer 
         T8 'Transfer prohibited inhibition timer (transient 
         solution)' defined in ITU-T Recommendation Q.704, ANSI 
         T1.111.4. The value range is from 800 to 1200 ms."
    REFERENCE   
        "ITU-T Rec. Q.751.1 (1997), ANSI T1.116.1 (2000), 
         lsTimersProfileId attribute."
    DEFVAL      { 1000 } -- 1000 milliseconds
    ::= { lsTimersProfileEntry 3 }

lsTimersProfileMtpT10 OBJECT-TYPE
    SYNTAX      TimeInterval
    UNITS       "centiseconds"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "This attribute defines the initial timer value of timer 
         T10 'Waiting to repeat signalling route set test 
         message' defined in ITU-T Recommendation Q.704, ANSI 
         T1.111.4. The value range is from 30 to 60 seconds. The 
         maximum value may be extended at the discretion of the 
         management function in certain situations, e.g. many 
         signalling points being unavailable or signalling 
         points of known long term unavailability."
    REFERENCE   
        "ITU-T Rec. Q.751.1 (1997), ANSI T1.116.1 (2000), 
         lsTimersProfileId attribute."
    DEFVAL      { 4500 } -- 45 seconds
    ::= { lsTimersProfileEntry 4 }

lsTimersProfileName OBJECT-TYPE
    SYNTAX      SimpleNameType
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "This attribute is an additional name for instances of 
         the lsTimersProfile managed object class."
    REFERENCE   
        "ITU-T Rec. Q.751.1 (1997), ANSI T1.116.1 (2000), 
         lsTimersProfileId attribute."
    DEFVAL      { "" }
    ::= { lsTimersProfileEntry 5 }

lsTimersProfileRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "Provides a mechanism whereby management stations may 
         create and delete rows in this table.  Rows created in 
         this table must survive an agent reboot.  Columns that 
         are not specified at the time of row creation will 
         assume their defined default value.  Rows that are 
         referenced by other tables in this MIB cannot be 
         deleted."
    REFERENCE   
        "STD 58, SNMPv2-TC RowStatus.
         ITU-T Rec. Q.751.1 (1997), ANSI T1.116.1 (2000), 
             managedSwitchingElement-lsTimersProfile name binding."
    ::= { lsTimersProfileEntry 6 }

