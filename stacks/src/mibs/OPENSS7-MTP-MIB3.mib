    REFERENCE   
        "ITU-T Rec. Q.751.1 (1997), ANSI T1.116.1 (2000), 
         signRouteSetNePartName attribute."
    ::= { signRouteSetNePartEntry 10 }

signRouteSetNePartRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "Provides a mechanism whereby management stations may 
         create and delete entries in this table."
    REFERENCE   
        "STD 58, SNMPv2-TC RowStatus."
    ::= { signRouteSetNePartEntry 11 }

