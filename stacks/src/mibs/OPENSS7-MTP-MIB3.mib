    SYNTAX      SEQUENCE OF NarrowbandProtocolProfileEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Provides an auxillary table of narrowband specific 
         MTP Level 2 protocol profile information."
    ::= { narrowbandProtocolProfile 1 }

narrowbandProtocolProfileEntry OBJECT-TYPE
    SYNTAX      NarrowbandProtocolProfileEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Each entry in this table shares an index with an entry 
         in the mtpL2ProtocolProfileTable which provides the 
         common MTP Level 2 protocol profile information whereas 
         each of these entries provides narrowband signalling 
         link specific information."
    INDEX       { mtpL2ProtocolProfileId }
    ::= { narrowbandProtocolProfileTable 1 }

NarrowbandProtocolProfileEntry ::= SEQUENCE {
    transmissionRate                        INTEGER,
    narrowbandProtocolProfileRowStatus      RowStatus
}

transmissionRate OBJECT-TYPE
    SYNTAX      INTEGER { kbits4point8(0), kbits56(1), kbits64(2), 
                    kbits1544(3), kbits2048(4) }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "This attribute specifies the transmissionRate and 
         implies the transmissionType (i.e. 56 and 64 kbits 
         imply digital, 4.8 kbits implies analog 
         transmission)."
    REFERENCE   
        "ITU-T Rec. Q.751.1 (1997), ANSI T1.116.1 (2000), 
         transmissionRate attribute."
    ::= { narrowbandProtocolProfileEntry 1 }

narrowbandProtocolProfileRowStatus
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "Provides a mechanism whereby management stations may 
         create and delete rows in this table.  Rows in this 
         table provide narrowband information pertaining to the 
         indexed mtpL2ProtocolProfileTable entry.  Deleting the 
         corresponding entry in the mtpL2ProtocolProfileTable 
         results in deletion of this entry."
    REFERENCE   
        "STD 58, SNMPv2-TC RowStatus."
    ::= { narrowbandProtocolProfileEntry 2 }

