    signRouteSetNePartAdministrativeState                       AdministrativeState,
    signRouteSetNePartOperationalState                          OperationalState,
    signRouteSetNePartAlarmSeverityAssignmentProfilePointer     RowPointer,
    congestedState                                              INTEGER,
    congestionLevel                                             CongestionLevel,
    loadsharingInformationRouteSetNePart                        VisibleString,
    mtpLoadsharingObjectForRouteSetNePartPointer                RowPointer,
    remoteExchangeLabel                                         VisibleString,
    signRouteSetNePartName                                      VisibleString,
    signRouteSetNePartRowStatus                                 RowStatus
}

