	}

slProfileIndex OBJECT-TYPE
	SYNTAX		Unsigned32
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"Provides an index into the SL Profile table."
	::= { slProfileEntry 1 }

slProfileName OBJECT-TYPE
	SYNTAX		CommonLanguageName
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"Provides the name of the entry in the SL Profile table."
	::= { slProfileEntry 2 }

slProfileT1 OBJECT-TYPE
	SYNTAX		TimeInterval
	UNITS		"milliseconds"
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"Timeout interval T1.  This is the time interval that a
		 signalling link will await an MSU or FISU after sending
