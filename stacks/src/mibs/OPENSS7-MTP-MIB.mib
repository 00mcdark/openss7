		MtpTimerT10			Os7MtpTimerT10,
		MtpLsTimersProfileStatus	RowStatus
	}

MtpLsTimersProfileName OBJECT-TYPE
	SYNTAX		Os7MtpLsTimersProfileId
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"The name of the Ls Timer Profile."
