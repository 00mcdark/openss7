SlProfileEntry ::=
	SEQUENCE {
		slProfileIndex		Unsigned32,
		slProfileName		CommonLanguageName,
		slProfileT1		TimeInterval,
		slProfileT2		TimeInterval,
		slProfileT2l		TimeInterval,
		slProfileT2h		TimeInterval,
		slProfileT3		TimeInterval,
		slProfileT4n		TimeInterval,
		slProfileT4e		TimeInterval,
		slProfileT5		TimeInterval,
		slProfileT6		TimeInterval,
		slProfileT7		TimeInterval,
		slProfileRbAccept	Unsigned32,
		slProfileRbDiscard	Unsigned32,
		slProfileRbAbate	Unsigned32,
		slProfileTbAbate0	Unsigned32,
		slProfileTbOnset0	Unsigned32,
		slProfileTbDiscard0	Unsigned32,
		slProfileTbAbate1	Unsigned32,
		slProfileTbOnset1	Unsigned32,
		slProfileTbDiscard1	Unsigned32,
		slProfileTbAbate2	Unsigned32,
		slProfileTbOnset2	Unsigned32,
		slProfileTbDiscard2	Unsigned32,
		slProfileN1		Unsigned32,
		slProfileN2		Unsigned32,
