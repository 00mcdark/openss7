		MtpSignLinkSetLoadShareAlgorithm	Os7MtpSignLinkSetLoadShareAlgorithm,
		MtpLsTimersProfileName			Os7MtpLsTimersProfileId,
		MtpLsNumberOfNormallyActiveLinks	Os7MtpLsNumberOfNormallyActiveLinks,
		MtpSignLinkSetStatus			RowStatus
	}

MtpSignLinkSetName OBJECT-TYPE
	SYNTAX		Os7MtpSignLinkSetId
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"The name of the SS7 MTP Signalling Link Set."
	::= { MtpSignLinkSetEntry 1 }

MtpSignLinkSetLocSp OBJECT-TYPE
	SYNTAX		Os7MtpSignPointId
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"The name of the SS7 MTP Signalling Point to which this Link Set
		 belongs."
