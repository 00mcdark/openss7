MIBS=+OPENSS7-M2PA-MIB:OPENSS7-M2UA-ASP-MIB:OPENSS7-M2UA-MIB:OPENSS7-M3UA-ASP-MIB:OPENSS7-M3UA-SGP-MIB:OPENSS7-METRIC-MIB:OPENSS7-SIGTRAN-MIB:OPENSS7-SIGTRAN-TC
MIBDIRS=./mibs:/usr/share/snmp/mibs
MIBFILES=./mibs/OPENSS7-M2PA-MIB.mib:./mibs/OPENSS7-M2UA-ASP-MIB.mib:./mibs/OPENSS7-M2UA-MIB.mib:./mibs/OPENSS7-M3UA-ASP-MIB.mib:./mibs/OPENSS7-M3UA-SGP-MIB.mib:./mibs/OPENSS7-METRIC-MIB.mib:./mibs/OPENSS7-SIGTRAN-MIB.mib:./mibs/OPENSS7-SIGTRAN-TC.mib

#mibs/OPENSS7-M2PA-MIB::m2paMIB
#mibs/OPENSS7-M2UA-ASP-MIB::m2uaAspMIB
#mibs/OPENSS7-M2UA-MIB::m2uaMIB
#mibs/OPENSS7-M3UA-ASP-MIB::m3uaAspMIB
#mibs/OPENSS7-M3UA-SGP-MIB::m3uaSgpMIB
#mibs/OPENSS7-METRIC-MIB::metricMIB
#mibs/OPENSS7-SIGTRAN-MIB::sigtranMIB

export MIBS
export MIBDIRS
export MIBFILES

all: simple storage openss7 master

check: \
	m2paMIB.tree \
	m2uaAspMIB.tree \
	m2uaMIB.tree \
	m3uaAspMIB.tree \
	m3uaSgpMIB.tree \
	sigtranMIB.tree
#	metricMIB.tree

m2paMIB.tree: mibs/OPENSS7-M2PA-MIB.mib
	snmptranslate -Tp OPENSS7-M2PA-MIB::m2paMIB 2>&1 | tee $@

m2uaAspMIB.tree: mibs/OPENSS7-M2UA-ASP-MIB.mib
	snmptranslate -Tp OPENSS7-M2UA-ASP-MIB::m2uaAspMIB 2>&1 | tee $@

m2uaMIB.tree: mibs/OPENSS7-M2UA-MIB.mib
	snmptranslate -Tp OPENSS7-M2UA-MIB::m2uaMIB 2>&1 | tee $@

m3uaAspMIB.tree: mibs/OPENSS7-M3UA-ASP-MIB.mib
	snmptranslate -Tp OPENSS7-M3UA-ASP-MIB::m3uaAspMIB 2>&1 | tee $@

m3uaSgpMIB.tree: mibs/OPENSS7-M3UA-SGP-MIB.mib
	snmptranslate -Tp OPENSS7-M3UA-SGP-MIB::m3uaSgpMIB 2>&1 | tee $@

metricMIB.tree: mibs/OPENSS7-METRIC-MIB.mib
	snmptranslate -Tp OPENSS7-METRIC-MIB::metricMIB 2>&1 | tee $@

sigtranMIB.tree: mibs/OPENSS7-SIGTRAN-MIB.mib
	snmptranslate -Tp OPENSS7-SIGTRAN-MIB::sigtranMIB 2>&1 | tee $@

simple: \
	m2paMIB_simple.c m2paMIB_simple.h \
	m2uaAspMIB_simple.c m2uaAspMIB_simple.h \
	m2uaMIB_simple.c m2uaMIB_simple.h \
	m3uaAspMIB_simple.c m3uaAspMIB_simple.h \
	m3uaSgpMIB_simple.c m3uaSgpMIB_simple.h \
	sigtranMIB_simple.c sigtranMIB_simple.h
#	metricMIB_simple.c metricMIB_simple.h

m2paMIB_simple.c m2paMIB_simple.h: mib2c mib2c.conf mibs/OPENSS7-M2PA-MIB.mib
	./mib2c -c ./mib2c.conf -f m2paMIB_simple m2paMIB
	indent -nhnl m2paMIB_simple.[ch]

m2uaAspMIB_simple.c m2uaAspMIB_simple.h: mib2c mib2c.conf mibs/OPENSS7-M2UA-ASP-MIB.mib
	./mib2c -c ./mib2c.conf -f m2uaAspMIB_simple m2uaAspMIB
	indent -nhnl m2uaAspMIB_simple.[ch]

m2uaMIB_simple.c m2uaMIB_simple.h: mib2c mib2c.conf mibs/OPENSS7-M2UA-MIB.mib
	./mib2c -c ./mib2c.conf -f m2uaMIB_simple m2uaMIB
	indent -nhnl m2uaMIB_simple.[ch]

m3uaAspMIB_simple.c m3uaAspMIB_simple.h: mib2c mib2c.conf mibs/OPENSS7-M3UA-ASP-MIB.mib
	./mib2c -c ./mib2c.conf -f m3uaAspMIB_simple m3uaAspMIB
	indent -nhnl m3uaAspMIB_simple.[ch]

m3uaSgpMIB_simple.c m3uaSgpMIB_simple.h: mib2c mib2c.conf mibs/OPENSS7-M3UA-SGP-MIB.mib
	./mib2c -c ./mib2c.conf -f m3uaSgpMIB_simple m3uaSgpMIB
	indent -nhnl m3uaSgpMIB_simple.[ch]

sigtranMIB_simple.c sigtranMIB_simple.h: mib2c mib2c.conf mibs/OPENSS7-SIGTRAN-MIB.mib
	./mib2c -c ./mib2c.conf -f sigtranMIB_simple sigtranMIB
	indent -nhnl sigtranMIB_simple.[ch]

metricMIB_simple.c metricMIB_simple.h: mib2c mib2c.conf mibs/OPENSS7-METRIC-MIB.mib
	./mib2c -c ./mib2c.conf -f metricMIB_simple metricMIB
	indent -nhnl metricMIB_simple.[ch]

storage: \
	m2paMIB_storage.c m2paMIB_storage.h \
	m2uaAspMIB_storage.c m2uaAspMIB_storage.h \
	m2uaMIB_storage.c m2uaMIB_storage.h \
	m3uaAspMIB_storage.c m3uaAspMIB_storage.h \
	m3uaSgpMIB_storage.c m3uaSgpMIB_storage.h \
	sigtranMIB_storage.c sigtranMIB_storage.h
#	metricMIB_storage.c metricMIB_storage.h

m2paMIB_storage.c m2paMIB_storage.h: mib2c mib2c.storage.conf mibs/OPENSS7-M2PA-MIB.mib
	./mib2c -c ./mib2c.storage.conf -f m2paMIB_storage m2paMIB
	indent -nhnl m2paMIB_storage.[ch]

m2uaAspMIB_storage.c m2uaAspMIB_storage.h: mib2c mib2c.storage.conf mibs/OPENSS7-M2UA-ASP-MIB.mib
	./mib2c -c ./mib2c.storage.conf -f m2uaAspMIB_storage m2uaAspMIB
	indent -nhnl m2uaAspMIB_storage.[ch]

m2uaMIB_storage.c m2uaMIB_storage.h: mib2c mib2c.storage.conf mibs/OPENSS7-M2UA-MIB.mib
	./mib2c -c ./mib2c.storage.conf -f m2uaMIB_storage m2uaMIB
	indent -nhnl m2uaMIB_storage.[ch]

m3uaAspMIB_storage.c m3uaAspMIB_storage.h: mib2c mib2c.storage.conf mibs/OPENSS7-M3UA-ASP-MIB.mib
	./mib2c -c ./mib2c.storage.conf -f m3uaAspMIB_storage m3uaAspMIB
	indent -nhnl m3uaAspMIB_storage.[ch]

m3uaSgpMIB_storage.c m3uaSgpMIB_storage.h: mib2c mib2c.storage.conf mibs/OPENSS7-M3UA-SGP-MIB.mib
	./mib2c -c ./mib2c.storage.conf -f m3uaSgpMIB_storage m3uaSgpMIB
	indent -nhnl m3uaSgpMIB_storage.[ch]

sigtranMIB_storage.c sigtranMIB_storage.h: mib2c mib2c.storage.conf mibs/OPENSS7-SIGTRAN-MIB.mib
	./mib2c -c ./mib2c.storage.conf -f sigtranMIB_storage sigtranMIB
	indent -nhnl sigtranMIB_storage.[ch]

metricMIB_storage.c metricMIB_storage.h: mib2c mib2c.storage.conf mibs/OPENSS7-METRIC-MIB.mib
	./mib2c -c ./mib2c.storage.conf -f metricMIB_storage metricMIB
	indent -nhnl metricMIB_storage.[ch]

openss7: \
	m2paMIB_openss7.c m2paMIB_openss7.h \
	m2uaAspMIB_openss7.c m2uaAspMIB_openss7.h \
	m2uaMIB_openss7.c m2uaMIB_openss7.h \
	m3uaAspMIB_openss7.c m3uaAspMIB_openss7.h \
	m3uaSgpMIB_openss7.c m3uaSgpMIB_openss7.h \
	sigtranMIB_openss7.c sigtranMIB_openss7.h
#	metricMIB_openss7.c metricMIB_openss7.h

m2paMIB_openss7.c m2paMIB_openss7.h: mib2c mib2c.openss7.conf mibs/OPENSS7-M2PA-MIB.mib
	./mib2c -c ./mib2c.openss7.conf -f m2paMIB_openss7 m2paMIB
	indent -nhnl m2paMIB_openss7.[ch]

m2uaAspMIB_openss7.c m2uaAspMIB_openss7.h: mib2c mib2c.openss7.conf mibs/OPENSS7-M2UA-ASP-MIB.mib
	./mib2c -c ./mib2c.openss7.conf -f m2uaAspMIB_openss7 m2uaAspMIB
	indent -nhnl m2uaAspMIB_openss7.[ch]

m2uaMIB_openss7.c m2uaMIB_openss7.h: mib2c mib2c.openss7.conf mibs/OPENSS7-M2UA-MIB.mib
	./mib2c -c ./mib2c.openss7.conf -f m2uaMIB_openss7 m2uaMIB
	indent -nhnl m2uaMIB_openss7.[ch]

m3uaAspMIB_openss7.c m3uaAspMIB_openss7.h: mib2c mib2c.openss7.conf mibs/OPENSS7-M3UA-ASP-MIB.mib
	./mib2c -c ./mib2c.openss7.conf -f m3uaAspMIB_openss7 m3uaAspMIB
	indent -nhnl m3uaAspMIB_openss7.[ch]

m3uaSgpMIB_openss7.c m3uaSgpMIB_openss7.h: mib2c mib2c.openss7.conf mibs/OPENSS7-M3UA-SGP-MIB.mib
	./mib2c -c ./mib2c.openss7.conf -f m3uaSgpMIB_openss7 m3uaSgpMIB
	indent -nhnl m3uaSgpMIB_openss7.[ch]

sigtranMIB_openss7.c sigtranMIB_openss7.h: mib2c mib2c.openss7.conf mibs/OPENSS7-SIGTRAN-MIB.mib
	./mib2c -c ./mib2c.openss7.conf -f sigtranMIB_openss7 sigtranMIB
	indent -nhnl sigtranMIB_openss7.[ch]

metricMIB_openss7.c metricMIB_openss7.h: mib2c mib2c.openss7.conf mibs/OPENSS7-METRIC-MIB.mib
	./mib2c -c ./mib2c.openss7.conf -f metricMIB_openss7 metricMIB
	indent -nhnl metricMIB_openss7.[ch]

master: \
	sigtrand_master.c sigtrand_master.h

sigtrand_master.c sigtrand_master.h: mib2c mib2c.master.conf mibs/OPENSS7-SIGTRAN-MIB.mib
	./mib2c -c ./mib2c.master.conf -f sigtrand_master sigtranMIB
	indent -nhnl sigtrand_master.[ch]

clean:
	rm -f sigtrand_master.[ch]
	rm -f sigtrand_master.[ch]~
	rm -f m2paMIB.tree
	rm -f m2uaAspMIB.tree
	rm -f m2uaMIB.tree
	rm -f m3uaAspMIB.tree
	rm -f m3uaSgpMIB.tree
	rm -f sigtranMIB.tree
	rm -f metricMIB.tree
	rm -f m2paMIB_simple.[ch]
	rm -f m2uaAspMIB_simple.[ch]
	rm -f m2uaMIB_simple.[ch]
	rm -f m3uaAspMIB_simple.[ch]
	rm -f m3uaSgpMIB_simple.[ch]
	rm -f sigtranMIB_simple.[ch]
	rm -f metricMIB_simple.[ch]
	rm -f m2paMIB_simple.[ch]~
	rm -f m2uaAspMIB_simple.[ch]~
	rm -f m2uaMIB_simple.[ch]~
	rm -f m3uaAspMIB_simple.[ch]~
	rm -f m3uaSgpMIB_simple.[ch]~
	rm -f sigtranMIB_simple.[ch]~
	rm -f metricMIB_simple.[ch]~
	rm -f m2paMIB_storage.[ch]
	rm -f m2uaAspMIB_storage.[ch]
	rm -f m2uaMIB_storage.[ch]
	rm -f m3uaAspMIB_storage.[ch]
	rm -f m3uaSgpMIB_storage.[ch]
	rm -f sigtranMIB_storage.[ch]
	rm -f metricMIB_storage.[ch]
	rm -f m2paMIB_storage.[ch]~
	rm -f m2uaAspMIB_storage.[ch]~
	rm -f m2uaMIB_storage.[ch]~
	rm -f m3uaAspMIB_storage.[ch]~
	rm -f m3uaSgpMIB_storage.[ch]~
	rm -f sigtranMIB_storage.[ch]~
	rm -f metricMIB_storage.[ch]~
	rm -f m2paMIB_openss7.[ch]
	rm -f m2uaAspMIB_openss7.[ch]
	rm -f m2uaMIB_openss7.[ch]
	rm -f m3uaAspMIB_openss7.[ch]
	rm -f m3uaSgpMIB_openss7.[ch]
	rm -f sigtranMIB_openss7.[ch]
	rm -f metricMIB_openss7.[ch]
	rm -f m2paMIB_openss7.[ch]~
	rm -f m2uaAspMIB_openss7.[ch]~
	rm -f m2uaMIB_openss7.[ch]~
	rm -f m3uaAspMIB_openss7.[ch]~
	rm -f m3uaSgpMIB_openss7.[ch]~
	rm -f sigtranMIB_openss7.[ch]~
	rm -f metricMIB_openss7.[ch]~
