objname	module	m2pa-sl	    m2pa_sl
#objname module	m2ua-as	    m2ua_as
#objname module	m3ua-as	    m3ua_as
objname	module	sdt-tpi	    sdt_tpi
objname	module	sl-tpi	    sl_tpi
objname	module	sdl-sctp    sdl_sctp
objname	module	sdt-sctp    sdt_sctp
objname	module	sl-sctp	    sl_sctp
objname	module	m2tp	    m2tp

##
## Loadable specifications
## -----------------------------------------------------------
