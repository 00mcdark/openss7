m4_divert_push([DEFAULTS])
function test_ip() {
    test_prog test-ip "$@"
}
function test_ip_root() {
    test_prog_root test-ip "$@"
}
m4_divert_pop([DEFAULTS])

AT_TESTED([test-ip])
AT_BANNER([NPI-IP driver tests])

AT_SETUP([npi-ip test 1.1])
AT_KEYWORDS([npi-ip])
AT_CHECK([test_ip 1.1], [], [ignore], [], [], [])
AT_CLEANUP

AT_SETUP([npi-ip test 1.2.1])
AT_KEYWORDS([npi-ip])
AT_CHECK([test_ip 1.2.1], [], [ignore], [], [], [])
AT_CLEANUP

