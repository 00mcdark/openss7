networkEntityTable OBJECT-TYPE
	SYNTAX		SEQUENCE OF NetworkEntityEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"The network entity managed object.  There may be 
		 multiple instances of these MOs within a system.  Its 
		 definition permits it to be deleted and created 
		 explicitly by management operation, or to be created 
		 and deleted automatically as a part of system 
		 operation."
	REFERENCE
		"ITU-T Rec. X.283 | ISO/IEC 10733 networkEntity MOC."
	::= { networkEntity 1 }

networkEntityEntry OBJECT-TYPE
	SYNTAX		NetworkEntityEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"Entries in this table generated an objectCreation 
		 notification when a row is added and an objectDeletion 
		 notification when a row is removed."
	REFERENCE
		"ITU-T Rec. X.283 | ISO/IEC 10733 networkEntity MO."
	INDEX	{
			communicationsEntityId -- shared index (derived from communicationsEntity)
		}
	::= { networkEntityTable 1 }

NetworkEntityEntry ::=
	SEQUENCE {
		networkEntityTitles
			OCTET STRING,
		networkEntitySystemTypes
			SystemTypes,
		-- networkEntity-networkSubsystem-Automatic name binding
		-- networkEntity-networkSubsystem-Management name binding
		networkEntityRowStatus
			RowStatus
	}

networkEntityTitles OBJECT-TYPE
	SYNTAX		NAddresses
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"The set of Network Entity Titles (having the same 
		 abstract syntax as an NSAP address), which 
		 unambiguously identify the Network Entity in an End or 
		 Intermediate System. The value may be entered by a 
		 system management operation or it may be derived by 
		 some local means, for example by autoconfiguration.

		 The OCTET STRING is a set of NAddresses which consist 
		 of one octet length (0..20) and length octets of 
		 address, followed by the next NAddress."
	REFERENCE
		"ITU-T Rec. X.283 | ISO/IEC 10733 networkEntityTitles."
	::= { networkEntityEntry 1 }

networkEntitySystemTypes OBJECT-TYPE
	SYNTAX		SystemTypes
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
