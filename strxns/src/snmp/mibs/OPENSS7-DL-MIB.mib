reachableAddressTable OBJECT-TYPE
	SYNTAX		SEQUENCE OF ReachableAddressEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"Reachable address managed object.  Created either 
		 through the reachableAddress-linkage-imported Name 
		 binding for reachable addresses instantiated by the 
		 importation of routeing information from another 
		 routeing protocol (such as the interdomain routeing 
		 protocol), or the reachableAddress-linkage-management 
		 name binding for adjacencies created via explicit 
		 system management operation.
		 
		 The table issues an objectCreation notification when 
		 entries are created and added to the table.  The table 
		 issues an objectDeletion notification when entries are 
		 deleted from the table.
		 "
	REFERENCE
		"ISO/IEC 10589 reachableAddress MOC."
	::= { reachableAddress 1 }

reachableAddressEntry OBJECT-TYPE
	SYNTAX		ReachableAddressEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"
		 Each entry issues a stateChange notification should the 
		 operationalState change indicating that the 
		 operationalState has changed.

		 Each entry supports an activate action and a deactivate 
		 action.  These actions are emulated in SNMP using the 
		 administrativeState column.
		 "
	REFERENCE
		"ISO/IEC 10589 reachableAddress MO."
	INDEX	{
		}
	::= { reachableAddressTable 1 }

ReachableAddressEntry ::=
	SEQUENCE {
		reachableAddressId
			SimpleNameType,
		reachableAddressPrefix
			AddressPrefix,
		reachableAddressMappingType
			INTEGER,
		reachableAddressDefaultMetric
			HopMetric,
		reachableAddressDelayMetric
			HopMetric,
		reachableAddressExpenseMetric
			HopMetric,
		reachableAddressErrorMetric
			HopMetric,
		reachableAddressDefaultMetricType
			MetricType,
		reachableAddressDelayMetricType
			MetricType,
		reachableAddressExpenseMetricType
			MetricType,
		reachableAddressErrorMetricType
			MetricType,
		reachableAddressOperationalState
			OperationalState,
		reachableAddressAdministrativeState
			AdministrativeState,
		-- explicitSNPA-P conditional package
		reachableAddressSNPAAddresses
			SNPAAddresses,
		-- extractDSP-P conditional package
		reachableAddressSNPAMask
			NAddress,
		reachableAddressSNPAPrefix
			SNPAPrefix,
		-- reachableAddress-linkage-imported name binding
		-- reachableAddress-linkage-management name binding
		reachableAddressRowStatus
			RowStatus
	}

reachableAddressId OBJECT-TYPE
	SYNTAX		SimpleNameType
	MAX-ACCESS	not-accessible -- index
	STATUS		current
	DESCRIPTION
		"A string which is the Identifier for the 
		 ReachableAddress and which is unique amongst the set of 
		 reachable addresses maintained for this linkage. If 
		 this is a reachableAddress created by system 
		 management, it is set by the System Manager when the 
		 ReachableAddress is created, otherwise it is generated 
		 by the implementation such that it is unique. The set 
		 of identifiers containing the leading string 'Auto' are 
		 reserved for reachable addresses imported from other 
		 routeing protocols. An attempt by system management to 
		 create a reachableAddress with such an identifier will 
		 cause a reserved name violation."
	REFERENCE
		"ISO/IEC 10589 reachableAddressId."
	::= { reachableAddressEntry xx }

reachableAddressPrefix OBJECT-TYPE
	SYNTAX		AddressPrefix
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"An Area Address of a destination area or a prefix for a 
		 reachable address.

		 Value is supplied either by the protocol machine if the 
		 MO is automatically created, or supplied via the CREATE 
		 Operation. The value cannot be changed thereafter."
	REFERENCE
		"ISO/IEC 10589 addressPrefix."
	::= { reachableAddressEntry xx }

reachableAddressMappingType OBJECT-TYPE
	SYNTAX	INTEGER {
			none(0),
			explicit(1),
			extractIDI(2),
			extractDSP(3)
		}
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"The type of mapping to be employed to ascertain the 
		 SNPA Address which should be used in forwarding NPDUs 
		 for this Reachable Address Prefix. The following values 
		 of mappingType are defined:

		 none: The mapping is null because the neighbour SNPA is 
		     implicit by nature of the subnetwork (e.g. a 
		     point-to-point linkage).

		 explicit: The set of subnetwork addresses in the 
		     sNPAAddresses attribute are to be used.

		 extractIDI: The SNPA is embedded in the IDI of the 
		     destination NSAP address. The mapping algorithm 
		     extracts the SNPA to be used according to the 
		     format and encoding rules of ISO8348/Add2. This 
		     SNPA extraction algorithm can be used in 
		     conjunction with Reachable Address Prefixes from 
		     the X.121, F.69, E.163, and E.164 addressing 
		     subdomains.

		 extractDSP: All, or a suffix, of the SNPA is embedded 
		     in the DSP of the destination address. This SNPA 
		     extraction algorithm extracts the embedded 
		     subnetwork addressing information by performing a 
		     logical AND of the sNPAMask attribute with the 
		     destinaition address. The part of the SNPA 
		     extracted from the destination NSAP is appended to 
		     the sNPAPrefix to form the next hop subnetwork 
		     addressing information.

		 Value is supplied either by the protocol machine if the 
		 MO is automatically created, or supplied via the CREATE 
		 Operation. The value cannot be changed thereafter."
	REFERENCE
		"ISO/IEC 10589 mappingType."
	::= { reachableAddressEntry xx }

reachableAddressDefaultMetric OBJECT-TYPE
	SYNTAX		HopMetric(1..63)
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"The default metric value for reaching the specified 
		 prefix over this Circuit. If this attribute is changed 
		 while both the Reachable Address and the linkage are 
		 Enabled (i.e. state On), the actions described in 
		 clause 8.3.5.4 must be taken."
	REFERENCE
		"ISO/IEC 10589 defaultMetric."
	DEFVAL		{ 20 }
	::= { reachableAddressEntry xx }

reachableAddressDelayMetric OBJECT-TYPE
	SYNTAX		HopMetric
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"The delay metric value for reaching the specified 
		 prefix over this Circuit. If this attribute is changed 
		 while both the Reachable Address and the linkage are 
		 Enabled (i.e. state On), the actions described in 
		 clause 8.3.5.4 must be taken.  The value of zero is 
		 reserved to indicate that this metric is not 
		 supported."
	REFERENCE
		"ISO/IEC 10589 delayMetric."
	DEFVAL		{ 0 }
	::= { reachableAddressEntry xx }

reachableAddressExpenseMetric OBJECT-TYPE
	SYNTAX		HopMetric
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"The expense metric value for reaching the specified 
		 prefix over this Circuit. If this attribute is changed 
		 while both the Reachable Address and the linkage are 
		 Enabled (i.e. state On), the actions described in 
		 clause 8.3.5.4 must be taken.  The value of zero is 
		 reserved to indicate that this metric is not 
		 supported."
	REFERENCE
		"ISO/IEC 10589 expenseMetric."
	DEFVAL		{ 0 }
	::= { reachableAddressEntry xx }

reachableAddressErrorMetric OBJECT-TYPE
	SYNTAX		HopMetric
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"The error metric value for reaching the specified 
		 prefix over this Circuit. If this attribute is changed 
		 while both the Reachable Address and the linkage are 
		 Enabled (i.e. state On), the actions described in 
		 clause 8.3.5.4 must be taken.  The value of zero is 
		 reserved to indicate that this metric is not 
		 supported."
	REFERENCE
		"ISO/IEC 10589 errorMetric."
	DEFVAL		{ 0 }
	::= { reachableAddressEntry xx }

reachableAddressDefaultMetricType OBJECT-TYPE
	SYNTAX		MetricType
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"Indicates whether the default metric is internal or 
		 external."
	REFERENCE
		"ISO/IEC 10589 defaultMetricType."
	DEFVAL		{ internal(0) }
	::= { reachableAddressEntry xx }

reachableAddressDelayMetricType OBJECT-TYPE
	SYNTAX		MetricType
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"Indicates whether the delay metric is internal or 
		 external."
	REFERENCE
		"ISO/IEC 10589 delayMetricType."
	DEFVAL		{ internal(0) }
	::= { reachableAddressEntry xx }

reachableAddressExpenseMetricType OBJECT-TYPE
	SYNTAX		MetricType
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"Indicates whether the expense metric is internal or 
		 external."
	REFERENCE
		"ISO/IEC 10589 expenseMetricType."
	DEFVAL		{ internal(0) }
	::= { reachableAddressEntry xx }

reachableAddressErrorMetricType OBJECT-TYPE
	SYNTAX		MetricType
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"Indicates whether the error metric is internal or 
		 external."
	REFERENCE
		"ISO/IEC 10589 errorMetricType."
	DEFVAL		{ internal(0) }
	::= { reachableAddressEntry xx }

reachableAddressOperationalState OBJECT-TYPE
	SYNTAX		OperationalState
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
		"Provides the operational state of the reachable address 
		 in accordance with the OperationalState textual 
		 convention from the OPENSS7-TMN-TC module."
	REFERENCE
		"ITU-T Rec. X.721 | ISO/IEC 10165-2 operationalState."
