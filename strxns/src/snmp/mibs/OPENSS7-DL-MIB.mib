-- =====================================================================
--
-- Textual Conventions

LLCServices ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"This textual convention represents the LLC services the 
		 are available or selected.  Per ISO/IEC 8802-2, the 
		 services are type1(0), type2(1), type3Initiate(2), 
		 type3ReceiveData(3) and type3ReturnData(4)."
	REFERENCE
		"IEEE 802.2 | ISO/IEC 8802-2 ASN.1, LLCServices."
	SYNTAX	BITS {
			type1(0),
			type2(1),
			type3Initiate(2),
			type3ReceiveData(3),
			type3ReturnData(4)
		}

LLCStationStatus ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"This textual convetion represents the status of an LLC 
		 station.  Per ISO/IEC 8802-2, the status can be up(0), 
		 down(1), or other (2)."
	REFERENCE
		"IEEE 802.2 | ISO/IEC 8802-2 ASN.1, Status."
	SYNTAX	INTEGER {
			up(0),
			down(1),
			other(2)
		}

STRIndicator ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"This textual convention indicates the type of source 
		 routed MAC bridging per ISO/IEC 8802-2 (IEEE 802.2), 
		 the types are nsr(0) or ste(1)."
	SYNTAX	BITS {
			nsr(0),
			ste(1)
		}

MACAddress ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"This textual convention represents a MAC Address.  The 
		 minmum length is 6 octets.  If the MAC address is a 
		 48-bit address, the length of the octet string shall be 
		 6 octets.  If the MAC address is a 60-bit address, the 
		 length of the octet string shall be 8 octets.  The 
		 octet encoding is derived from the hexadecimal 
		 representation order for the MAC address.  
		 AB-CD-EF-GH-IJ-KL for the 48-bit address format; 
		 AB-CD-EF-GH-IJ-KL-MN-OP ofr the 60-bit address format.  
		 The octets are encoded as follows: the first pari of 
		 hexadecimal digits, AB, are encoded in the first octet.  
		 The second pair, CD, is encoded in the second octet, 
		 etc.  (See ISO/IEC 8802-2 6.2, MAC address attribute, 
		 for definition of 60-bit hexadecimal display format.)"
	SYNTAX		OCTET STRING(SIZE(0|6|8))

DLSAP ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"This texutal convention represents a fully qualified 
		 DLSAP which consistes of a 6 or 8 octet MACAddress (as 
		 described for the MACAddress texutal convention) 
		 followed by a one octet DSAP.  When null (zero length 
		 octet string) the value represents no specific address."
	SYNTAX		OCTET STRING(SIZE(0|7|9))

LSAP ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"This textual convention represents an LLC LSAP which 
		 consists of a 1-octet value which contains the LLC 
		 LSAP.  Note that technically the LSAP could be 
		 continued into subsequent octets, however, this is not 
		 normally necessary for X.25/ISO 8208 and CLNS."
	REFERENCE
		"IEEE 802.2 (ISO/IEC 8802-2) ASN.1, LSAP."
	SYNTAX		OCTET STRING(SIZE(1))

LSAPPairId ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"This textual convention represents an RDE Pair ID whcih 
		 consists of a 1-octet value which contains the LLC 
		 SSAP, followed by the 6 or 8 octet MACAddress (the 
		 combination as described in the DLSAP textual 
		 convention), following by anoth 1-octet value which 
		 contains the LLC DSAP, followed by the 6 or 8 octet 
		 MACAddress."
	REFERENCE
		"IEEE 802.2 (ISO/IEC 8802-2) ASN.1, LSAPPairID."
	SYNTAX		OCTET STRING(SIZE(0|14|18))

RoutingInformationField ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"The routing information field varies in length up to 30 
		 octets.  When source routing is not used the RIF is 
		 said to be NULL and contains no information.  The first 
		 bit of the MAC source address is defined as the Routing 
		 Information Indicator (RII). When the RIF is null, the 
		 RII is set to zero (0).  When the RIF is not null, the 
		 RII is set to 1.  The first two octets are the Routing 
		 Control field (RC) and the remainder of the RIF 
		 contains Routing Descriptors (RD).  Each RD is two 
		 octets in length, therefore a maximum of 14 RDs can 
		 exists in a RIF."
	REFERENCE
		"IEEE 802.2 (ISO/IEC 8802-2) ASN.1, RIF."
	SYNTAX		OCTET STRING(SIZE(0..30))

SystemTypes ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"A set-valued textual convention which indicates whether 
		 the system is an end-system, eS(1), or an intermediate 
		 system, iS(2), or both."
	REFERENCE
		"ITU-T Rec. X.283 | ISO/IEC 10733 ASN.1 SystemTypes."
	SYNTAX	BITS {
			eS(1),
			iS(2)
		}

NAddress ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"A network address."
	REFERENCE
		"ITU-T Rec. X.283 | ISO/IEC 10733 ASN.1 NAddress."
	SYNTAX		OCTET STRING(SIZE(0..20))

NAddresses ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"A set of network addresses.  Each network address is 
		 formatted as a single octet length indicator, following 
		 by 'length' octets SIZE(0..20) of NAddress (see the 
		 NAddress textual convention), followed by another 
		 length indicator and NAddress pair, etc."
	REFERENCE
		"ITU-T Rec. X.283 | ISO/IEC 10733 ASN.1 NAddresses."
	SYNTAX		OCTET STRING

X25PLEMode ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"The mode an an X25PLE interface."
	REFERENCE
		"ITU-T Rec. X.283 ASN.1 X25PLEMode | ISO/IEC 10733 ASN.1 
		 InterfaceMode."
	SYNTAX	INTEGER {
			dTE(0),
			dCE(1),
			dTEasDCE(2) -- only X.283
		}

FastSelect ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"Indicates the fast select option selection.  Type of 
		 fast select to used for call. This specifies that one 
		 of `fast select', `fast select with retricted 
		 response', or no fast select facility is to be used for 
		 the call. Includes a value `not specified' which 
		 indicates that no preference is expressed."
	REFERENCE
		"ITU-T Rec. X.283 | ISO/IEC 10733 ASN.1 FastSelect."
	SYNTAX	INTEGER {
			notSpecified(0),
			fastSelect(1),
			fastSelectWithRestrictedResponse(2),
			noFastSelect(3)
		}

DatabaseState ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"The state of the Level 1 or 2 ISIS database."
	REFERENCE
		"ISO/IEC 10589 ASN.1 DatabaseState."
	SYNTAX	INTEGER {
			off(0),
			on(1),
			waiting(2)
		}

OriginatingLSPBufferSize ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		""
	REFERENCE
		"ISO/IEC 10589 ASN.1 OriginatingLSPBufferSize."
	SYNTAX	INTEGER(512..1492)

AreaAddress ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"An area address."
	REFERENCE
		"ISO/IEC 10589 ASN.1 AreaAddress."
	SYNTAX		OCTET STRING(SIZE(0..20))

AreaAddresses ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"A set of area addresses.  Each area address is 
		 formatted as a single octet length indicator, followed 
		 by 'length' octets SIZE(0..20) of AreaAddress (see the 
		 AreaAddress textual convention), followed by another 
		 length indicator and AreaAddress pair, etc."
	REFERENCE
		"ISO/IEC 10589 ASN.1 AreaAddresses."
	SYNTAX		OCTET STRING

--	SNPAAddress ::= SEQUENCE {
--		type [1] OBJECT IDENTIFIER,
--		address [2] OCTET STRING }

SNPAAddress ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"An Subnetwork Point of Attachment Address.  This is 
		 either a DTE address or a MAC address expressed as an 
		 octet string."
	REFERENCE
		"ISO/IEC 10589 ASN.1 SNPAAddress."
	SYNTAX		OCTET STRING(SIZE(0..20))

SNPAAddresses ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"A list of SNPA addresses.  Each address contained in 
		 the list consists of one length octet followed by 
		 'length' address bytes forming the SNPAAddress."
	REFERENCE
		"ISO/IEC 10589 ASN.1 SNPAAddresses."
	SYNTAX		OCTET STRING

PathMetric ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"Describes a path metric for ISO/IEC 10589 IS-IS 
		 operation."
	REFERENCE
		"ISO/IEC 10589 ASN.1 PathMetric."
	SYNTAX		INTEGER(0..1023)

HopMetric ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"Describes a hop metric for ISO/IEC 10589 IS-IS 
		 operation."
	REFERENCE
		"ISO/IEC 10589 ASN.1 HopMetric."
	SYNTAX		INTEGER(0..63)

MetricType ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"Describes a hop metric type for ISO/IEC 10589 IS-IS 
		 operation."
	REFERENCE
		"ISO/IEC 10589 ASN.1 MetricType."
	SYNTAX		INTEGER { internal(0), external(1) }


AddressPrefix ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"Describes a network address prefix (or a complete 
		 network address).  The prefix is formatted in 
		 semi-octets and contains a first octet which contains 
		 the length of the remainder of the address prefix in 
		 semi-octets (1..40), followed by an integral number of 
		 octets containing the indicated number of semi-octets 
		 in order.  When the number of semi-octets is odd, the 
		 last 4 bits in the last octet are not significant.  A 
		 null string represents an absent address prefix rather 
		 than a zero length prefix."
	REFERENCE
		"ISO/IEC 10589 ASN.1 AddressPrefix."
	SYNTAX		OCTET STRING(SIZE(0..21))

SNPAPrefix ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"Describes an SNPA prefix.  The prefix is formatted in 
		 octets, but valued in bits: that is, it is a bit 
		 string.  The first octet contains the number of 
		 significant bits that follow and the bits are coded in 
		 the following octets padded to the next integral 
		 octet.
		 
		 A null octet string represents an absent SNPA prefix 
		 rather than a zero length prefix."
	REFERENCE
		"ISO/IEC 10589 ASN.1 SNPAPrefix."
	SYNTAX		OCTET STRING(SIZE(0..21))

