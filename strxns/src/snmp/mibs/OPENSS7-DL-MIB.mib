-- lLCConnectionlessAck-P mandatory package
--
-- The LLC Connectionless Ack object is responsible for processing the 
-- attributes, actions and notifications which affect a specific data 
-- link connection (for LLC Type 3 connections). One data connection 
-- shall exist for each data link connection supported.

lLCConnectionlessAckName OBJECT-TYPE
	SYNTAX		SimpleNameType
	MAX-ACCESS	not-accessible -- index
	STATUS		current
	DESCRIPTION
		"The localLSAP and remoteLSAPId and the local and remote 
		 MAC Address."
	REFERENCE
		"IEEE 802.2 (ISO/IEC 8802-2), lLCConnectionlessAckName."
	::= { lLCConnectionlessAckEntry 1 }

lLCConnectionlessAckMaximumLLCInformationFieldSize OBJECT-TYPE
	SYNTAX		INTEGER
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION
		"The maximum length SDU that the LSAP will accomodate."
	REFERENCE
		"IEEE 802.2 (ISO/IEC 8802-2), 
		 maximumLLCInformationFieldSize."
	::= { lLCConnectionlessAckEntry 2 }


lLCConnectionlessAckMaximumRetransmissions OBJECT-TYPE
	SYNTAX		INTEGER
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION
		"The number of times that the LLC Type 2 should attempt 
		 to realize a successful PDU transfer."
	REFERENCE
		"IEEE 802.2 (ISO/IEC 8802-2), maximumRetransmissions."
	::= { lLCConnectionlessAckEntry 3 }


lLCConnectionlessAckTESTReceivedABBResponse OBJECT-TYPE
	SYNTAX		Counter32
	UNITS		"TEST ABB responses received"
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
		"Provides a count of TEST responses received by this 
		 LSAP without the LSDU due to limited resources at the 
		 remote station."
	REFERENCE
		"IEEE 802.2 (ISO/IEC 8802-2), tESTReceivedABBResponse."
	::= { lLCConnectionlessAckEntry 4 }


lLCConnectionlessAckTESTReceivedCommand OBJECT-TYPE
	SYNTAX		Counter32
	UNITS		"TEST commands received"
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
		"Provides a count of TEST commands received from the MAC 
		 sublayer by this LSAP.  This includes commands recieved 
		 with any group address recognized by this LSAP."
	REFERENCE
		"IEEE 802.2 (ISO/IEC 8802-2), tESTReceivedCommand."
	::= { lLCConnectionlessAckEntry 5 }


lLCConnectionlessAckTESTReceivedResponse OBJECT-TYPE
	SYNTAX		Counter32
	UNITS		"TEST responses received"
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
		"Provides a count of TEST responses received from the 
		 MAC sublayer by this LSAP."
	REFERENCE
		"IEEE 802.2 (ISO/IEC 8802-2), tESTReceivedResponse."
	::= { lLCConnectionlessAckEntry 6 }


lLCConnectionlessAckTESTSentABBResponse OBJECT-TYPE
	SYNTAX		Counter32
	UNITS		"TEST ABB responses sent"
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
		"Provides a count of TEST responses that were delivered 
		 to the MAC without the LSDU data due to limited 
		 resources."
	REFERENCE
		"IEEE 802.2 (ISO/IEC 8802-2), tESTSentABBResponse."
	::= { lLCConnectionlessAckEntry 7 }


lLCConnectionlessAckTESTSentCommand OBJECT-TYPE
	SYNTAX		Counter32
	UNITS		"TEST commands sent"
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
		"Provides a count of TEST command set to the MAC 
		 sublayer by this LSAP."
	REFERENCE
		"IEEE 802.2 (ISO/IEC 8802-2), tESTSentCommand."
	::= { lLCConnectionlessAckEntry 8 }


lLCConnectionlessAckTESTSentResponse OBJECT-TYPE
	SYNTAX		Counter32
	UNITS		"TEST responses sent"
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
		"Provides a count of TEST responses sent to the MAC 
		 sublayer by this LSAP."
	REFERENCE
		"IEEE 802.2 (ISO/IEC 8802-2), tESTSentResponse."
	::= { lLCConnectionlessAckEntry 9 }


lLCConnectionlessAckReceiveResources OBJECT-TYPE
	SYNTAX		TruthValue
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION
		"Causes the LSAP entity to either enable or disable the 
		 resources for reception of the data field of Type 3 
		 command PDUs sent to this LSAP.  If a Type 3 PDU 
		 containing data is received while disabled, the UN 
		 status code is returned in the CCCC field of the 
		 response PDU."
