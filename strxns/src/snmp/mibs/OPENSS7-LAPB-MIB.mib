lAPBDLETable OBJECT-TYPE
	SYNTAX		SEQUENCE OF LAPBDLEEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"The LAPB data link entity managed object.  There may be 
		 multiple instances of the lLAPBDLE managed object in a 
		 system.  Systems not supporting the LAPB data link 
		 procedures defined in ISO/IEC 7776 are not required to 
		 support the lAPBDLE and contained managed objects.
		 
		 The lAPBDLE managed object has a conditional mlp 
		 package.  There may be multiple instances of lAPBDLE MO 
		 in a system, both with and without the mlp package.  
		 For those instances in which the package is absent, the 
		 mlp procedures do not apply and the lAPBDLE MO may 
		 contain at most on sLPPM MO.  For those instances in 
		 which the package is present, the mlp proceudres do 
		 apply and the lAPBDLE MO may contain multiplex 
		 instances of sLPPM MO.
		 
		 A lAPBDLE MO may contain at most one dLSAP MO.  The 
		 value of the sap1Address atribute is of no 
		 significance.  The cardinality of the userEntityNames 
		 attribute is limited to one.

		 A lAPBDLE MO may be created and deleted explicitly by 
		 management operation or created and deleted 
		 automatically as a part of system operation.

		 When the lAPBDLE MO is operable, the operationalState 
		 shall have the value 'enabled'; otherwise, it shall 
		 have the value 'disabled'.  Transitions of 
		 operationalState shall be reported using the 
		 stateChange notification."
	REFERENCE
		"ITU-T Rec. X.282 | ISO/IEC 10742 lAPBDLE MOC."
	::= { lAPBDLE 1 }

lAPBDLEEntry OBJECT-TYPE
	SYNTAX		LAPBDLEEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"This entiry is derived from the datalinkEntity and uses 
		 the same primary index."
	REFERENCE
		"ITU-T Rec. X.282 | ISO/IEC 10742 lAPBDLE MOC."
	INDEX	{
			lAPBDLEcommunicationsEntityId
		}
	::= { lAPBDLETable 1 }

LAPBDLEEntry ::=
	SEQUENCE { 
		-- X.723 communicationsEntity inherited attributes:
		lAPBDLEcommunicationsEntityId
			SimpleNameType,
		lAPBDLElocalSapNames
			RowPointer,
		lAPBDLEoperationalState
			OperationalState,
		-- X.282 datalinkEntity inherited attributes:
		lAPBDLEproviderEntityNames
			RowPointer,
		-- X.282 lAPBDLE attributes:
		-- mLP-P package
		lAPBDLEmT1Timer
			TimeInterval,
		lAPBDLEmT3Timer
			TimeInterval,
		lAPBDLEmW
			INTEGER,
		lAPBDLEmXSend
			INTEGER,
		lAPBDLEmXReceive
			INTEGER,
		-- mT2-P package
		lAPBDLEmT2Timer
			TimeInterval,
		-- mLP-Counters-P package
		lAPBDLEreceivedMlpResets
			Counter32,
		lAPBDLEtimesMT1Expired
			Counter32,
		lAPBDLEIframesReassignments
			Counter32,
		lAPBDLEmlpFramesReceived
			Counter32,
		lAPBDLEmlpFramesSent
			Counter32,
		lAPBDLEmlpFramesOutsideWindowGuard
			Counter32,
		lAPBDLEreceivedMlpFramesInGuardRegion
			Counter32,
		--
		lAPBDLERowStatus
			RowStatus
	}

-- X.723 communicationsEntity inherited attributes:
lAPBDLEcommunicationsEntityId OBJECT-TYPE
	SYNTAX		SimpleNameType
	MAX-ACCESS	not-accessible -- index
	STATUS		current
	DESCRIPTION	""
