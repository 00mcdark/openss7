-- =====================================================================
--
-- Notification parameters.

fRMR OBJECT-TYPE
	SYNTAX		OCTET STRING
	MAX-ACCESS	accessible-for-notify
	STATUS		current
	DESCRIPTION
		"Provides a parameter containing the FRMR frame for use 
		 as additional information with the communicationsAlarm 
		 issued for an sLPConnection table entry when 
		 specificProblems has the value fRMRReceived."
	REFERENCE
		"ITU-T Rec. X.282 | ISO/IEC 10742 ASN.1 fRMR."
	::= { lapbNotifs 1 }

-- =====================================================================
--
-- Specific Problems for use with TMN alarms.

lapbSpecificProblems OBJECT IDENTIFIER ::= { lapbOids }
--	{ joint-iso-itu-t(3) datalink-layer(15) standardSpecificExtension(0) specificProblems(11) }

fRMRReceived OBJECT-IDENTITY
	STATUS		current
	DESCRIPTION
		"This object identity is used as the value of the 
		 specificProblems parameter in the communicationsAlarm 
		 notification issued for an sLPConnection or 
		 lLCConnection2 table entry.  It indicates that an FRMR 
		 was received.  The FRMR frame itself is include in the 
		 fRMR parameter provided as additional information for 
		 the communicationsAlarm notification.
		 
		 A FRMR frame was received.  The received FRMR frame is 
		 reported as additionalInformation in the notification, 
		 using the fRMR parameter.  THe significance 
		 sub-parameter shall be set as described above.
		 
		 The probableCause parameter is set to 
		 communicationsProtocolError.  This value, fRMRReceived, 
		 shall be reported in the specificProblems parameter.  
		 In addition, the reason why the frame was sent is also 
		 returned in specificProblems.
		 
		 The perceivedSeverity parameter is set to the value 
		 Minor.  A subsequent communicationsAlarm with a 
		 preceivedSeverity of Cleared is not generated.  No 
		 other parameters are used."
	REFERENCE
		"ITU-T Rec. X.282 | ISO/IEC 10742 ASN.1 fRMRReceived."
	::= { lapbSpecificProblems fRMRReceived(5) }

fRMRReasons OBJECT IDENTIFIER ::= { lapbOids fRMRReasons(6) }

fRMRReasonsControlFieldUndefinedOrUnimplemented OBJECT-IDENTITY
	STATUS		current
	DESCRIPTION
		"This object identity is used as an additional value of 
		 the specificProblems parameter in the 
		 communicationsAlarm notification issued for an 
		 sLPConnection or lLCConnection2 table entry.  It 
		 indicates that the reason for receiving the FRMR was an 
		 undefined or unimplemented control field."
	REFERENCE
		"ITU-T Rec. X.282 | ISO/IEC 10742 ASN.1 
		 fRMRReasonsControlFieldUndefinedOrUnimplemented."
	::= { fRMRReasons controlFieldUndefinedOrUnimplemented(1) }

fRMRReasonsFormatError OBJECT-IDENTITY
	STATUS		current
	DESCRIPTION
		"This object identity is used as an additional value of 
		 the specificProblems parameter in the 
		 communicationsAlarm notification issued for an 
		 sLPConnection or lLCConnection2 table entry.  It 
		 indicates that the reason for receiving the FRMR was a 
		 format error."
	REFERENCE
		"ITU-T Rec. X.282 | ISO/IEC 10742 ASN.1 
		 fRMRReasonsFormatError."
	::= { fRMRReasons formatError(2) }

fRMRReasonsInfoFieldLengthGreaterThanMaximum OBJECT-IDENTITY
	STATUS		current
	DESCRIPTION
		"This object identity is used as an additional value of 
		 the specificProblems parameter in the 
		 communicationsAlarm notification issued for an 
		 sLPConnection or lLCConnection2 table entry.  It 
		 indicates that the reason for receiving the FRMR was 
		 that the information field length was greater than the 
		 maximum."
	REFERENCE
		"ITU-T Rec. X.282 | ISO/IEC 10742 ASN.1 
		 fRMRReasonsInfoFieldLengthGreaterThanMaximum."
	::= { fRMRReasons infoFieldLengthGreaterThanMaximum(3) }

fRMRReasonsInvalidNR OBJECT-IDENTITY
	STATUS		current
	DESCRIPTION
		"This object identity is used as an additional value of 
		 the specificProblems parameter in the 
		 communicationsAlarm notification issued for an 
		 sLPConnection or lLCConnection2 table entry.  It 
		 indicates that the reason for receiving the FRMR was 
		 that the NR included in the command was invalid."
	REFERENCE
		"ITU-T Rec. X.282 | ISO/IEC 10742 ASN.1 fRMRReasonsInvalidNR."
	::= { fRMRReasons invalidNR(4) }

fRMRReasonsNonSpecific OBJECT-IDENTITY
	STATUS		current
	DESCRIPTION
		"This object identity is used as an additional value of 
		 the specificProblems parameter in the 
		 communicationsAlarm notification issued for an 
		 sLPConnection or lLCConnection2 table entry.  It 
		 indicates that the reason for receiving the FRMR was 
		 unknown or unspecified."
	REFERENCE
		"ITU-T Rec. X.282 | ISO/IEC 10742 ASN.1 fRMRReasonsNonSpecific."
	::= { fRMRReasons nonSpecific(5) }


-- Oids for use with specific errorIds for use with processingFailure 
-- for error on activate, but will be used with processingErrorAlarm 
-- instead.

lapbErrors OBJECT IDENTIFIER ::= { lapbOids activate(1) errors(1) }
--	{ joint-iso-itu-t(3) datalink-layer(15) standardSpecificExtensions(0) action(9) activate(1) errors(1) }

activateFailure OBJECT-IDENTITY
	STATUS		current
	DESCRIPTION
		"This object identity is used as the value of the 
		 probableCause parameter in the processingErrorAlarm 
		 notification issued for an sLPConnection table entry.  
		 It indicates that the processing error was a failure to 
		 activate the sLPConnection.  The perceivedSeverity for 
		 the processingErrorAlarm is Minor."
	::= { lapbErrors processingFailure(1) }

activateFailureInsufficientResources OBJECT-IDENTITY
	STATUS		current
	DESCRIPTION
		"This object identity is used as the value of the 
		 specificProblems parameter in the processingErrorAlarm 
		 notification issued for an sLPConnection table entry.  
		 It indicates that the reason for the activation failure 
		 was insufficient resources."
	::= { activateFailure insufficientResources(1) }

activateFailureProviderDoesNotExist OBJECT-IDENTITY
	STATUS		current
	DESCRIPTION
		"This object identity is used as the value of the 
		 specificProblems parameter in the processingErrorAlarm 
		 notification issued for an sLPConnection table entry.  
		 It indicates that the reason for the activation failure 
		 was that the specified provider does not exist."
	::= { activateFailure providerDoesNotExist(2) }

activateFailureProviderNotAvailable OBJECT-IDENTITY
	STATUS		current
	DESCRIPTION
		"This object identity is used as the value of the 
		 specificProblems parameter in the processingErrorAlarm 
		 notification issued for an sLPConnection table entry.  
		 It indicates that the reason for the activation failure 
		 was that the specified provider is not available for 
		 activation."
	::= { activateFailure providerNotAvailable(3) }

activateFailureRequiredServiceNotAvailable OBJECT-IDENTITY
	STATUS		current
	DESCRIPTION
		"This object identity is used as the value of the 
		 specificProblems parameter in the processingErrorAlarm 
		 notification issued for an sLPConnection table entry.  
		 It indicates that the reason for the activation failure 
		 was that an underlying service required for activation 
		 is not available."
	::= { activateFailure requiredServiceNotAvailable(4) }

activateFailureSystemSpecific OBJECT-IDENTITY
	STATUS		current
	DESCRIPTION
		"This object identity is used as the value of the 
		 specificProblems parameter in the processingErrorAlarm 
		 notification issued for an sLPConnection table entry.  
		 It indicates that the reason for the activation is 
		 system specific."
	::= { activateFailure systemSpecific(5) }

-- =====================================================================
--
-- Scalars

lapbDiscontinuityTime OBJECT-TYPE
	SYNTAX		TimeStamp
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
		"Provides the time since the last reboot at which a 
		 discontinuity in counters and gauges occurred possibly 
		 resulting in the reset of counters and gauges to zero."
	REFERENCE
		"SMIv2"
	::= { lapbGlobals 1 }

--
-- Scalars
--
-- The following scalars define the default values to be used for 
-- the initial value of entries in the sLPConnectionTable.  These 
-- defaults satisfy two purposes: to provide default values for the 
-- new entries in the sLPConnectionIVMOTable and to provide initial 
-- values for entries in the sLPConnectionTable if there is no 
-- corresponding subordinate sLPConnectionIVMO entry.

sLPConnectionDefaultInterfaceType OBJECT-TYPE
	SYNTAX		INTEGER { dTE(0), dCE(1) }
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION
		"Sets the default initial value for the 
		 sLPConnectionInterfaceType element of the 
		 sLPConnectionTable and the default value for the 
		 sLPConnectionIVMOinterfaceType element of the 
		 sLPConnectionIVMOTable.  When set by a management 
		 station, the value set should persist across agent and 
		 system reboots.  The DEFVAL, therefore, is only 
		 applicable when this default has never been set by a 
		 management station."
	REFERENCE
		"ITU-T Rec. X.282 | ISO/IEC 10742."
	DEFVAL		{ dTE(0) }
	::= { sLPConnectionDefaults 1 }

