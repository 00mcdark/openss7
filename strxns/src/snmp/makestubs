MIBS=+OPENSS7-LLC2-MIB:OPENSS7-TMN-TC:OPENSS7-TMN-SMI
MIBDIRS=./mibs:/usr/share/snmp/mibs:/home/brian/.snmp/mibs
MIBFILES=./mibs/OPENSS7-LLC2-MIB.mib

export MIBS
export MIBDIRS
export MIBFILES

all: basic storage tables

basic:
	./mib2c -d -c ./mib2c.conf -f llc2_simple llcMIB
	indent -nhnl llc2_simple.[ch]

storage:
	./mib2c -d -c ./mib2c.storage.conf -f llc2_storage llcMIB
	indent -nhnl llc2_storage.[ch]

tables:
	./mib2c -d -c ./mib2c.storage.conf -f llc2_mACDLETable mACDLETable
	./mib2c -d -c ./mib2c.storage.conf -f llc2_mACTable mACTable
	./mib2c -d -c ./mib2c.storage.conf -f llc2_resourceTypeIdTable resourceTypeIdTable
	./mib2c -d -c ./mib2c.storage.conf -f llc2_dLSAPTable dLSAPTable
	./mib2c -d -c ./mib2c.storage.conf -f llc2_lLCDLETable lLCDLETable
	./mib2c -d -c ./mib2c.storage.conf -f llc2_lLCStationTable lLCStationTable
	./mib2c -d -c ./mib2c.storage.conf -f llc2_lLCSAPTable lLCSAPTable
	./mib2c -d -c ./mib2c.storage.conf -f llc2_rDESetupTable rDESetupTable
	./mib2c -d -c ./mib2c.storage.conf -f llc2_rDEPairTable rDEPairTable
	./mib2c -d -c ./mib2c.storage.conf -f llc2_lLCCLPMTable lLCCLPMTable
	./mib2c -d -c ./mib2c.storage.conf -f llc2_lLCConnectionLessTable lLCConnectionLessTable
	./mib2c -d -c ./mib2c.storage.conf -f llc2_lLCCOPMTable lLCCOPMTable
	./mib2c -d -c ./mib2c.storage.conf -f llc2_lLCConnection2Table lLCConnection2Table
	./mib2c -d -c ./mib2c.storage.conf -f llc2_lLCConnection2IVMOTable lLCConnection2IVMOTable
	./mib2c -d -c ./mib2c.storage.conf -f llc2_lLCConnectionlessAckTable lLCConnectionlessAckTable
	./mib2c -d -c ./mib2c.storage.conf -f llc2_lLCConnectionlessAckIVMOTable lLCConnectionlessAckIVMOTable
	indent -nhnl llc2_*Table.[ch]

clean:
	rm -f llc2_simple.[ch]
	rm -f llc2_storage.[ch]
	rm -f llc2_*Table.[ch]
	rm -f llc2_simple.[ch]~
	rm -f llc2_storage.[ch]~
	rm -f llc2_*Table.[ch]~


