MIBS=+OPENSS7-LLC2-MIB:OPENSS7-LAPB-MIB:OPENSS7-DL-MIB:OPENSS7-TMN-TC:OPENSS7-SMI-MIB
MIBDIRS=./mibs:/usr/share/snmp/mibs:/home/brian/.snmp/mibs
MIBFILES=./mibs/OPENSS7-LLC2-MIB.mib:./mibs/OPENSS7-LAPB-MIB.mib:./mibs/OPENSS7-DL-MIB.mib:./mibs/OPENSS7-TMN-TC.mib:./mibs/OPENSS7-SMI-MIB.mib

export MIBS
export MIBDIRS
export MIBFILES

all: openss7

check: llcMIB.tree lapbMIB.tree osiMIB.tree

llcMIB.tree: mibs/OPENSS7-LLC2-MIB.mib mibs/OPENSS7-SMI-MIB.mib mibs/OPENSS7-TMN-TC.mib
	snmptranslate -Tp OPENSS7-LLC2-MIB::llcMIB 2>&1 | tee $@

lapbMIB.tree: mibs/OPENSS7-LAPB-MIB.mib mibs/OPENSS7-SMI-MIB.mib mibs/OPENSS7-TMN-TC.mib
	snmptranslate -Tp OPENSS7-LAPB-MIB::lapbMIB 2>&1 | tee $@

osiMIB.tree: mibs/OPENSS7-DL-MIB.mib mibs/OPENSS7-SMI-MIB.mib mibs/OPENSS7-TMN-TC.mib
	snmptranslate -Tp OPENSS7-DL-MIB::osiMIB 2>&1 | tee $@

basic: llcMIB_simple.c llcMIB_simple.h lapbMIB_simple.c lapbMIB_simple.h osiMIB_simple.c osiMIB_simple.h

llcMIB_simple.c llcMIB_simple.h: mib2c mib2c.conf mibs/OPENSS7-LLC2-MIB.mib
	./mib2c -c ./mib2c.conf -f llcMIB_simple llcMIB
	indent -nhnl llcMIB_simple.[ch]

lapbMIB_simple.c lapbMIB_simple.h: mib2c mib2c.conf mibs/OPENSS7-LAPB-MIB.mib
	./mib2c -c ./mib2c.conf -f lapbMIB_simple lapbMIB
	indent -nhnl lapbMIB_simple.[ch]

osiMIB_simple.c osiMIB_simple.h: mib2c mib2c.conf mibs/OPENSS7-DL-MIB.mib
	./mib2c -c ./mib2c.conf -f osiMIB_simple osiMIB
	indent -nhnl osiMIB_simple.[ch]

storage: llcMIB_storage.c llcMIB_storage.h lapbMIB_storage.c lapbMIB_storage.h osiMIB_storage.c osiMIB_storage.h

llcMIB_storage.c llcMIB_storage.h: mib2c mib2c.storage.conf mibs/OPENSS7-LLC2-MIB.mib
	./mib2c -c ./mib2c.storage.conf -f llcMIB_storage llcMIB
	indent -nhnl llcMIB_storage.[ch]

lapbMIB_storage.c lapbMIB_storage.h: mib2c mib2c.storage.conf mibs/OPENSS7-LAPB-MIB.mib
	./mib2c -c ./mib2c.storage.conf -f lapbMIB_storage lapbMIB
	indent -nhnl lapbMIB_storage.[ch]

osiMIB_storage.c osiMIB_storage.h: mib2c mib2c.storage.conf mibs/OPENSS7-DL-MIB.mib
	./mib2c -c ./mib2c.storage.conf -f osiMIB_storage osiMIB
	indent -nhnl osiMIB_storage.[ch]

openss7: llcMIB_openss7.c llcMIB_openss7.h lapbMIB_openss7.c lapbMIB_openss7.h osiMIB_openss7.c osiMIB_openss7.h

llcMIB_openss7.c llcMIB_openss7.h: mib2c mib2c.openss7.conf mibs/OPENSS7-LLC2-MIB.mib
	./mib2c -c ./mib2c.openss7.conf -f llcMIB_openss7 llcMIB
	indent -nhnl llcMIB_openss7.[ch]

lapbMIB_openss7.c lapbMIB_openss7.h: mib2c mib2c.openss7.conf mibs/OPENSS7-LAPB-MIB.mib
	./mib2c -c ./mib2c.openss7.conf -f lapbMIB_openss7 lapbMIB
	indent -nhnl lapbMIB_openss7.[ch]

osiMIB_openss7.c osiMIB_openss7.h: mib2c mib2c.openss7.conf mibs/OPENSS7-DL-MIB.mib
	./mib2c -c ./mib2c.openss7.conf -f osiMIB_openss7 osiMIB
	indent -nhnl osiMIB_openss7.[ch]

tables:
	./mib2c -c ./mib2c.storage.conf mACDLETable
	./mib2c -c ./mib2c.storage.conf mACTable
	./mib2c -c ./mib2c.storage.conf resourceTypeIdTable
	./mib2c -c ./mib2c.storage.conf dLSAPTable
	./mib2c -c ./mib2c.storage.conf lLCDLETable
	./mib2c -c ./mib2c.storage.conf lLCStationTable
	./mib2c -c ./mib2c.storage.conf lLCSAPTable
	./mib2c -c ./mib2c.storage.conf rDESetupTable
	./mib2c -c ./mib2c.storage.conf rDEPairTable
	./mib2c -c ./mib2c.storage.conf lLCCLPMTable
	./mib2c -c ./mib2c.storage.conf lLCConnectionLessTable
	./mib2c -c ./mib2c.storage.conf lLCCOPMTable
	./mib2c -c ./mib2c.storage.conf lLCConnection2Table
	./mib2c -c ./mib2c.storage.conf lLCConnection2IVMOTable
	./mib2c -c ./mib2c.storage.conf lLCConnectionlessAckTable
	./mib2c -c ./mib2c.storage.conf lLCConnectionlessAckIVMOTable
	indent -nhnl *Table.[ch]

clean:
	rm -f llc2_simple.[ch]
	rm -f llc2_storage.[ch]
	rm -f llc2_openss7.[ch]
	rm -f *Table.[ch]
	rm -f llc2_simple.[ch]~
	rm -f llc2_storage.[ch]~
	rm -f llc2_openss7.[ch]~
	rm -f *Table.[ch]~
	rm -f llcMIB_simple.[ch]
	rm -f llcMIB_storage.[ch]
	rm -f llcMIB_openss7.[ch]
	rm -f *Table.[ch]
	rm -f llcMIB_simple.[ch]~
	rm -f llcMIB_storage.[ch]~
	rm -f llcMIB_openss7.[ch]~
	rm -f *Table.[ch]~
	rm -f lapbMIB_simple.[ch]
	rm -f lapbMIB_storage.[ch]
	rm -f lapbMIB_openss7.[ch]
	rm -f *Table.[ch]
	rm -f lapbMIB_simple.[ch]~
	rm -f lapbMIB_storage.[ch]~
	rm -f lapbMIB_openss7.[ch]~
	rm -f *Table.[ch]~
	rm -f osiMIB_simple.[ch]
	rm -f osiMIB_storage.[ch]
	rm -f osiMIB_openss7.[ch]
	rm -f *Table.[ch]
	rm -f osiMIB_simple.[ch]~
	rm -f osiMIB_storage.[ch]~
	rm -f osiMIB_openss7.[ch]~
	rm -f *Table.[ch]~
	rm -f llcMIB.tree
	rm -f lapbMIB.tree
	rm -f osiMIB.tree


