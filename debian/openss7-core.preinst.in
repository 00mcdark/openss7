@openss7_preinst@

preinst_core_install () { :
    preinst_rmmod
}

preinst_core_upgrade () { :
    preinst_shutdown
}

# -----------------------------------------------------------------------------
# Primary functions:
# -----------------------------------------------------------------------------

# new-preinst install
install_core_preinst () { :
    preinst_core_install
}

# new-preinst install old-version
reinstall_core_preinst () { :
}

# new-preinst upgrade old-version
upgrade_core_preinst () { :
    preinst_core_upgrade
}

# old-postrm upgrade new-version ...FAILED
# new-postrm failed-upgrade old-version ...FAILED
# old-preinst abort-upgrade new-version
abort_upgrade_core_preinst () { :
}

subpkg=core

@mscript_preinst@

