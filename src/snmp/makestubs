MIBDIRS=../mibs:/usr/share/snmp/mibs
MIBFILES=../mibs/OPENSS7-DL-MIB.mib:../mibs/OPENSS7-LAPB-MIB.mib:../mibs/OPENSS7-LLC2-MIB.mib:../mibs/OPENSS7-M2PA-MIB.mib:../mibs/OPENSS7-M2UA-ASP-MIB.mib:../mibs/OPENSS7-M2UA-MIB.mib:../mibs/OPENSS7-M3UA-ASP-MIB.mib:../mibs/OPENSS7-M3UA-SGP-MIB.mib:../mibs/OPENSS7-METRIC-MIB.mib:../mibs/OPENSS7-SIGTRAN-MIB.mib:../mibs/OPENSS7-SIGTRAN-TC.mib:../mibs/OPENSS7-SMI-MIB.mib:../mibs/OPENSS7-STREAMS-EXT-MIB.mib:../mibs/OPENSS7-STREAMS-MIB.mib:../mibs/OPENSS7-TMN-TC.mib:../mibs/x25-1.mib
MIBS=+OPENSS7-DL-MIB:OPENSS7-LAPB-MIB:OPENSS7-LLC2-MIB:OPENSS7-M2PA-MIB:OPENSS7-M2UA-ASP-MIB:OPENSS7-M2UA-MIB:OPENSS7-M3UA-ASP-MIB:OPENSS7-M3UA-SGP-MIB:OPENSS7-METRIC-MIB:OPENSS7-SIGTRAN-MIB:OPENSS7-SIGTRAN-TC:OPENSS7-SMI-MIB:OPENSS7-SMI-MIB:OPENSS7-SMI-MIB:OPENSS7-STREAMS-EXT-MIB:OPENSS7-STREAMS-MIB:OPENSS7-TMN-TC:OPENSS7-X25-PLP-MIB:OPENSS7-X25-PLP-MIB

export MIBS
export MIBDIRS
export MIBFILES

all: simple storage openss7 master

check: \
	cdr2MIB.tree \
	dlMIB.tree \
	ds0Bundle.tree \
	ds0.tree \
	ds1EXT.tree \
	ds1EXT.tree \
	ds1.tree \
	gmiMIB.tree \
	lapbMIB.tree \
	llcMIB.tree \
	m2paMIB.tree \
	m2uaAspMIB.tree \
	m2uaMIB.tree \
	m3uaAspMIB.tree \
	m3uaSgpMIB.tree \
	metricMIB.tree \
	mtpMIB.tree \
	mtpOmMIB.tree \
	mxMIB.tree \
	mxMIB.tree \
	phyMIB.tree \
	sccpMIB.tree \
	sccpOmMIB.tree \
	sigtranMIB.tree \
	slMIB.tree \
	smiMIB.tree \
	ss7MIB.tree \
	ss7OmMIB.tree \
	strExtMIB.tree \
	strMIB.tree \
	tcapMIB.tree \
	tcapOmMIB.tree \
	x25MIB.tree \
	x400pMIB.tree \
	x400pMIB.tree

cdr2MIB.tree: MIBS+=OPENSS7-CDR2-MIB:OPENSS7-TC
cdr2MIB.tree: mibs/OPENSS7-CDR2-MIB.mib
	snmptranslate -Tp OPENSS7-CDR2-MIB::cdr2MIB >cdr2MIB.tree

dlMIB.tree: mibs/OPENSS7-DL-MIB.mib mibs/OPENSS7-SMI-MIB.mib mibs/OPENSS7-TMN-TC.mib
	snmptranslate -Tp OPENSS7-DL-MIB::dlMIB 2>&1 | tee $@

ds0Bundle.tree: MIBS+=DS0BUNDLE-MIB:PerfHist-TC-MIB
ds0Bundle.tree: mibs/RFC2494-EXT-MIB.mib
	snmptranslate -Tp DS0BUNDLE-MIB::ds0Bundle >ds0Bundle.tree

ds0.tree: MIBS+=DS0-MIB:PerfHist-TC-MIB
ds0.tree: mibs/RFC2494-MIB.txt
	snmptranslate -Tp DS0-MIB::ds0>ds0.tree

ds1EXT.tree: MIBS+=DS1-EXT-MIB:PerfHist-TC-MIB
ds1EXT.tree: mibs/RFC4805-EXT-MIB.mib
	snmptranslate -Tp DS1-EXT-MIB::ds1EXT >ds1EXT.tree

ds1EXT.tree: MIBS+=DS1-EXT-MIB:PerfHist-TC-MIB
ds1EXT.tree: mibs/RFC4805-MIB.txt
	snmptranslate -Tp DS1-EXT-MIB::ds1EXT >ds1EXT.tree

ds1.tree: MIBS+=DS1-MIB:PerfHist-TC-MIB
ds1.tree: mibs/RFC4805-MIB.txt
	snmptranslate -Tp DS1-MIB::ds1>ds1.tree

gmiMIB.tree: MIBS+=OPENSS7-GMI-MIB:OPENSS7-TC
gmiMIB.tree: mibs/OPENSS7-GMI-MIB.mib
	snmptranslate -Tp OPENSS7-GMI-MIB::gmiMIB >gmiMIB.tree

lapbMIB.tree: mibs/OPENSS7-LAPB-MIB.mib mibs/OPENSS7-SMI-MIB.mib mibs/OPENSS7-TMN-TC.mib
	snmptranslate -Tp OPENSS7-LAPB-MIB::lapbMIB 2>&1 | tee $@

llcMIB.tree: mibs/OPENSS7-LLC2-MIB.mib mibs/OPENSS7-SMI-MIB.mib mibs/OPENSS7-TMN-TC.mib
	snmptranslate -Tp OPENSS7-LLC2-MIB::llcMIB 2>&1 | tee $@

m2paMIB.tree: mibs/OPENSS7-M2PA-MIB.mib
	snmptranslate -Tp OPENSS7-M2PA-MIB::m2paMIB 2>&1 | tee $@

m2uaAspMIB.tree: mibs/OPENSS7-M2UA-ASP-MIB.mib
	snmptranslate -Tp OPENSS7-M2UA-ASP-MIB::m2uaAspMIB 2>&1 | tee $@

m2uaMIB.tree: mibs/OPENSS7-M2UA-MIB.mib
	snmptranslate -Tp OPENSS7-M2UA-MIB::m2uaMIB 2>&1 | tee $@

m3uaAspMIB.tree: mibs/OPENSS7-M3UA-ASP-MIB.mib
	snmptranslate -Tp OPENSS7-M3UA-ASP-MIB::m3uaAspMIB 2>&1 | tee $@

m3uaSgpMIB.tree: mibs/OPENSS7-M3UA-SGP-MIB.mib
	snmptranslate -Tp OPENSS7-M3UA-SGP-MIB::m3uaSgpMIB 2>&1 | tee $@

metricMIB.tree: mibs/OPENSS7-METRIC-MIB.mib
	snmptranslate -Tp OPENSS7-METRIC-MIB::metricMIB 2>&1 | tee $@

mtpMIB.tree: MIBS+=OPENSS7-MTP-MIB:OPENSS7-SIGTRAN-TC:OPENSS7-TC:PerfHist-TC-MIB:HC-PerfHist-TC-MIB
mtpMIB.tree: mibs/OPENSS7-MTP-MIB.mib
	snmptranslate -Tp OPENSS7-MTP-MIB::mtpMIB >mtpMIB.tree

mtpOmMIB.tree: MIBS+=OPENSS7-MTP-OM-MIB2:OPENSS7-TC:PerfHist-TC-MIB:HC-PerfHist-TC-MIB
mtpOmMIB.tree: mibs/OPENSS7-MTP-OM-MIB2.mib
	snmptranslate -Tp OPENSS7-MTP-OM-MIB2::mtpOmMIB >mtpOmMIB.tree

mxMIB.tree: MIBS+=OPENSS7-MX-MIB:OPENSS7-SMI-MIB:OPENSS7-TC
mxMIB.tree: mibs/OPENSS7-MX-MIB.mib
	snmptranslate -Tp OPENSS7-MX-MIB::mxMIB >mxMIB.tree

mxMIB.tree: MIBS+=OPENSS7-MX-MIB:PerfHist-TC-MIB
mxMIB.tree: mibs/OPENSS7-MX-MIB.mib
	snmptranslate -Tp OPENSS7-MX-MIB::mxMIB >mxMIB.tree

phyMIB.tree: MIBS+=OPENSS7-PHY-MIB
phyMIB.tree: mibs/OPENSS7-PHY-MIB.mib
	snmptranslate -Tp OPENSS7-PHY-MIB::phyMIB >phyMIB.tree

sccpMIB.tree: MIBS+=OPENSS7-SCCP-MIB:OPENSS7-TC
sccpMIB.tree: mibs/OPENSS7-SCCP-MIB.mib
	snmptranslate -Tp OPENSS7-SCCP-MIB::sccpMIB >sccpMIB.tree

sccpOmMIB.tree: MIBS+=OPENSS7-SCCP-OM-MIB:OPENSS7-SCCP-MIB:OPENSS7-TC:PerfHist-TC-MIB:HC-PerfHist-TC-MIB
sccpOmMIB.tree: mibs/OPENSS7-SCCP-OM-MIB.mib
	snmptranslate -Tp OPENSS7-SCCP-OM-MIB::sccpOmMIB >sccpOmMIB.tree

sigtranMIB.tree: mibs/OPENSS7-SIGTRAN-MIB.mib
	snmptranslate -Tp OPENSS7-SIGTRAN-MIB::sigtranMIB 2>&1 | tee $@

slMIB.tree: MIBS+=OPENSS7-SL-MIB:OPENSS7-TC:PerfHist-TC-MIB:HC-PerfHist-TC-MIB
slMIB.tree: mibs/OPENSS7-SL-MIB.mib
	snmptranslate -Tp OPENSS7-SL-MIB::slMIB >slMIB.tree

smiMIB.tree: MIBS+=OPENSS7-SMI-MIB:OPENSS7-TC
smiMIB.tree: mibs/OPENSS7-SMI-MIB.mib
	snmptranslate -Tp OPENSS7-SMI-MIB::smiMIB >smiMIB.tree

ss7MIB.tree: MIBS+=OPENSS7-SS7-MIB:OPENSS7-TC
ss7MIB.tree: mibs/OPENSS7-SS7-MIB.mib
	snmptranslate -Tp OPENSS7-SS7-MIB::ss7MIB >ss7MIB.tree

ss7OmMIB.tree: MIBS+=OPENSS7-SS7-OM-MIB:OPENSS7-TC:PerfHist-TC-MIB:HC-PerfHist-TC-MIB
ss7OmMIB.tree: mibs/OPENSS7-SS7-OM-MIB.mib
	snmptranslate -Tp OPENSS7-SS7-OM-MIB::ss7OmMIB >ss7OmMIB.tree

strExtMIB.tree: mibs/OPENSS7-STREAMS-EXT-MIB.mib
	snmptranslate -Tp OPENSS7-STREAMS-EXT-MIB::strExtMIB 2>&1 | tee $@

strMIB.tree: mibs/OPENSS7-STREAMS-MIB.mib
	snmptranslate -Tp OPENSS7-STREAMS-MIB::strMIB 2>&1 | tee $@

tcapMIB.tree: MIBS+=OPENSS7-TCAP-MIB:OPENSS7-TC
tcapMIB.tree: mibs/OPENSS7-TCAP-MIB.mib
	snmptranslate -Tp OPENSS7-TCAP-MIB::tcapMIB >tcapMIB.tree

tcapOmMIB.tree: MIBS+=OPENSS7-TCAP-OM-MIB:OPENSS7-TC
tcapOmMIB.tree: mibs/OPENSS7-TCAP-OM-MIB.mib
	snmptranslate -Tp OPENSS7-TCAP-OM-MIB::tcapOmMIB >tcapOmMIB.tree

x25MIB.tree: ../mibs/x25-1.mib
	snmptranslate -Tp OPENSS7-X25-PLP-MIB::x25PLPMIB 2>&1 | tee $@

x400pMIB.tree: MIBS+=OPENSS7-X400P-MIB:OPENSS7-SMI-MIB:OPENSS7-TC
x400pMIB.tree: mibs/OPENSS7-X400P-MIB.mib
	snmptranslate -Tp OPENSS7-X400P-MIB::x400pMIB >x400pMIB.tree

x400pMIB.tree: MIBS+=OPENSS7-X400P-MX-MIB
x400pMIB.tree: mibs/OPENSS7-X400P-MX-MIB.mib
	snmptranslate -Tp OPENSS7-X400P-MX-MIB::x400pMIB >x400pMIB.tree

simple: \
	cdr2MIB_simple.c cdr2MIB_simple.h \
	dlMIB_simple.c dlMIB_simple.h \
	ds0Bundle_simple.c ds0Bundle_simple.h \
	ds0_simple.c ds0_simple.h \
	ds1EXT_simple.c ds1EXT_simple.h \
	ds1_simple.c ds1_simple.h \
	lapbMIB_simple.c lapbMIB_simple.h \
	llcMIB_simple.c llcMIB_simple.h \
	m2paMIB_simple.c m2paMIB_simple.h \
	m2uaAspMIB_simple.c m2uaAspMIB_simple.h \
	m2uaMIB_simple.c m2uaMIB_simple.h \
	m3uaAspMIB_simple.c m3uaAspMIB_simple.h \
	m3uaSgpMIB_simple.c m3uaSgpMIB_simple.h \
	metricMIB_simple.c metricMIB_simple.h \
	mtpMIB_simple.c mtpMIB_simple.h \
	mtpOmMIB_simple.c mtpOmMIB_simple.h \
	mxMIB_simple.c mxMIB_simple.h \
	phyMIB_simple.c phyMIB_simple.h \
	sccpMIB_simple.c sccpMIB_simple.h \
	sccpOmMIB_simple.c sccpOmMIB_simple.h \
	sigtranMIB_simple.c sigtranMIB_simple.h \
	slMIB_simple.c slMIB_simple.h \
	ss7MIB_simple.c ss7MIB_simple.h \
	ss7OmMIB_simple.c ss7OmMIB_simple.h \
	strExtMIB_simple.c strExtMIB_simple.h \
	strMIB_simple.c strMIB_simple.h \
	tcapMIB_simple.c tcapMIB_simple.h \
	tcapOmMIB_simple.c tcapOmMIB_simple.h \
	x25MIB_simple.c x25MIB_simple.h \
	x400pMIB_simple.c x400pMIB_simple.h

cdr2MIB_simple.c cdr2MIB_simple.h: MIBS+=OPENSS7-CDR2-MIB:OPENSS7-TC
cdr2MIB_simple.c cdr2MIB_simple.h: mib2c mib2c.conf mibs/OPENSS7-CDR2-MIB.mib
	./mib2c -c ./mib2c.conf -f cdr2MIB_simple cdr2MIB
	indent -nhnl cdr2MIB_simple.[ch]

dlMIB_simple.c dlMIB_simple.h: mib2c mib2c.conf mibs/OPENSS7-DL-MIB.mib
	./mib2c -c ./mib2c.conf -f dlMIB_simple dlMIB
	indent -nhnl dlMIB_simple.[ch]

ds0Bundle_simple.c ds0Bundle_simple.h: MIBS+=DS0BUNDLE-MIB:PerfHist-TC-MIB
ds0Bundle_simple.c ds0Bundle_simple.h: mib2c mib2c.conf mibs/RFC4805-EXT-MIB.mib
	./mib2c -c ./mib2c.conf -f ds0Bundle_simple ds0Bundle
	indent -nhnl ds0Bundle_simple.[ch]

ds0_simple.c ds0_simple.h: MIBS+=DS0-MIB:PerfHist-TC-MIB
ds0_simple.c ds0_simple.h: mib2c mib2c.conf mibs/RFC4805-MIB.txt
	./mib2c -c ./mib2c.conf -f ds0_simple ds0
	indent -nhnl ds0_simple.[ch]

