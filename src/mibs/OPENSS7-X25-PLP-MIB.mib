    ::= { openss7Modules 3 }


DTEAddress ::= TEXTUAL-CONVENTION
    DISPLAY-HINT "1x"
    STATUS      current
    DESCRIPTION 
        "Provides the DTE address."
    SYNTAX      OCTET STRING (SIZE(0..15))

LogicalChannelNumber ::= TEXTUAL-CONVENTION
    DISPLAY-HINT "d-0"
    STATUS      current
    DESCRIPTION 
        "Provides the logical channel number.  Logical channel numbers 
         in X.25 can be between zero (0) and 4095, inclusive."
    SYNTAX      Integer32 (0..4095)

Throughput ::= TEXTUAL-CONVENTION
    DISPLAY-HINT "d-0"
    STATUS      current
    DESCRIPTION 
        "Provides the throughput associated with a virtual call as a 
         ordinal value between zero (0) and 15, inclusive."
    SYNTAX      Integer32 (0..15)

RedirectReason ::= TEXTUAL-CONVENTION
    DISPLAY-HINT "d-0"
    STATUS      current
    DESCRIPTION 
        "Provides the redirect reason for redirection of a virtual call.  
         The redirect reason is coded as an integer between zero (0) and 
         127, inclusive."
    SYNTAX      Integer32 (0..127)

NetworkAddress ::= TEXTUAL-CONVENTION
    DISPLAY-HINT "1x"
    STATUS      current
    DESCRIPTION 
        "Provides an CONS OSI network address for use the the X.25 
         address extension."
    SYNTAX      OCTET STRING (SIZE(0..20))

x25PLPNotifications OBJECT IDENTIFIER
    ::= { x25-PLP-MIB 0 }

x25PLPObjects OBJECT IDENTIFIER
    ::= { x25-PLP-MIB 1 }

x25PLPTables OBJECT IDENTIFIER
    ::= { x25PLPObjects 1 }

x25PLEConfig OBJECT IDENTIFIER
    ::= { x25PLPTables 1 }

x25PLEConfigTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF X25PLEConfigEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Provides a table of X.25 Packet Layer Protocol (PLP) Entities.  
         Each X.25 Packet Layer Entity corresponsds to an X.25 DTE or 
         DCE over which permanent virtual circuits exist or virtual 
         calls can be placed."
    REFERENCE   
        "ITU-T Rec. X.283 | ISO/IEC 10733."
    ::= { x25PLEConfig 1 }

x25PLEConfigEntry OBJECT-TYPE
    SYNTAX      X25PLEConfigEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Provides an entry in the x25PLEConfigTable.  Each entry 
         corresponds to an instance of an x25PLE managed object, which 
         represents a DTE, DCE or DXE X.25 packet layer entity, from 
         which permanent virtual circuits are formed or virtual calls 
         are established."
    REFERENCE   
        "ITU-T Rec. X.283 | ISO/IEC 10733."
    INDEX       { x25PLEConfigIndex }
    ::= { x25PLEConfigTable 1 }

X25PLEConfigEntry ::= SEQUENCE {
    x25PLEConfigIndex                               SimpleNameType,
    x25PLEConfigProtocolVersionSupported            Integer32,
    x25PLEConfigLocalDTEAddress                     DTEAddress,
    x25PLEConfigInterfaceMode                       INTEGER,
    x25PLEConfigDefaultThroughputClass              Throughput,
    x25PLEConfigFlowControlNegotiationPermitted     TruthValue,
    x25PLEConfigCallDeflectionSubscription          TruthValue,
    x25PLEConfigMaxActiveCalls                      Integer32,
    x25PLEConfigRestartTime                         TimeInterval,
    x25PLEConfigDefaultPacketSizeIncoming           Integer32,
    x25PLEConfigDefaultPacketSizeOutgoing           Integer32,
    x25PLEConfigDefaultWindowSizeIncoming           Integer32,
    x25PLEConfigDefaultWindowSizeOutgoing           Integer32,
    x25PLEConfigMinimumRecallTimer                  TimeInterval,
    x25PLEConfigRestartCount                        Integer32,
    x25PLEConfigSN-ServiceProvider                  RowPointer,
    x25PLEConfigSN-SA-P                             RowPointer,
    x25PLEConfigLogicalChannelAssignmentsPVC        OCTET STRING,
    x25PLEConfigLogicalChannelAssignmentsLIC        LogicalChannelNumber,
    x25PLEConfigLogicalChannelAssignmentsHIC        LogicalChannelNumber,
    x25PLEConfigLogicalChannelAssignmentsL2W        LogicalChannelNumber,
    x25PLEConfigLogicalChannelAssignmentsH2W        LogicalChannelNumber,
    x25PLEConfigLogicalChannelAssignmentsLOG        LogicalChannelNumber,
    x25PLEConfigLogicalChannelAssignmentsHOG        LogicalChannelNumber,
    x25PLEConfigPacketSequencing                    Integer32,
    x25PLEConfigPLEClientMOName                     RowPointer,
    x25PLEConfigRegistrationRequestTime             TimeInterval,
    x25PLEConfigRegistrationRequestCount            Integer32,
    x25PLEConfigRegistrationPermitted               TruthValue
}

x25PLEConfigIndex OBJECT-TYPE
    SYNTAX      SimpleNameType
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Provides and index for the PLE configuraiton entry."
    ::= { x25PLEConfigEntry 1 }

x25PLEConfigProtocolVersionSupported OBJECT-TYPE
    SYNTAX      Integer32 (1..255)
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "The protocol versions of ISO8208 available on the PLE 
         interface."
    REFERENCE   
        "ISO/IEC 10733 : protocolVersionSupported."
    ::= { x25PLEConfigEntry 2 }

x25PLEConfigLocalDTEAddress OBJECT-TYPE
    SYNTAX      DTEAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "The full DTE address of this PLE expressed as an X.121, X.31, 
         etc. address."
    REFERENCE   
        "ISO/IEC 10733 : localDTEAddress."
    ::= { x25PLEConfigEntry 3 }

x25PLEConfigInterfaceMode OBJECT-TYPE
    SYNTAX      INTEGER { dTE(0), dCE(1) }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "The DCE/DTE mode in which the interface is currently 
         operating."
    REFERENCE   
        "ISO/IEC 10733 : interfaceMode."
    ::= { x25PLEConfigEntry 4 }

x25PLEConfigDefaultThroughputClass OBJECT-TYPE
    SYNTAX      Throughput
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "The default throughput class value currently agreed with the 
         DCE.  This may be the normal default, or may have been changed 
         as a result of the use of the defaultThroughputClassAssignment 
         facility."
    REFERENCE   
        "ISO/IEC 10733 : defaultThroughputClass."
    ::= { x25PLEConfigEntry 5 }

x25PLEConfigFlowControlNegotiationPermitted OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Indicates whether flow control parameter negotiation is 
         permitted.  When this has the value True, the use of flow 
         control parameter negotiation (by specifying values for the 
         window and packet size in call request and accept packets) is 
         permitted.  When it has the value False, no such values shall 
         be specified in call request and accept packets, and any values 
         specified in a Profile or via an internal interface shall be 
         ignored."
    REFERENCE   
        "ISO/IEC 10733 : flowControlNegotiationPermitted."
    ::= { x25PLEConfigEntry 6 }

x25PLEConfigCallDeflectionSubscription OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Indicates whether call deflection has been subscribed to.  When 
         this has value True, call deflection has been subscribed to."
    REFERENCE   
        "ISO/IEC 10733 : callDeflectionSubscription."
    ::= { x25PLEConfigEntry 7 }

x25PLEConfigMaxActiveCalls OBJECT-TYPE
    SYNTAX      Integer32 (0..4096)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "The maximum number of active circuits permitted on this PLE.  
         When the NULL value (zero (0)) is specified, the maximum number 
         of active circuits shall be likmited only by the resources 
         available to the entity."
    REFERENCE   
        "ISO/IEC 10733 : maxActiveCalls."
    DEFVAL      { 0 }
    ::= { x25PLEConfigEntry 8 }

x25PLEConfigRestartTime OBJECT-TYPE
    SYNTAX      TimeInterval
    UNITS       "seconds"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Value for Timer T20 (Restart Request Response Timer) in 
         centiseconds.  Note that the default timer is 
         ultimately dependent upon the underlying data link 
         provider type."
    REFERENCE   
        "ISO/IEC 10733 : restartTime."
    DEFVAL      { 18000 }
    ::= { x25PLEConfigEntry 9 }

x25PLEConfigDefaultPacketSizeIncoming OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "The default value of the packet size parameter for this DTE.  A 
         value of NULL (zero (0)) indicates the ISO 8208 default value 
         of 128.  Any other value indicates the value agreed by the 
         nonstandard default packet size facility."
    REFERENCE   
        "ISO/IEC 10733 : defaultPacketSize, ITU-T Rec.  X.283 : 
         defaultPacketSizes."
    DEFVAL      { 0 }
    ::= { x25PLEConfigEntry 10 }

x25PLEConfigDefaultPacketSizeOutgoing OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "The default value of the packet size parameter for this DTE.  A 
         value of NULL (zero (0)) indicates the ISO/IEC 8208  or ITU-T 
         Rec. X.25 default value of 128.  Any other value indicates the 
         value agreed by the nonstandard default packet size facility."
    REFERENCE   
        "ISO/IEC 10733 : defaultPacketSize, ITU-T Rec.  X.283 : 
         defaultPacketSizes."
    DEFVAL      { 0 }
    ::= { x25PLEConfigEntry 11 }

x25PLEConfigDefaultWindowSizeIncoming OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "The default value of the window size parameter for this DTE.  A 
         value of NULL (zero (0)) indicates the ISO/IEC 8208 or ITU-T 
         Rec X.25 default value of 2.  Any other value indicates the 
         value agreed by the nonstandard default window sizes facility."
    REFERENCE   
        "ISO/IEC 10733 : defaultWindowSize, ITU-T Rec.  X.283 : 
         defaultWindowSizes."
    DEFVAL      { 0 }
    ::= { x25PLEConfigEntry 12 }

x25PLEConfigDefaultWindowSizeOutgoing OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "The default value of the window size parameter for this DTE.  A 
         value of NULL (zero (0)) indicates the ISO/IEC 8208 or ITU-T 
         Rec X.25 default value of 2.  Any other value indicates the 
         value agreed by the nonstandard default window sizes facility."
    REFERENCE   
        "ISO/IEC 10733 : defaultWindowSize, ITU-T Rec.  X.283 : 
         defaultWindowSizes."
    DEFVAL      { 0 }
    ::= { x25PLEConfigEntry 13 }

x25PLEConfigMinimumRecallTimer OBJECT-TYPE
    SYNTAX      TimeInterval
    UNITS       "seconds"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Minimum time in centiseconds before recall permitted.  This 
         timer determines the minimum interval (in centiseconds) which 
         shall elapse following an unsuccessful first call attempt 
         before a subsequent call attempt is permitted."
    REFERENCE   
        "ISO/IEC 10733 : minimumRecallTime."
    ::= { x25PLEConfigEntry 14 }

x25PLEConfigRestartCount OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Value for count R20 (Restart Request Retransmission Count)."
    REFERENCE   
        "ISO/IEC 10733 : restartCount."
    DEFVAL      { 1 }
    ::= { x25PLEConfigEntry 15 }

x25PLEConfigSN-ServiceProvider OBJECT-TYPE
    SYNTAX      RowPointer
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Distinguished name of the subnetwork (SN) service provider MO.  
         This attribute identifies the subnetwork entity to be used to 
         support the linkage, when enabled.  The subnetwork service 
         provider may be in the datalink layer, or it may be in the 
         network layer (for example when operating ISO 8473 over the ISO 
         8208 SNDCF)."
    REFERENCE   
        "ISO/IEC 10733 : sN-ServiceProvider."
    ::= { x25PLEConfigEntry 16 }

x25PLEConfigSN-SA-P OBJECT-TYPE
    SYNTAX      RowPointer
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Distinguished name of the service provider SA-P MO (if 
         present).  THis is obtained via an internal interface when the 
         linkage is enabled.  The sN-SA-P may be a relationship to a 
         SA-P MO in the datalink layer, or it may be a relationship to 
         another Managed Object within the network layer which is not a 
         SA-P MO.  For example, when operating ISO 8473 over the ISO 
         8208 SNDCF, it is a relationship to the same x25PLE MO which is 
         pointed to by the sN-ServiceProvider Attribute."
    REFERENCE   
        "ISO/IEC 10733 : sN-SA-P."
    ::= { x25PLEConfigEntry 17 }

x25PLEConfigLogicalChannelAssignmentsPVC OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Represents the logical channel assigments of this PLE, 
         expressed as a four-tuple where the values represent the set 
         (with maximum permitted cardinality (LIC - 1), minimum required 
         cardinality of zero) of PVC channels (with a maximum value (LIC 
         - 1), and minmum value 1) assigned, the incoming channel range, 
         the two-way channel range, the outgoing channel range, 
         respectively.
         
         The presence of each of the rangest shall be optional.  Absense 
         of a particular range shall signify that there are no channels 
         of that type assigned.  Within each range, the low value shall 
         be less than or equal to the high value, and there shall be no 
         value in any set or range which is greater than or equal to a 
         value in a subsequent range when ordered as above.
         
         This attribute is subject to the rules for logical assignements 
         described in ISO/IEC 8208 clause 3.7.  it is understood that 
         the Highest Permanent Channel (HPC) is defined by the Lowest 
         Incoming Channel (LIC) value minus one.
         
         The OCTET STRING is encoded as two octets, most significant 
         octet followed by least significant octet, where each pair of 
         octets represents another channel number from 1..4095 (but less 
         than LIC), the logical channel number to which a Permanent 
         Virtual Circuit (PVC) has been assigned."
    REFERENCE   
        "ISO/IEC 10733 : logicalChannelAssignments, ISO/IEC 8208 clause 
         3.7."
    ::= { x25PLEConfigEntry 18 }

x25PLEConfigLogicalChannelAssignmentsLIC OBJECT-TYPE
    SYNTAX      LogicalChannelNumber
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "The lowest incoming logical channel in the incoming logical 
         channel range.  When set to zero (0), or the same value as the 
         highest incoming logical channel, it indicates that there is no 
         incoming logical channel range and the highest incoming logical 
         channel is ignored.
         
         This value is subject to the rules for logical channel 
         assignments described in ISO/IEC 8208 clause 3.7."
    REFERENCE   
        "ISO/IEC 10733 ; logicalChannelAssignments, ISO/IEC 8208 clause 
         3.7."
    ::= { x25PLEConfigEntry 19 }

x25PLEConfigLogicalChannelAssignmentsHIC OBJECT-TYPE
    SYNTAX      LogicalChannelNumber
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "The highest incoming logical channel in the incoming logical 
         channel range.  When set to zero (0), or the same value as the 
         lowest incoming logical channel, it indicates that there is no 
         incoming logical channel range and the lowest incoming logical 
         channel is ignored.
         
         This value is subject to the rules for logical channel 
         assignments described in ISO/IEC 8208 clause 3.7."
    REFERENCE   
        "ISO/IEC 10733 ; logicalChannelAssignments, ISO/IEC 8208 clause 
         3.7."
    ::= { x25PLEConfigEntry 20 }

x25PLEConfigLogicalChannelAssignmentsL2W OBJECT-TYPE
    SYNTAX      LogicalChannelNumber
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "The lowest two-way logical channel in the two-way logical 
         channel range.  When set to zero (0), or to the same value as 
         the highest two-way logical channel, it indicates that there is 
         no two-way logical channel range and the highest two-way 
         logical channel is ignored.
         
         This value is subject to the rules for logical channel 
         assignments described in ISO/IEC 8208 clause 3.7."
    REFERENCE   
        "ISO/IEC 10733 ; logicalChannelAssignments, ISO/IEC 8208 clause 
         3.7."
    ::= { x25PLEConfigEntry 21 }

x25PLEConfigLogicalChannelAssignmentsH2W OBJECT-TYPE
    SYNTAX      LogicalChannelNumber
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "The highest two-way logical channel in the two-way logical 
         channel range.  When set to zero (0), or to the same value as 
         the lowest two-way logical channel, it indicates that there is 
         no two-way logical channel range and the lowest two-way logical 
         channel is ignored.
         
         This value is subject to the rules for logical channel 
         assignments described in ISO/IEC 8208 clause 3.7."
    REFERENCE   
        "ISO/IEC 10733 ; logicalChannelAssignments, ISO/IEC 8208 clause 
         3.7."
    ::= { x25PLEConfigEntry 22 }

x25PLEConfigLogicalChannelAssignmentsLOG OBJECT-TYPE
    SYNTAX      LogicalChannelNumber
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "The lowest outgoing logical channel in the outgoing logical 
         channel range.  When set to zero (0), or to the  same value as 
         the highest outgoing logical channel, it indicates that there 
         is no outgoing channel range and the highest outgoing logical 
         channel is ignored.
         
         This value is subject to the rules for logical channel 
         assignments described in ISO/IEC 8208 clause 3.7."
    REFERENCE   
        "ISO/IEC 10733 ; logicalChannelAssignments, ISO/IEC 8208 clause 
         3.7."
    ::= { x25PLEConfigEntry 23 }

x25PLEConfigLogicalChannelAssignmentsHOG OBJECT-TYPE
    SYNTAX      LogicalChannelNumber
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "The highest outgoing logical channel in the outgoing logical 
         channel range.  When set to zero (0), or to the same value as 
         the lowest outgoing logical channel, it indicates that there is 
         no outgoing logical channel range and the lowest outgoing 
         logical channel is otherwise ignored.
         
         This value is subject to the rules for logical channel 
         assignments described in ISO/IEC 8208 clause 3.7."
    REFERENCE   
        "ISO/IEC 10733 ; logicalChannelAssignments, ISO/IEC 8208 clause 
         3.7."
    ::= { x25PLEConfigEntry 24 }

x25PLEConfigPacketSequencing OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "The module of the packet sequence number space.  Expressed as 
         an integer.  ISO/IEC 8208 only requires support for at least 
         one of the two values 8 and 128, but it is possible that some 
         future revision may extend the range.  A system is only 
         required to support the setting of values which are also 
         required by the protocol standard.  A system shall return an 
         error when an attempt is made to set the value to a value which 
         is not supported by that system."
    REFERENCE   
        "ISO/IEC 10733 : packetSequencing."
    ::= { x25PLEConfigEntry 25 }

x25PLEConfigPLEClientMOName OBJECT-TYPE
    SYNTAX      RowPointer
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "The Distinguished name of the client MO.  Note that this will 
         either be a Transport Layer MO or a CLNS MO."
    REFERENCE   
        "ISO/IEC 10733 : pLEClientMOName."
    ::= { x25PLEConfigEntry 26 }

x25PLEConfigRegistrationRequestTime OBJECT-TYPE
    SYNTAX      TimeInterval
    UNITS       "seconds"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Value for Timer T28 (Registration Request Timer) in 
         centiseconds."
    REFERENCE   
        "ISO/IEC 10733 : regsitrationRequestTime."
    DEFVAL      { 30000 }
    ::= { x25PLEConfigEntry 27 }

x25PLEConfigRegistrationRequestCount OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Value for count R28 (Registration Request Count)."
    REFERENCE   
        "ISO/IEC 10733 : registrationRequestCount."
    DEFVAL      { 1 }
    ::= { x25PLEConfigEntry 28 }

x25PLEConfigRegistrationPermitted OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "When true, the use of online facility registration is 
         permitted."
    REFERENCE   
        "ISO/IEC 10733 : registrationPermitted."
    DEFVAL      { false }
    ::= { x25PLEConfigEntry 29 }

x25PLEProfile OBJECT IDENTIFIER
    ::= { x25PLPTables 2 }

x25PLEProfileTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF X25PLEProfileEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "This table provides profiles for the x25PLE.  These profiles 
         are not created nor deleted by management stations; however, 
         their values may be altered.  Each entry in the table consists 
         of a separate profile.  The managemed element may choose to use 
         profile values when creating instances of x25PLEs (entries in 
         the x25PLEConfig table."
    REFERENCE   
        "ITU-T Rec. X.283 | ISO/IEC 10733."
    ::= { x25PLEProfile 1 }

x25PLEProfileEntry OBJECT-TYPE
    SYNTAX      X25PLEProfileEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Each entry in the table provides a separate profile identified 
         by the x25PLEProfileId which consists of a DisplayString used 
         to identify the profile.  The agent or implementation may used 
         specific profiles to create instances of x25PLEs.  Management 
         stations may not create nor delete entries in this table, 
         however, the values associated with a given profile may be 
         altered."
    REFERENCE   
        "ITU-T Rec. X.283 | ISO/IEC 10733."
    INDEX       { x25PLEProfileName }
    ::= { x25PLEProfileTable 1 }

X25PLEProfileEntry ::= SEQUENCE {
    x25PLEProfileName                               SimpleNameType,
    x25PLEProfileLocalDTEAddress                    DTEAddress,
    x25PLEProfileInterfaceMode                      INTEGER,
    x25PLEProfileDefaultThroughputClass             Throughput,
    x25PLEProfileFlowControlNegotiationPermitted    TruthValue,
    x25PLEProfileCallDeflectionSubscription         TruthValue,
    x25PLEProfileMaxActiveCircuits                  Integer32,
    x25PLEProfileRestartTime                        TimeInterval,
    x25PLEProfileDefaulPacketSizeIncoming           Integer32,
    x25PLEProfileDefaulPacketSizeOutgoing           Integer32,
    x25PLEProfileDefaultWindowSizeIncoming          Integer32,
    x25PLEProfileDefaultWindowSizeOutgoing          Integer32,
    x25PLEProfileMinimumRecallTimer                 TimeInterval,
    x25PLEProfileRestartCount                       Integer32,
    x25PLEProfileSN-ServiceProvider                 RowPointer,
    x25PLEProfileSN-SA-P                            RowPointer,
    x25PLEProfileLogicalChannelAssignementsPVC      Integer32,
    x25PLEProfileLogicalChannelAssignementsLIC      Integer32,
    x25PLEProfileLogicalChannelAssignementsHIC      Integer32,
    x25PLEProfileLogicalChannelAssignementsL2W      Integer32,
    x25PLEProfileLogicalChannelAssignementsH2W      Integer32,
    x25PLEProfileLogicalChannelAssignementsLOG      Integer32,
    x25PLEProfileLogicalChannelAssignementsHOG      Integer32,
    x25PLEProfilePacketSequencing                   Integer32,
    x25PLEProfileRegistrationRequestTime            TimeInterval,
    x25PLEProfileRegistrationRequestCount           Integer32,
    x25PLEProfileRegistrationPermitted              TruthValue
}

x25PLEProfileName OBJECT-TYPE
    SYNTAX      SimpleNameType
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "A simple name index for this PLE profile."
    ::= { x25PLEProfileEntry 1 }

x25PLEProfileLocalDTEAddress OBJECT-TYPE
    SYNTAX      DTEAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "The protocol versions of ISO8208 available on the PLE 
         interface."
    REFERENCE   
        "ISO/IEC 10733 : protocolVersionSupported."
    ::= { x25PLEProfileEntry 2 }

x25PLEProfileInterfaceMode OBJECT-TYPE
    SYNTAX      INTEGER { dTE(0), dCE(1) }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "The DCE/DTE mode in which the interface is currently 
         operating."
    REFERENCE   
        "ISO/IEC 10733 : interfaceMode."
    ::= { x25PLEProfileEntry 3 }

x25PLEProfileDefaultThroughputClass OBJECT-TYPE
    SYNTAX      Throughput
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "The default throughput class value currently agreed with the 
         DCE.  This may be the normal default, or may have been changed 
         as a result of the use of the defaultThroughputClassAssignment 
         facility."
    ::= { x25PLEProfileEntry 4 }

x25PLEProfileFlowControlNegotiationPermitted OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Indicates whether flow control parameter negotiation is 
         permitted.  When this has the value True, the use of flow 
         control parameter negotiation (by specifying values for the 
         window and packet size in call request and accept packets) is 
         permitted.  When it has the value False, no such values shall 
         be specified in call request and accept packets, and any values 
         specified in a Profile or via an internal interface shall be 
         ignored."
    REFERENCE   
        "ISO/IEC 10733 : flowControlNegotiationPermitted."
    ::= { x25PLEProfileEntry 5 }

x25PLEProfileCallDeflectionSubscription OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Indicates whether call deflection has been subscribed to.  When 
         this has value True, call deflection has been subscribed to."
    REFERENCE   
        "ISO/IEC 10733 : callDeflectionSubscription."
    ::= { x25PLEProfileEntry 6 }

x25PLEProfileMaxActiveCircuits OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "The maximum number of active circuits permitted on this PLE.  
         When the NULL value (zero (0)) is specified, the maximum number 
         of active circuits shall be likmited only by the resources 
         available to the entity."
    REFERENCE   
        "ISO/IEC 10733 : maxActiveCalls."
    DEFVAL      { 0 }
    ::= { x25PLEProfileEntry 7 }

x25PLEProfileRestartTime OBJECT-TYPE
    SYNTAX      TimeInterval
    UNITS       "seconds"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Value for Timer T20 (Restart Request Response Timer) in 
         centiseconds.  Note that the default timer is ultimately 
         dependent upon the underlying data link provider type."
    REFERENCE   
        "ISO/IEC 10733 : restartTime."
    DEFVAL      { 18000 }
    ::= { x25PLEProfileEntry 8 }

x25PLEProfileDefaulPacketSizeIncoming OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "The default value of the packet size parameter for this DTE.  A 
         value of NULL (zero (0)) indicates the ISO 8208 default value 
         of 128.  Any other value indicates the value agreed by the 
         nonstandard default packet size facility."
    REFERENCE   
        "ISO/IEC 10733 : defaultPacketSize, ITU-T Rec.  X.283 : 
         defaultPacketSizes."
    DEFVAL      { 0 }
    ::= { x25PLEProfileEntry 9 }

x25PLEProfileDefaulPacketSizeOutgoing OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "The default value of the packet size parameter for this DTE.  A 
         value of NULL (zero (0)) indicates the ISO/IEC 8208  or ITU-T 
         Rec. X.25 default value of 128.  Any other value indicates the 
         value agreed by the nonstandard default packet size facility."
    REFERENCE   
        "ISO/IEC 10733 : defaultPacketSize, ITU-T Rec.  X.283 : 
         defaultPacketSizes."
    DEFVAL      { 0 }
    ::= { x25PLEProfileEntry 10 }

x25PLEProfileDefaultWindowSizeIncoming OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "The default value of the window size parameter for this DTE.  A 
         value of NULL (zero (0)) indicates the ISO/IEC 8208 or ITU-T 
         Rec X.25 default value of 2.  Any other value indicates the 
         value agreed by the nonstandard default window sizes facility."
    REFERENCE   
        "ISO/IEC 10733 : defaultWindowSize, ITU-T Rec.  X.283 : 
         defaultWindowSizes."
    DEFVAL      { 0 }
    ::= { x25PLEProfileEntry 11 }

x25PLEProfileDefaultWindowSizeOutgoing OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "The default value of the window size parameter for this DTE.  A 
         value of NULL (zero (0)) indicates the ISO/IEC 8208 or ITU-T 
         Rec X.25 default value of 2.  Any other value indicates the 
         value agreed by the nonstandard default window sizes facility."
    REFERENCE   
        "ISO/IEC 10733 : defaultWindowSize, ITU-T Rec.  X.283 : 
         defaultWindowSizes."
    DEFVAL      { 0 }
    ::= { x25PLEProfileEntry 12 }

x25PLEProfileMinimumRecallTimer OBJECT-TYPE
    SYNTAX      TimeInterval
    UNITS       "seconds"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Minimum time in centiseconds before recall permitted.  This 
         timer determines the minimum interval (in centiseconds) which 
         shall elapse following an unsuccessful first call attempt 
         before a subsequent call attempt is permitted."
    REFERENCE   
        "ISO/IEC 10733 : minimumRecallTime."
    ::= { x25PLEProfileEntry 13 }

x25PLEProfileRestartCount OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Value for count R20 (Restart Request Retransmission Count)."
    REFERENCE   
        "ISO/IEC 10733 : restartCount."
    DEFVAL      { 1 }
    ::= { x25PLEProfileEntry 14 }

x25PLEProfileSN-ServiceProvider OBJECT-TYPE
    SYNTAX      RowPointer
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Distinguished name of the subnetwork (SN) service provider MO.  
         This attribute identifies the subnetwork entity to be used to 
         support the linkage, when enabled.  The subnetwork service 
         provider may be in the datalink layer, or it may be in the 
         network layer (for example when operating ISO 8473 over the ISO 
         8208 SNDCF)."
    REFERENCE   
        "ISO/IEC 10733 : sN-ServiceProvider."
    ::= { x25PLEProfileEntry 15 }

x25PLEProfileSN-SA-P OBJECT-TYPE
    SYNTAX      RowPointer
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Distinguished name of the service provider SA-P MO (if 
         present).  THis is obtained via an internal interface when the 
         linkage is enabled.  The sN-SA-P may be a relationship to a 
         SA-P MO in the datalink layer, or it may be a relationship to 
         another Managed Object within the network layer which is not a 
         SA-P MO.  For example, when operating ISO 8473 over the ISO 
         8208 SNDCF, it is a relationship to the same x25PLE MO which is 
         pointed to by the sN-ServiceProvider Attribute."
    REFERENCE   
        "ISO/IEC 10733 : sN-SA-P."
    ::= { x25PLEProfileEntry 16 }

x25PLEProfileLogicalChannelAssignementsPVC OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Represents the logical channel assigments of this PLE, 
         expressed as a four-tuple where the values represent the set 
         (with maximum permitted cardinality (LIC - 1), minimum required 
         cardinality of zero) of PVC channels (with a maximum value (LIC 
         - 1), and minmum value 1) assigned, the incoming channel range, 
         the two-way channel range, the outgoing channel range, 
         respectively.
         
         The presence of each of the rangest shall be optional.  Absense 
         of a particular range shall signify that there are no channels 
         of that type assigned.  Within each range, the low value shall 
         be less than or equal to the high value, and there shall be no 
         value in any set or range which is greater than or equal to a 
         value in a subsequent range when ordered as above.
         
         This attribute is subject to the rules for logical assignements 
         described in ISO/IEC 8208 clause 3.7.  it is understood that 
         the Highest Permanent Channel (HPC) is defined by the Lowest 
         Incoming Channel (LIC) value minus one.
         
         The OCTET STRING is encoded as two octets, most significant 
         octet followed by least significant octet, where each pair of 
         octets represents another channel number from 1..4095 (but less 
         than LIC), the logical channel number to which a Permanent 
         Virtual Circuit (PVC) has been assigned."
    REFERENCE   
        "ISO/IEC 10733 : logicalChannelAssignments, ISO/IEC 8208 clause 
         3.7."
    ::= { x25PLEProfileEntry 17 }

x25PLEProfileLogicalChannelAssignementsLIC OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "The lowest incoming logical channel in the incoming logical 
         channel range.  When set to zero (0), or the same value as the 
         highest incoming logical channel, it indicates that there is no 
         incoming logical channel range and the highest incoming logical 
         channel is ignored.
         
         This value is subject to the rules for logical channel 
         assignments described in ISO/IEC 8208 clause 3.7."
    REFERENCE   
        "ISO/IEC 10733 ; logicalChannelAssignments, ISO/IEC 8208 clause 
         3.7."
    ::= { x25PLEProfileEntry 18 }

x25PLEProfileLogicalChannelAssignementsHIC OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "The highest incoming logical channel in the incoming logical 
         channel range.  When set to zero (0), or the same value as the 
         lowest incoming logical channel, it indicates that there is no 
         incoming logical channel range and the lowest incoming logical 
         channel is ignored.
         
         This value is subject to the rules for logical channel 
         assignments described in ISO/IEC 8208 clause 3.7."
    REFERENCE   
        "ISO/IEC 10733 ; logicalChannelAssignments, ISO/IEC 8208 clause 
         3.7."
    ::= { x25PLEProfileEntry 19 }

x25PLEProfileLogicalChannelAssignementsL2W OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "The lowest two-way logical channel in the two-way logical 
         channel range.  When set to zero (0), or to the same value as 
         the highest two-way logical channel, it indicates that there is 
         no two-way logical channel range and the highest two-way 
         logical channel is ignored.
         
         This value is subject to the rules for logical channel 
         assignments described in ISO/IEC 8208 clause 3.7."
    REFERENCE   
        "ISO/IEC 10733 ; logicalChannelAssignments, ISO/IEC 8208 clause 
         3.7."
    ::= { x25PLEProfileEntry 20 }

x25PLEProfileLogicalChannelAssignementsH2W OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "The highest two-way logical channel in the two-way logical 
         channel range.  When set to zero (0), or to the same value as 
         the lowest two-way logical channel, it indicates that there is 
         no two-way logical channel range and the lowest two-way logical 
         channel is ignored.
         
         This value is subject to the rules for logical channel 
         assignments described in ISO/IEC 8208 clause 3.7."
    REFERENCE   
        "ISO/IEC 10733 ; logicalChannelAssignments, ISO/IEC 8208 clause 
         3.7."
    ::= { x25PLEProfileEntry 21 }

x25PLEProfileLogicalChannelAssignementsLOG OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "The lowest outgoing logical channel in the outgoing logical 
         channel range.  When set to zero (0), or to the  same value as 
         the highest outgoing logical channel, it indicates that there 
         is no outgoing channel range and the highest outgoing logical 
         channel is ignored.
         
         This value is subject to the rules for logical channel 
         assignments described in ISO/IEC 8208 clause 3.7."
    REFERENCE   
        "ISO/IEC 10733 ; logicalChannelAssignments, ISO/IEC 8208 clause 
         3.7."
    ::= { x25PLEProfileEntry 22 }

x25PLEProfileLogicalChannelAssignementsHOG OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "The highest outgoing logical channel in the outgoing logical 
         channel range.  When set to zero (0), or to the same value as 
         the lowest outgoing logical channel, it indicates that there is 
         no outgoing logical channel range and the lowest outgoing 
         logical channel is otherwise ignored.
         
         This value is subject to the rules for logical channel 
         assignments described in ISO/IEC 8208 clause 3.7."
    REFERENCE   
        "ISO/IEC 10733 ; logicalChannelAssignments, ISO/IEC 8208 clause 
         3.7."
    ::= { x25PLEProfileEntry 23 }

x25PLEProfilePacketSequencing OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "The module of the packet sequence number space.  Expressed as 
         an integer.  ISO/IEC 8208 only requires support for at least 
         one of the two values 8 and 128, but it is possible that some 
         future revision may extend the range.  A system is only 
         required to support the setting of values which are also 
         required by the protocol standard.  A system shall return an 
         error when an attempt is made to set the value to a value which 
         is not supported by that system."
    REFERENCE   
        "ISO/IEC 10733 : packetSequencing."
    ::= { x25PLEProfileEntry 24 }

x25PLEProfileRegistrationRequestTime OBJECT-TYPE
    SYNTAX      TimeInterval
    UNITS       "seconds"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Value for Timer T28 (Registration Request Timer) in 
         centiseconds."
    REFERENCE   
        "ISO/IEC 10733 : regsitrationRequestTime."
    DEFVAL      { 30000 }
    ::= { x25PLEProfileEntry 25 }

x25PLEProfileRegistrationRequestCount OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Value for count R28 (Registration Request Count)."
    REFERENCE   
        "ISO/IEC 10733 : registrationRequestCount."
    DEFVAL      { 1 }
    ::= { x25PLEProfileEntry 26 }

x25PLEProfileRegistrationPermitted OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "When true, the use of online facility registration is 
         permitted."
    REFERENCE   
        "ISO/IEC 10733 : registrationPermitted."
    DEFVAL      { false }
    ::= { x25PLEProfileEntry 27 }

x25PLEState OBJECT IDENTIFIER
    ::= { x25PLPTables 3 }

x25PLEStateTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF X25PLEStateEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "The x25PLEStateTable provides state information for each x25PLE 
         object which are represented by the rows in the table.  Each 
         row corresponds to an X.25 Packet Layer Entity."
    REFERENCE   
        "ITU-T Rec. X.283 | ISO/IEC 10733, ITU-T Rec.  X.721 | ISO/IEC 
         10165-2."
    ::= { x25PLEState 1 }

x25PLEStateEntry OBJECT-TYPE
    SYNTAX      X25PLEStateEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "The x25PLEStateEntry provides an entry in the X.25 Packet Layer 
         Entity state table which provides the state information for a 
         single packet layer entity as indicated by the index into the 
         table."
    REFERENCE   
        "ITU-T Rec. X.283 | ISO/IEC 10733, ITU-T Rec.  X.721 | ISO/IEC 
         10165-2."
    INDEX       { x25PLEStateIndex }
    ::= { x25PLEStateTable 1 }

X25PLEStateEntry ::= SEQUENCE {
    x25PLEStateIndex                SimpleNameType,
    x25PLEStateAdministrativeState  AdministrativeState,
    x25PLEStateOperationalState     OperationalState,
    x25PLEStateUsageState           UsageState,
    x25PLEStateProceduralStatus     ProceduralStatus,
    x25PLEStateAlarmStatus          AlarmStatus
}

x25PLEStateIndex OBJECT-TYPE
    SYNTAX      SimpleNameType
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Provides an index for the PLE state table."
    ::= { x25PLEStateEntry 1 }

x25PLEStateAdministrativeState OBJECT-TYPE
    SYNTAX      AdministrativeState
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Provides the administrativee state of the PLE following the 
         AdministrativeState textual convention of the OPENSS7-SMI-MIB 
         module."
    REFERENCE   
        "ITU-T Rec. X.721 | ISO/IEC 10165-2 administrativeState."
    ::= { x25PLEStateEntry 2 }

x25PLEStateOperationalState OBJECT-TYPE
    SYNTAX      OperationalState
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Provides the operational state of the PLE following the 
         OperationalState textual convention of the OPENSS7-SMI-MIB 
         module."
    REFERENCE   
        "ITU-T Rec. X.721 | ISO/IEC 10165-2 operationalState."
    ::= { x25PLEStateEntry 3 }

x25PLEStateUsageState OBJECT-TYPE
    SYNTAX      UsageState
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Provides the usage status of the PLE following the UsageStatus 
         textual convention of the OPENSS7-SMI-MIB module."
    REFERENCE   
        "ITU-T Rec. X.721 | ISO/IEC 10165-2 usageStatus."
    ::= { x25PLEStateEntry 4 }

x25PLEStateProceduralStatus OBJECT-TYPE
    SYNTAX      ProceduralStatus
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Provides the procedural status of the PLE following the 
         ProceduralStatus textual convention of the OPENSS7-SMI-MIB 
         module."
    REFERENCE   
        "ITU-T Rec. X.721 | ISO/IEC 10165-2 proceduralStatus."
    ::= { x25PLEStateEntry 5 }

x25PLEStateAlarmStatus OBJECT-TYPE
    SYNTAX      AlarmStatus
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Provides the alarm status of the PLE following the AlarmStatus 
         textual convention of the OPENSS7-SMI-MIB module."
    REFERENCE   
        "ITU-T Rec. X.721 | ISO/IEC 10165-2 alarmStatus."
    ::= { x25PLEStateEntry 6 }

x25PLEStats OBJECT IDENTIFIER
    ::= { x25PLPTables 4 }

x25PLEStatsTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF X25PLEStatsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "The x25PLEStatsTable provides statistics and counts for each 
         X25PLE object which are represented by the rows in the table.  
         Each row corresponds to an X.25 Packet Layer Entity."
    REFERENCE   
        "ITU-T Rec. X.283 | ISO/IEC 10733."
    ::= { x25PLEStats 1 }

x25PLEStatsEntry OBJECT-TYPE
    SYNTAX      X25PLEStatsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "An entry in the x25PLEStatsTable.  Each row or entry provides 
         statistics for one X.25 Packet Layer Entity.  Rows cannot be 
         created or deleted by management stations."
    REFERENCE   
        "ITU-T Rec. X.283 | ISO/IEC 10733."
    INDEX       { x25PLEStatsIndex }
    ::= { x25PLEStatsTable 1 }

X25PLEStatsEntry ::= SEQUENCE {
    x25PLEStatsIndex                                    SimpleNameType,
    x25PLEStatsOctetsSentCounter                        Counter32,
    x25PLEStatsOctetsReceivedCounter                    Counter32,
    x25PLEStatsDataPacketsSent                          Counter32,
    x25PLEStatsDataPacketsReceived                      Counter32,
    x25PLEStatsCallAttempts                             Counter32,
    x25PLEStatsCallsConnected                           Counter32,
    x25PLEStatsProviderInitiatedDisconnects             Counter32,
    x25PLEStatsCallTimeouts                             Counter32,
    x25PLEStatsClearTimeouts                            Counter32,
    x25PLEStatsRemotelyInitiatedResets                  Counter32,
    x25PLEStatsDataRetransmissionTimerExpires           Counter32,
    x25PLEStatsProviderInitiatedResets                  Counter32,
    x25PLEStatsResetTimeouts                            Counter32,
    x25PLEStatsRemotelyInitiatedRestarts                Counter32,
    x25PLEStatsRestartCountsExceeded                    Counter32,
    x25PLEStatsProtocolErrorsDetectedLocally            Counter32,
    x25PLEStatsProtocolErrorsAccusedOf                  Counter32,
    x25PLEStatsCallEstablishmentRetryCountsExceeded     Counter32,
    x25PLEStatsClearCountsExceeded                      Counter32
}

x25PLEStatsIndex OBJECT-TYPE
    SYNTAX      SimpleNameType
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "A simple name that provides an index for the PLE to which the 
         statistics in this row correspond."
    ::= { x25PLEStatsEntry 1 }

x25PLEStatsOctetsSentCounter OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "This corresponds to the ISO/IEC 8208 Octets Sent attribute.  
         Note that the DMI definition is in terms of user data octets."
    REFERENCE   
        "ISO/IEC 10733 : octetsSentCounter."
    ::= { x25PLEStatsEntry 2 }

x25PLEStatsOctetsReceivedCounter OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "This corresponds to the ISO/IEC 8208 Octets Received attribute.  
         Note that the DMI definition is in terms of user data octets."
    REFERENCE   
        "ISO/IEC 10733 : octetsSentCounter."
    ::= { x25PLEStatsEntry 3 }

x25PLEStatsDataPacketsSent OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Counter of the total number of data packets sent."
    REFERENCE   
        "ISO/IEC 10733 : octetsSentCounter."
    ::= { x25PLEStatsEntry 4 }

x25PLEStatsDataPacketsReceived OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Counter of the total number of data packets received."
    REFERENCE   
        "ISO/IEC 10733 : octetsSentCounter."
    ::= { x25PLEStatsEntry 5 }

x25PLEStatsCallAttempts OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Counter of the total number of calls attempted."
    REFERENCE   
        "ISO/IEC 10733 : octetsSentCounter."
    ::= { x25PLEStatsEntry 6 }

x25PLEStatsCallsConnected OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Counter of the total number of calls which have reached the 
         open state."
    REFERENCE   
        "ISO/IEC 10733 : octetsSentCounter."
    ::= { x25PLEStatsEntry 7 }

x25PLEStatsProviderInitiatedDisconnects OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Counter for the provider initiated disconnect events which 
         generate communications alarm notifications."
    REFERENCE   
        "ISO/IEC 10733 : providerInitiatedDisconnects."
    ::= { x25PLEStatsEntry 8 }

x25PLEStatsCallTimeouts OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Counter of the number of times timer T21 expiry is experienced 
         by the PLE."
    REFERENCE   
        "ISO/IEC 10733 : octetsSentCounter."
    ::= { x25PLEStatsEntry 9 }

x25PLEStatsClearTimeouts OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Counter of the number of times timer T23 expiry is experienced 
         by the PLE."
    REFERENCE   
        "ISO/IEC 10733 : clearTimeouts."
    ::= { x25PLEStatsEntry 10 }

x25PLEStatsRemotelyInitiatedResets OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Counter associated with the remotely initiated reset event 
         which generates a communications alarm notification."
    REFERENCE   
        "ISO/IEC 10733 : octetsSentCounter."
    ::= { x25PLEStatsEntry 11 }

x25PLEStatsDataRetransmissionTimerExpires OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Counter of the number of expires of timer T25.  Returns zero if 
         the option is not implemented."
    REFERENCE   
        "ISO/IEC 10733 : octetsSentCounter."
    ::= { x25PLEStatsEntry 12 }

x25PLEStatsProviderInitiatedResets OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Counter associated with the provider initiated reset event 
         which generates a communication alarm notification."
    REFERENCE   
        "ISO/IEC 10733 : octetsSentCounter."
    ::= { x25PLEStatsEntry 13 }

x25PLEStatsResetTimeouts OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Counter of the number of timer T22 expires experienced by the 
         PLE."
