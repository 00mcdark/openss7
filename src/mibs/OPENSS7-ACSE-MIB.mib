aCSE OBJECT IDENTIFIER
    ::= { acseObjectClass 1 }

    -- DERIVED FROM aso
        -- communiationsEntityId
        -- asoTitle
        -- asoQualifier
        -- operationalState
        -- aso-aso NAME BINDING
        -- aso-asoEntity NAME BINDING
    -- acseFUinitiator
    -- acseEditionsSupported
    -- acseFUresponder

acseTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF AcseEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Provides a table of application context service entities.
         
         This subclass of ASO (augmentation of the asoTable) is 
         a specialized application service object that holds reference 
         information about an ACSE application service element.  It 
         specializes by adding the following attributes:
         
         - acseFUinitiator, the ACSE functional units supported as 
           initiator;
         - acseFUresponder, the ACSE functional units supported as 
           responder;
         - acseEditionsSupported, the ACSE editions supported.
         
         An application service element does not qualify as an ASO 
         entity or as an X.723 communications entity because it does 
         not have both the association phase and the transfer phase of 
         communication."
    REFERENCE   
        "ITU-T Rec. X.287 | ISO/IEC 10165-8 : 2000, aCSE MOC."
    ::= { aCSE 1 }

acseEntry OBJECT-TYPE
    SYNTAX      AcseEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Each row in thist able is indexed by the subsystem, application 
         process, ASO entity and ASO identifiers.  Each row contains 
         characteristics fo the ACSE."
    REFERENCE   
        "ITU-T Rec. X.287 | ISO/IEC 10165-8 : 2000, aCSE MO."
    INDEX       { subsystemId, applicationProcessId, asoEntityId, asoId }
    ::= { acseTable 1 }

AcseEntry ::= SEQUENCE {
    acseFUinitiator         AcseFUs,
    acseEditionsSupported   AcseEditionsSupported,
    acseFUresponder         AcseFUs
}

acseFUinitiator OBJECT-TYPE
    SYNTAX      AcseFUs
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Indicates the optional ACSE (X.217) functional units 
         (protocol options) that are supported by this ACSE in the role 
         of the initiator (client).  The ACSE kernel (required protocol 
         features) are always supported."
    REFERENCE   
        "ITU-T Rec. X.287 | ISO/IEC 10165-8 : 2000, acseFUinitiator 
               ATTRIBUTE."
    ::= { acseEntry 1 }

acseEditionsSupported OBJECT-TYPE
    SYNTAX      AcseEditionsSupported
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Provides the ACSE (X.217) editions (versions) that are 
         supported by this ACSE.  This attribute is set-valued and can 
         contain zero ore more of the following:
         
         - version1(0), when set version 1 of the ACSE protocol as 
           defined by ITU-T Rec. X.227 is supported.
           
         There is only one version of the ASCE protocol."
    REFERENCE   
        "ITU-T Rec. X.287 | ISO/IEC 10165-8 : 2000, 
               acseEditionsSupported ATTRIBUTE."
    ::= { acseEntry 2 }

acseFUresponder OBJECT-TYPE
    SYNTAX      AcseFUs
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Provides the optional ACSE (X.217) functional units (protocol 
         options) that are supported by this ACSE in the role of 
         a responder (server).  The ACSE kernel (required protocol 
         features) are always supported."
    REFERENCE   
        "ITU-T Rec. X.287 | ISO/IEC 10165-8 : 2000, acseFUresponder 
               ATTRIBUTE."
    ::= { acseEntry 3 }

acseAssociation OBJECT IDENTIFIER
    ::= { acseObjectClass 2 }

    -- DERIVED FROM asoInvocation
        -- connectionId
        -- invocationOfPointer
        -- asoInvocation-asoInvocation NAME BINDING
        -- asoInvocation-apInvocation NAME BINDING
    -- callingAEtitle
    -- acseFUinUse
    -- applContextInUse
    -- parentASOinvoc
    -- calledAEtitlePackage PACKAGE
        -- calledAEtitle

acseAssociationTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF AcseAssociationEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "This subclass of asoInvocation holds references information for 
         an ACSE association.  It specializes by adding the following 
         attributes:
         
         - calling AE title,
         - ACSE functional units negotiated for use,
         - Application context in use,
         - parent ASO invocation (relationship pointer to parent ASO 
           invocation, which could be the AEI).
           
         It may also have an attribute for the called AE title, if 
         known."
    ::= { acseAssociation 1 }

acseAssociationEntry OBJECT-TYPE
    SYNTAX      AcseAssociationEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        ""
    INDEX       { applicationSubsystemId, apInvocationId, 
                  asoInvocationId }
    ::= { acseAssociationTable 1 }

AcseAssociationEntry ::= SEQUENCE {
    acseAssociationCallingAEtitle       OCTET STRING,
    acseAssociationFUinUse              AcseFUs,
    acseAssociationApplContextInUse     OBJECT IDENTIFIER,
    acseAssociationParentAsoinvoc       RowPointer,
    acseAssociationCalledAEtitle        OCTET STRING,
    acseAssociationRowStatus            RowStatus
}

acseAssociationCallingAEtitle OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        ""
    ::= { acseAssociationEntry 1 }

acseAssociationFUinUse OBJECT-TYPE
    SYNTAX      AcseFUs
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Indicates the (optional) ACSE functional units that were 
         negotiated for use on the association.  The ACSE kernel is 
         always provided.  This attribute is set-valued and can contain 
         zero or more of the following:
         
         - authentication(0), authentication is provided when present;
         - applicationContextNegotiation(1), application context can be 
           negotiated when present.
         
         Note that additional ACSE function units might be added in the 
         future (hah!)."
    ::= { acseAssociationEntry 2 }

acseAssociationApplContextInUse OBJECT-TYPE
    SYNTAX      OBJECT IDENTIFIER
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        ""
    ::= { acseAssociationEntry 3 }

acseAssociationParentAsoinvoc OBJECT-TYPE
    SYNTAX      RowPointer
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        ""
    ::= { acseAssociationEntry 4 }

acseAssociationCalledAEtitle OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        ""
    ::= { acseAssociationEntry 5 }

acseAssociationRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        ""
    ::= { acseAssociationEntry 6 }

apInvocation OBJECT IDENTIFIER
    ::= { acseObjectClass 3 }

    -- apInvocationId
    -- invocationOfPointer
    -- apInvocation-applicationSubsystem NAME BINDING

apInvocationTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF ApInvocationEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "This subclass of Rec. X.721 | ISO/IEC 10165-2 : 1992 : top 
         holds reference information about an Application Process 
         Invocation. It specializes by adding the following attributes:
         
         - apInvocationId;
         
         - invocationOfPointer.
         
         The apInvocationId attribute serves as the naming attribute for 
         instances of this class. The invocationOfPointer attribute 
         identifies the specific Application Process (using the 
         distinguished name, DN, of an instance of the Rec. X.723 | 
         ISO/IEC 10165-5 : 1994 : applicationProcess managed object 
         class) of which the instance of this class is an invocation."
    ::= { apInvocation 1 }

apInvocationEntry OBJECT-TYPE
    SYNTAX      ApInvocationEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        ""
    INDEX       { applicationSubsystemId, apInvocationId }
    ::= { apInvocationTable 1 }

ApInvocationEntry ::= SEQUENCE {
    apInvocationId          SimpleNameType,
    apInvocationOfPointer   RowPointer,
