PresentationContext ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION 
        "Providews a specification of a presentation context with the 
         syntax of an OBJECT IDENITIFER."
    SYNTAX      OBJECT IDENTIFIER

PresentationRequirements ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION 
        "Provides a set of presentation requirements."
    REFERENCE   
        "ITU-T Rec. X.226 | ISO/IEC 8823, 8823-PRESENTATION ASN module, 
             Presentation-requirements."
    SYNTAX      BITS { contextManagement(0), restoration(1) }

PresentationSelectorValue ::= TEXTUAL-CONVENTION
    DISPLAY-HINT "1x"
    STATUS      current
    DESCRIPTION 
        "Provides a specification of a presentation layer selector."
    REFERENCE   
        "ITU-T Rec. X.226 | ISO/IEC 8823, 8823-PRESENTATION ASN module, 
             Presentation-selector."
    SYNTAX      OCTET STRING

SessFUs ::= TEXTUAL-CONVENTION
