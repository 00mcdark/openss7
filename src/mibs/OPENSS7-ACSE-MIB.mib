    SYNTAX      BITS { halfDuplex(0), duplex(1), expeditedData(2), 
                    minorSynchronize(3), majorSynchronize(4), 
                    resynchronize(5), activityManagement(6), 
                    negotiatedRelease(7), capabilityData(8), 
                    exceptions(9), typedData(10) }

SessOptSupport ::= TEXTUAL-CONVENTION
