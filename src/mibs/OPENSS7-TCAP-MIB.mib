tcapApplicationSubsystem OBJECT IDENTIFIER
    ::= { tcapObjectClass 2 }

    -- DERIVED FROM applicationSubsystem
        -- DERIVED FROM subsystem
            -- subsystemId
            -- subsystem-system NAME BINDING

tcapApplicationSubsystemTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF TcapApplicationSubsystemEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "This table provides a simple mechanism for binding TCAP 
         appllication subsystem names into TCAP systems."
    ::= { tcapApplicationSubsystem 1 }

tcapApplicationSubsystemEntry OBJECT-TYPE
    SYNTAX      TcapApplicationSubsystemEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Each row in this table is indexed by TCAP system identifier and 
         application subsystem identifier.  Each row is otherwise empty 
         and an application subsystem is a simple container for 
         TCAP users."
    INDEX       { tcapSystemId, tcapApplicationSubsystemId }
    ::= { tcapApplicationSubsystemTable 1 }

TcapApplicationSubsystemEntry ::= SEQUENCE {
    tcapSystemId                        Integer32,
    tcapApplicationSubsystemId          SimpleNameType,
    tcapApplicationSubsystemRowStatus   RowStatus
}

tcapSystemId OBJECT-TYPE
    SYNTAX      Integer32 (0..65535)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Provides an ordinal composite index into this table.  This 
         index is used to identify TCAP systems."
    ::= { tcapApplicationSubsystemEntry 1 }

tcapApplicationSubsystemId OBJECT-TYPE
    SYNTAX      SimpleNameType
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Provides an index into this table.  This index is used in 
         conjunction with the TCAP system identifier to name TCAP 
         application subsystems."
    ::= { tcapApplicationSubsystemEntry 2 }

tcapApplicationSubsystemRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "Provides a mechanism whereby management stations may create and 
         delete rows in the TCAP application subsystem table.
         
         Entries in this table persist across an agent reboot."
    ::= { tcapApplicationSubsystemEntry 3 }

-- ====================================================================

tcapTransactionCopm OBJECT IDENTIFIER
    ::= { tcapObjectClass 3 }

    -- DERIVED FROM coProtocolMachine
        -- coProtocolMachineId
        -- operationalState
        -- coProtocolMachine-entity NAME BINDING

tcapTransactionCopmTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF TcapTransactionCopmEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "This table provides a simple mechanism for binding 
         connection-oriented protocol machines into TCAP entities."
    ::= { tcapTransactionCopm 1 }

tcapTransactionCopmEntry OBJECT-TYPE
    SYNTAX      TcapTransactionCopmEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Each row in this table is indexed by a connection-oriented 
         protocol machine identifier.  Each row contains the operational 
         state of the connection-oriented protocol machine."
    INDEX       { tcapTransactionCopmId }
    ::= { tcapTransactionCopmTable 1 }

TcapTransactionCopmEntry ::= SEQUENCE {
    tcapTransactionCopmId                   Unsigned32,
    tcapTransactionCopmOperationalState     OperationalState,
    tcapTransactionCopmRowStatus            RowStatus
}

tcapTransactionCopmId OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Provides an ordinal index into the connection-oriented protocol 
         machine table."
    ::= { tcapTransactionCopmEntry 1 }

tcapTransactionCopmOperationalState OBJECT-TYPE
    SYNTAX      OperationalState
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Provides the operational state of the TCAP transaction 
         connection-oriented protocol machine following the 
         OperationalState textual convention and ITU-T X.721 | ISO/IEC 
         10165-2.  This is a single-valued attribute that can assume the 
         following values:
         
         - disabled(0), the TCAP transaction conection-oriented protocol 
           machine is inoperable and unable to provide service to its 
           user(s);
           
         - enabled(1), the TCAP transaction connection-oriented protocol 
           machine is partially or fully operable and available for use.
           
         A TCAP transaction connection-oriented protocol machine can be 
         created in the enabled(1) state."
    REFERENCE   
        "ITU-T Rec. X.287 (2000) | ISO/IEC 10165-8 : 2000, 
             applicationEntity MO, operationalState ATTRIBUTE.
         ITU-T Rec. X.721 (1992) | ISO/IEC 10165-2 : 1992, 
               Attribute-ASN1Module.OperationalState.
         ITU-T Rec. X.731 (1992) | ISO/IEC 10164-2 : 1993, 
               Section 8.1.1.1."
    ::= { tcapTransactionCopmEntry 2 }

tcapTransactionCopmRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "Provides a mechanism whereby management stations may created 
         and delete rows in the TCAP transaction connection-oriented 
         protocol machine table.  Entries in this table persist across 
         an agent reboot."
    ::= { tcapTransactionCopmEntry 3 }

-- ====================================================================

tcapTransaction OBJECT IDENTIFIER
    ::= { tcapObjectClass 4 }

    -- DERIVED FROM monoULConnection
        -- DERIVED FROM applicationEntityInvocation
            -- DERIVED FROM singlePeerConnection
                -- connectionId
                -- underlyingConnectionNames
                -- singlePeerConnectionP2 PACKAGE
                    -- supportedConnectionNames
                -- singlePeerConnection-coProtocolMachine NAME BINDING
            -- DERIVED FROM asoInvocation
                -- connectionId
                -- invocationOfPointer
                -- asoInvocation-asoInvocation NAME BINDING
                -- asoInvocation-apInvocation NAME BINDING
        -- DERIVED FROM presentationConnection
            -- DERIVED FROM singlePeerConnection
                -- connectionId
                -- underlyingConnectionNames
                -- singlePeerConnectionP2 PACKAGE
                    -- supportedConnectionNames
                -- singlePeerConnection-coProtocolMachine NAME BINDING
            -- presFUinUse
            -- presContextInUse
        -- DERIVED FROM sessionConnection
            -- DERIVED FROM singlePeerConnection
                -- connectionId
                -- underlyingConnectionNames
                -- singlePeerConnectionP2 PACKAGE
                    -- supportedConnectionNames
                -- singlePeerConnection-coProtocolMachine NAME BINDING
            -- sessProtInUse
            -- sessFUinUse
            -- PACKAGE maxTSDUsizePackage
                -- maxTSDUsize

tcapTransactionTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF TcapTransactionEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Provides a table of TCAP transactions.
         
         Note that TCAP transactions are almost identical to the 
         monolithic upper layer connection described in ITU-T Rec. 
         X.287 | ISO/IEC 10165-8."
    ::= { tcapTransaction 1 }

tcapTransactionEntry OBJECT-TYPE
    SYNTAX      TcapTransactionEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Each entry in the table consists of a single instance of a TCAP 
         transaction.  Entries are added to the table autonomously by 
         the agent from within the managed system."
    INDEX       { tcapApInvocationId, tcapTransactionId }
    ::= { tcapTransactionTable 1 }

TcapTransactionEntry ::= SEQUENCE {
    tcapApInvocationId                          Integer32,
    tcapTransactionId                           Integer32,
    tcapTransactionUnderlyingConnectionNames    RowPointer,
    tcapTransactionInvocationOfPointer          RowPointer,
    tcapTransactionPresFUinUse                  PresentationRequirements,
    tcapTransactionPresContextInUse             PresentationContext,
    tcapTransactionSessProtInUse                SessionProtVer,
    tcapTransactionSessFUInUse                  SessFUs,
    tcapTransactionMaxTSDUsize                  Unsigned32
}

tcapApInvocationId OBJECT-TYPE
    SYNTAX      Integer32 (0..65535)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Provides an ordinal application invocation identifier as an 
         index into the transaction table."
    ::= { tcapTransactionEntry 1 }

tcapTransactionId OBJECT-TYPE
    SYNTAX      Integer32 (0..65535)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Provides an ordinal transaction identifier as an index into the 
         transaction table.  This transaction identifier may have 
         significance in the protocol: that is, it might be made equal 
         (by the agent) to the local transaction identifier that appears 
         in the TCAP messages themselves."
    ::= { tcapTransactionEntry 2 }

tcapTransactionUnderlyingConnectionNames OBJECT-TYPE
    SYNTAX      RowPointer
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Provides a pointer to side table entries containing a list of 
         underlying connection names supporting the transaction."
    ::= { tcapTransactionEntry 3 }

tcapTransactionInvocationOfPointer OBJECT-TYPE
    SYNTAX      RowPointer
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Provides a pointer to side table entries containing a list of 
         application subsystems(?) invoked by the transaction."
    ::= { tcapTransactionEntry 4 }

tcapTransactionPresFUinUse OBJECT-TYPE
    SYNTAX      PresentationRequirements
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Provides a set of presentation layer functional units that are 
         in use by the transaction."
    ::= { tcapTransactionEntry 5 }

tcapTransactionPresContextInUse OBJECT-TYPE
    SYNTAX      PresentationContext
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Provides a set of presentation layer contexts that are in use 
         by the transaction."
    ::= { tcapTransactionEntry 6 }

tcapTransactionSessProtInUse OBJECT-TYPE
    SYNTAX      SessionProtVer
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Provides an identification of the session layer protocol 
         version in use by the transaction."
    ::= { tcapTransactionEntry 7 }

tcapTransactionSessFUInUse OBJECT-TYPE
    SYNTAX      SessFUs
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Provides a set of session layer functional units that are in 
         use by the transaction."
    ::= { tcapTransactionEntry 8 }

tcapTransactionMaxTSDUsize OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Provides the maximum transport layer service data unit size 
         available to the transaction."
    ::= { tcapTransactionEntry 9 }

-- ====================================================================

tcapDialogue OBJECT IDENTIFIER
    ::= { tcapObjectClass 5 }

    -- DERIVED from acseAssociation
        -- DERIVED FROM asoInvocation
            -- connectionId
            -- invocationOfPointer
            -- asoInvocation-asoInvocation NAME BINDING
            -- asoInvocation-apInvocation NAME BINDING
        -- callingAEtitle
        -- acseFUinUse
        -- applContextInUse
        -- parentASOinvoc
        -- calledAEtitlePackage PACKAGE
            -- calledAEtitle

tcapDialogueTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF TcapDialogueEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Provides a table of TCAP dialogues."
    ::= { tcapDialogue 1 }

tcapDialogueEntry OBJECT-TYPE
    SYNTAX      TcapDialogueEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Each entry in the table consists of a single instance of a TCAP 
         dialogue.  Entries are added to the table autonomously by the 
         agent from within the managed system."
    INDEX       { tcapApInvocationId, tcapTransactionId, tcapDialogueId }
    ::= { tcapDialogueTable 1 }

TcapDialogueEntry ::= SEQUENCE {
    tcapDialogueId                      Unsigned32,
    tcapDialogueInvocationOfPointer     RowPointer,
    tcapDialogueCallingAEtitle          SubSystemNumber,
    tcapDialogueAcseFUinUse             AcseFUs,
    tcapDialogueApplContextInUse        OBJECT IDENTIFIER,
    tcapDialogueParentASOinvoc          RowPointer,
    tcapDialogueCalledAEtitle           SubSystemNumber
}

tcapDialogueId OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Provides a non-zero index into the TCAP dialogue table for each 
         TCAP application process invocation id and TCAP transaction id.  
         The value of this index is not significant."
    ::= { tcapDialogueEntry 1 }

tcapDialogueInvocationOfPointer OBJECT-TYPE
    SYNTAX      RowPointer
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Provides a pointer into the application entity table that 
         describes the invoked application entity."
    ::= { tcapDialogueEntry 2 }

tcapDialogueCallingAEtitle OBJECT-TYPE
    SYNTAX      SubSystemNumber
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Identifies the calling TCAP application entity title.  The 
         calling TCAP application entity title corresponds to the SCCP 
         calling party address."
    ::= { tcapDialogueEntry 3 }

tcapDialogueAcseFUinUse OBJECT-TYPE
    SYNTAX      AcseFUs
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Indicates the set of application layer functional units that 
         are in use by the dialogue."
    ::= { tcapDialogueEntry 4 }

tcapDialogueApplContextInUse OBJECT-TYPE
    SYNTAX      OBJECT IDENTIFIER
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Indicates the application context that is in use by the 
         dialogue."
    ::= { tcapDialogueEntry 5 }

tcapDialogueParentASOinvoc OBJECT-TYPE
    SYNTAX      RowPointer
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Indicates the parent application service object invocation of 
         the dialogue."
    ::= { tcapDialogueEntry 6 }

tcapDialogueCalledAEtitle OBJECT-TYPE
    SYNTAX      SubSystemNumber
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Identifies the called TCAP application entity title.  The 
         called TCAP application entity title corresponds to the SCCP 
         called part address."
    ::= { tcapDialogueEntry 7 }

