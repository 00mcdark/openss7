tcapOperation OBJECT IDENTIFIER
    ::= { tcapObjectClass 6 }

    -- DERIVED FROM apInvocation
        -- apInvocationId
        -- invocationOfPointer
        -- apInvocation-applicationSubsystem NAME BINDING

-- ====================================================================

tcapTcUser OBJECT IDENTIFIER
    ::= { tcapObjectClass 7 }

    -- DERIVED FROM apInvocation
        -- apInvocationId
        -- invocationOfPointer
        -- apInvocation-applicationSubsystem NAME BINDING

tcapTcUserTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF TcapTcUserEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "This table contains all instances of TC Users in the TCAP 
         system."
    ::= { tcapTcUser 1 }

tcapTcUserEntry OBJECT-TYPE
    SYNTAX      TcapTcUserEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Each entry represents an invocation of an application process.   
         For TCAP, an application process corresponds to an instance of 
         a TC User."
    INDEX       { tcapSystemId, tcapApplicationSubsystemId, tcapTcUserId }
    ::= { tcapTcUserTable 1 }

TcapTcUserEntry ::= SEQUENCE {
    tcapTcUserId                    Unsigned32,
    tcapTcUserInvocationOfPointer   RowPointer
}

tcapTcUserId OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Provides a non-zero ordinal index into the TC User table by 
         system id, application subsystem id and TC User id.  The value 
         of the index is not significant and is chosen by the agent."
    ::= { tcapTcUserEntry 1 }

tcapTcUserInvocationOfPointer OBJECT-TYPE
    SYNTAX      RowPointer
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "Specifies the TCAP application process of which this TC User 
         is an instantiation.  The value of this field may be 
         zeroDotZero whe the TC User has not yet bound itself to a TCAP 
         application process.  When bound to a TCAP application process, 
         the value is a pointer to the TCAP application process to which 
         the TC User is bound."
    ::= { tcapTcUserEntry 2 }

-- ====================================================================

tcapApplicationProcess OBJECT IDENTIFIER
    ::= { tcapObjectClass 8 }

    -- DERIVED FROM applicationProcess
        -- applicationProcessId
        -- applicationProcessTitle
        -- supportEntityNames
        -- operationalState
        -- applicationProcess-system NAME BINDING (applicationProcessId)
        -- ap-applicationSubsystem NAME BINDING (applicationProcessId)

tcapApplicationProcessTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF TcapApplicationProcessEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Provides a table of TCAP application processes."
    ::= { tcapApplicationProcess 1 }

tcapApplicationProcessEntry OBJECT-TYPE
    SYNTAX      TcapApplicationProcessEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Each entry in this table is indexed by the TCAP system 
         identifier, application subsystem identifier and application 
         process identifier.  Each row contains the supported entity 
         names and operational state of the application process."
    INDEX       { tcapSystemId, tcapApplicationSubsystemId, 
                  tcapApplicationProcessId }
    ::= { tcapApplicationProcessTable 1 }

TcapApplicationProcessEntry ::= SEQUENCE {
    tcapApplicationProcessId                    SimpleNameType,
    tcapApplicationProcessTitle                 OBJECT IDENTIFIER,
    tcapApplicationProcessSupportEntityNames    Unsigned32,
    tcapApplicationProcessOperationalState      OperationalState,
    tcapApplicationProcessRowStatus             RowStatus
}

tcapApplicationProcessId OBJECT-TYPE
    SYNTAX      SimpleNameType
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Provides a non-zero index into the TCAP application process 
         table using the SimpleNameType textual convention of the 
         OPENSS7-SMI-MIB."
    ::= { tcapApplicationProcessEntry 1 }

tcapApplicationProcessTitle OBJECT-TYPE
    SYNTAX      OBJECT IDENTIFIER
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "Provides a specfication of an TCAP application process title.  
         A TCAP application process title is an SCCP address that 
         corresponds to the TCAP application process.  This represents 
         the SCCP address of a local SCCP against which the TCAP 
         application process is associated.  Application process titles 
         in OSI applications are object identifiers.  In the case of 
         TCAP, they are an SCCP called party address."
    ::= { tcapApplicationProcessEntry 2 }

tcapApplicationProcessSupportEntityNames OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "This field provides the number of associated support entity 
         names at that appear in the tcapSupportEntityNameTable 
         side-table that correspond to this application process."
    ::= { tcapApplicationProcessEntry 3 }

tcapApplicationProcessOperationalState OBJECT-TYPE
    SYNTAX      OperationalState
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Provides the operational state of the TCAP application process 
         following the OperationalState textual convention and ITU-T 
         Rec. X.721 | ISO/IEC 10165-2.  This state is single-valued and 
         may contain one of the following values:
         
         - disabled(0): the TCAP application process is inoperable and 
           unable to provide service to its users(s);
         
         - enabled(1): the TCAP application entity is partially or fully 
           operable and available for use.
         
         When the TCAP application process does not have defined any 
         support entities, the TCAP AP has an operational state of 
         disabled(0).
         
         When the TCAP AP contains support entities, but no support 
         entity is operable, the TCAP AP has an operational state of 
         disabled(0)."
    ::= { tcapApplicationProcessEntry 4 }

tcapApplicationProcessRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "Provides a mechanism whereby management stations may create 
         and delete rows in the TCAP application process table.  Entries 
         in this table persist across an agent reboot."
    ::= { tcapApplicationProcessEntry 5 }

-- side table for supportEntityNames

tcapSupportEntityNameTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF TcapSupportEntityNameEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Provides a set of support entity names for a given application 
         process.  Because this table represents a set, a given value of 
         the non-index fields of an entry can only occur once for a 
         given TCAP application process."
    ::= { tcapApplicationProcess 2 }

tcapSupportEntityNameEntry OBJECT-TYPE
    SYNTAX      TcapSupportEntityNameEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Each row in this side table is indexed by the TCAP system 
         identifier, application subsystem identifier, application 
         process identifier and defines the support application entity 
         index.  Each row contains a pointer into the application entity 
         table."
    INDEX       { tcapSystemId, tcapApplicationSubsystemId, 
                  tcapApplicationProcessId, tcapSupportEntityNameId }
    ::= { tcapSupportEntityNameTable 1 }

TcapSupportEntityNameEntry ::= SEQUENCE {
    tcapSupportEntityNameId         Unsigned32,
    tcapSupportEntityNamePointer    RowPointer,
    tcapSupportEntityRowStatus      RowStatus
}

tcapSupportEntityNameId OBJECT-TYPE
    SYNTAX      Unsigned32
