rtpswOmCurrent OBJECT IDENTIFIER
    ::= { rtpswOmScalars 1 }

rtpswOmOctetsSent OBJECT-TYPE
    SYNTAX      Counter64
    UNITS       "Octets"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "A count of the number of RTP switching octets sent 
         during the current interval.  Counts include UDP, IP 
         and MAC headers."
    ::= { rtpswOmCurrent 1 }

rtpswOmOctetsRecv OBJECT-TYPE
    SYNTAX      Counter64
    UNITS       "Octets"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "A count of the number of RTP switching octets received 
         during the current interval.  Counts include UDP, IP 
         and MAC headers."
    ::= { rtpswOmCurrent 2 }

rtpswOmPacketsSent OBJECT-TYPE
    SYNTAX      Counter64
    UNITS       "Packets"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "A count of the number of RTP switching packets sent 
         during the current interval.  Counts include STUN 
         packets."
    ::= { rtpswOmCurrent 3 }

rtpswOmPacketsRecv OBJECT-TYPE
    SYNTAX      Counter64
    UNITS       "Packets"
    MAX-ACCESS  read-only
    STATUS      current
