rtpswOmTables OBJECT IDENTIFIER
    ::= { rtpswOmMIB 3 }

rtpswOm5MinHistory OBJECT IDENTIFIER
    ::= { rtpswOmTables 1 }

rtpswOm5MinHistoryTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF RtpswOm5MinHistoryEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "The 5-minute history table contains various statistics 
         collected for the host over the pervious 24 hours of 
         operation.  The past 24 hours are broken into 288 
         completed 5-minute intervals."
    ::= { rtpswOm5MinHistory 1 }

rtpswOm5MinHistoryEntry OBJECT-TYPE
    SYNTAX      RtpswOm5MinHistoryEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Each row in the rtpswOm5MinHistoryTable represents one 
         of 288 5-minute intervals collected over the past 
         24-hour period.  Each row represents one ssuch interval 
         (identified by its rtpswOm5MinInterval)."
    INDEX       { rtpswOm5MinInterval }
    ::= { rtpswOm5MinHistoryTable 1 }

RtpswOm5MinHistoryEntry ::= SEQUENCE {
    rtpswOm5MinOctetsSent       Counter64,
    rtpswOm5MinOctetsRecv       Counter64,
    rtpswOm5MinPacketsSent      Counter64,
    rtpswOm5MinPacketsRecv      Counter64,
    rtpswOm5MinStunPktsSent     Counter64,
    rtpswOm5MinStunPktsRecv     Counter64
}

rtpswOm5MinOctetsSent OBJECT-TYPE
    SYNTAX      Counter64
    UNITS       "Octets"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Provides the historical count of sent octets within the 
         indexed 5-minute interval."
    ::= { rtpswOm5MinHistoryEntry 1 }

rtpswOm5MinOctetsRecv OBJECT-TYPE
    SYNTAX      Counter64
    UNITS       "Octets"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
