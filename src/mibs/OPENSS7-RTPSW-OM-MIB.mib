rtpswInterface5MinHistory OBJECT IDENTIFIER
    ::= { rtpswOmTables 4 }

rtpswInterface5MinHistoryTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF RtpswInterface5MinHistoryEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "The interface 5-minute history table contains various 
         statistics collected for each interface over the 
         previous 24 hours of operation.  The past 24 hours are 
         broken into 288 completed 5-minute intervals."
    ::= { rtpswInterface5MinHistory 1 }

rtpswInterface5MinHistoryEntry OBJECT-TYPE
    SYNTAX      RtpswInterface5MinHistoryEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Each row in the rtpswInterface5MinHistoryTable 
         represents one of 288 5-minute intervals collected over 
         the past 24-hour period.  Each row represents one such 
         interval (identified by its rtpswOm5MinInterval number) for 
         one specific instance (identified by its 
         rtpswInterfaceId)."
    INDEX       { rtpswInterfaceId, rtpswOm5MinInterval }
    ::= { rtpswInterface5MinHistoryTable 1 }

RtpswInterface5MinHistoryEntry ::= SEQUENCE {
    rtpswOm5MinInterval         Integer32,
    rtpswIfOm5MinOctetsSent     HCPerfIntervalCount,
    rtpswIfOm5MinOctetsRecv     HCPerfIntervalCount,
    rtpswIfOm5MinPacketsSent    HCPerfIntervalCount,
    rtpswIfOm5MinPacketsRecv    HCPerfIntervalCount,
    rtpswIfOm5MinStunPktsSent   HCPerfIntervalCount,
    rtpswIfOm5MinStunPktsRecv   HCPerfIntervalCount
}

rtpswOm5MinInterval OBJECT-TYPE
    SYNTAX      Integer32 (0..288)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "A historical data collection interval for use as an 
         index.  Specifies the historical data collection 
         interval as a time-sequence with the lowest ordinal 
         value '1' representing the newest data collected and 
         the highest ordinal value '288' being the oldest data 
         collected."
    ::= { rtpswInterface5MinHistoryEntry 1 }

rtpswIfOm5MinOctetsSent OBJECT-TYPE
    SYNTAX      HCPerfIntervalCount
    UNITS       "Octets"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
