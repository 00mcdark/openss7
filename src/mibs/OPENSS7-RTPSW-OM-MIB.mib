rtpswInterfaceCurrent OBJECT IDENTIFIER
    ::= { rtpswOmTables 3 }

rtpswInterfaceCurrentTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF RtpswInterfaceCurrentEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "The interface current table contains various statistics 
         being collected for the current 5- and 15-minute 
         intervals."
    ::= { rtpswInterfaceCurrent 1 }

rtpswInterfaceCurrentEntry OBJECT-TYPE
    SYNTAX      RtpswInterfaceCurrentEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Each entry in the rtpswInterfaceCurrentTable represents 
         a set of statistics that are being collected for the 
         current 5- and 15-minute intervals for a specific 
         interface as indexed by the rtpswInterfaceId."
    INDEX       { rtpswInterfaceId }
    ::= { rtpswInterfaceCurrentTable 1 }

RtpswInterfaceCurrentEntry ::= SEQUENCE {
    rtpswIfOmOctetsSent             Counter64,
    rtpswIfOmOctetsRecv             Counter64,
    rtpswIfOmPacketsSent            Counter64,
    rtpswIfOmPacketsRecv            Counter64,
    rtpswIfOmStunPktsSent           Counter64,
    rtpswIfOmStunPktsRecv           Counter64,
    rtpswIfOm5minValidIntervals     HCPerfValidIntervals,
    rtpswIfOm15minValidIntervals    HCPerfValidIntervals
}

rtpswIfOmOctetsSent OBJECT-TYPE
    SYNTAX      Counter64
    UNITS       "Octets"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "A count of the number of RTP switching octets sent on 
         the interface during the current interval.  Counts do 
         not include UDP headers."
    ::= { rtpswInterfaceCurrentEntry 1 }

rtpswIfOmOctetsRecv OBJECT-TYPE
    SYNTAX      Counter64
    UNITS       "Octets"
    MAX-ACCESS  read-only
    STATUS      current
