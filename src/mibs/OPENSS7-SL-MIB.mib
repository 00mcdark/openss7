slNotifications OBJECT IDENTIFIER
    ::= { slMIB 0 }

slObjects OBJECT IDENTIFIER
    ::= { slMIB 1 }

slTables OBJECT IDENTIFIER
    ::= { slObjects 1 }

slSl OBJECT IDENTIFIER
    ::= { slTables 1 }

slProfileTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF SlProfileEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Provides the MTP signalling link level 2 protocol profile 
         table.  This able contains MTP Level 2 protocol profiles for 
         various transmission rates."
    REFERENCE   
        "ITU-T Rec. Q.751.1 (1997), ANSI T1.116.1 (2000), 
         mtpL2ProtocolProfile MOC."
    ::= { slSl 1 }

slProfileEntry OBJECT-TYPE
    SYNTAX      SlProfileEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Provides the MTP signalling link level 2 protocol profile 
         table entry.  Included in the entry is the transmission rate 
         which is applicable to the entry as well as the settings of the 
         various timer values and other fundamental protocol parameters.  
         Each entry can be referred to by index or by RowPointer.  When 
         a table refers to this one, the agent should ensure the 
         consistency of MTP SL values with the transmission rate of the 
         referencing table, and the MTP L3 protocol values associated 
         with the referenceing table."
    REFERENCE   
        "ITU-T Rec. Q.751.1 (1997), ANSI T1.116.1 (2000), 
         mtpL2ProtocolProfile MO."
    INDEX       { slProfileId, slProfileTransmissionRate }
    ::= { slProfileTable 1 }

SlProfileEntry ::= SEQUENCE {
    slProfileId                         SimpleNameType,
    slProfileTransmissionRate           MtpTransmissionRate,
    slProfileErrorCorrectionMethod      INTEGER,
    slProfileProtocolVariant            Ss7ProtocolVariant,
    slProfileTimerT1                    TimeInterval,
    slProfileTimerT2                    TimeInterval,
    slProfileTimerT2L                   TimeInterval,
    slProfileTimerT2H                   TimeInterval,
    slProfileTimerT3                    TimeInterval,
    slProfileTimerT4N                   TimeInterval,
    slProfileTimerT4E                   TimeInterval,
    slProfileTimerT5                    TimeInterval,
    slProfileTimerT6                    TimeInterval,
    slProfileTimerT7                    TimeInterval,
    slProfileTbOnset1                   Unsigned32,
    slProfileTbAbate1                   Unsigned32,
    slProfileNumberOfThresholdLevels    Unsigned32,
    slProfileCongestionCounting         INTEGER,
    slProfileCongestionRepBaseObject    INTEGER,
    slProfileLoopDelay                  MilliSeconds,
    slProfileName                       SimpleNameType,
    slProfileTbOnset2                   Unsigned32,
    slProfileTbAbate2                   Unsigned32,
    slProfileTbOnset3                   Unsigned32,
    slProfileTbAbate3                   Unsigned32,
    slProfileTbDiscard1                 Unsigned32,
    slProfileTbDiscard2                 Unsigned32,
    slProfileTbDiscard3                 Unsigned32,
    slProfileTimerTx                    TimeInterval,
    slProfileTimerTy                    TimeInterval,
    slProfileNumOfCongestionStates      Unsigned32,
    slProfileInitLevelOfCongestion      Unsigned32,
    slProfileMaxMSUsRetransN1           Unsigned32,
    slProfileMaxOctRetransN2            Unsigned32,
    slProfileReceiveCongThreshOnset     Unsigned32,
    slProfileReceiveCongThreshAbate     Unsigned32,
    slProfileReceiveCongThreshDisc      Unsigned32,
    slProfileM                          Unsigned32,
    slProfileRowStatus                  RowStatus
}

slProfileId OBJECT-TYPE
    SYNTAX      SimpleNameType
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Provides an index into the signalling link MTP Level 2 profile 
         table.  Rows in this table can be referred to using this index 
         or using the RowPointer pointer.  The index value zero (0) has 
         special semantics: the zero index contains default default 
         values.  Other vendors requiring additional device specific 
         profile information can augment this table.
         
         Identifies a mtpL2ProtocolProfile instance within the 
         managedSwitching-element; this is an attribute type whose 
         distinguished value can be used as an RDN when naming an 
         instance of the mtpL2ProtocolProfile object class."
    REFERENCE   
        "ITU-T Rec. Q.751.1 (1997), ANSI T1.116.1 (2000), 
         mtpL2ProtocolProfileId attribute."
    ::= { slProfileEntry 1 }

slProfileTransmissionRate OBJECT-TYPE
    SYNTAX      MtpTransmissionRate
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Provides an index into the signalling link MTP Level 2 profile 
         table by transmission rate.  The transmission rate zero (0) has 
         special semantics: the zero value indicates defaults for any 
         transmission rate.  Otherwise, the row in the slProfileTable 
         with this value only applies to signalling links with the 
         specific transmission rate."
    REFERENCE   
        "ITU-T Rec. Q.703 (1996).  ANSI T1.111.3/2000.  ETSI 300 008-1."
    ::= { slProfileEntry 2 }

slProfileErrorCorrectionMethod OBJECT-TYPE
    SYNTAX      INTEGER { basicRetr(0), prevCyclRetr(1) }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "This attribute is used to specify the type of error-correction 
         the signalling link is using.
         
         - basicRetr(0), basic retransmission method;
         
         - prevCyclRetr(1), preventative cyclic retransmission method.
         
         Not all methods are applicable to all signalling links and all 
         implementations."
    REFERENCE   
        "ITU-T Rec. Q.751.1 (1997), ANSI T1.116.1 (2000), 
         errorCorrectionMethod attribute."
    DEFVAL      { basicRetr }
    ::= { slProfileEntry 3 }

slProfileProtocolVariant OBJECT-TYPE
    SYNTAX      Ss7ProtocolVariant
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "Provides the SS7 protocol variant used by the signalling link.  
         The SS7 protocol variant can determine the presence of other 
         columns and their default or required values."
    ::= { slProfileEntry 4 }

slProfileTimerT1 OBJECT-TYPE
    SYNTAX      TimeInterval (1290..60000)
    UNITS       "seconds"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "MTP Level 2 T1 timer.
         
         Q.703/T1.111.3: Level 2 timer 'Alignment ready'.
         
         Timeout interval T1.  This is the time interval that a 
         signalling link will await an MSU or FISU after sending an MSU 
         or FISU at the end of the initial alignment procedure.
         
         For modems, ITU-T specifies a range from 500 to 600 seconds 
         with a nominal value of 550 seconds.  For ds0, ITU-T specifies 
         a range from 40 to 50 seconds with a nominal value of 45 
         seconds.  For t1 and e1, ITU-T specifies a range of 25 to 350 
         seconds with a nomiinal value of 300 seconds.
         
         For ds0a and ds0, ANSI specifies a range from 12.9 to 16 
         seconds, with a nominal value of 13 seconds.  For t1, ANSI 
         specifies a range of 165 to 200 seconds with a nominal value of 
         170 seconds.
         
         It is the responsibility of the agent to enforce any necessary 
         bounds."
    REFERENCE   
        "ITU-T Rec. Q.751.1 (1997), ANSI T1.116.1 (2000), l2TimerT1 
         attribute.
         ITU-T Rec. Q.703 (1996).  ANSI T1.111.3/2000.  ETSI 300 008-1.
         ITU-T Rec. Q.703, ANSI T1.111.3, T1 timer.
         ITU-T Rec. Q.703 (07/96) 12.3, Timer T1.
         ITU-T Rec. Q.703 (07/96) Annex A, A.12.3, Timer T1."
    DEFVAL      { 4500 }
    ::= { slProfileEntry 5 }

slProfileTimerT2 OBJECT-TYPE
    SYNTAX      TimeInterval (500..15000)
    UNITS       "seconds"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "MTP Level 2 T2 timer.
         
         Q.703/T1.111.3: Level 2 timer 'Not aligned'.
         
         Timeout interval T2.  This is the time interval that a 
         signalling link will await an SIO after sending an SIO at the 
         start of the initial alignment procedure.
         
         For all signalling link transmission rates, ITU-T specifies a 
         range of 5 to 150 seconds, with a nominal value of 5 seconds."
    REFERENCE   
        "ITU-T Rec. Q.751.1 (1997), ANSI T1.116.1 (2000), l2TimerT2 
         attribute.
         ITU-T Rec. Q.703 (1996).  ANSI T1.111.3/2000.  ETSI 300 008-1.
         ITU-T Rec. Q.703 (07/96) 12.3, Timer T2.
         ITU-T Rec. Q.703, ANSI T1.111.3, T2 timer."
    DEFVAL      { 500 }
    ::= { slProfileEntry 6 }

slProfileTimerT2L OBJECT-TYPE
    SYNTAX      TimeInterval
    UNITS       "seconds"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "MTP Level 2 T2 timer: low value for automatic allocation of 
         signalling data terminals.
         
         Timeout interval T2l.  This is the time interval that a 
         signalling link will await an SIO after sending an SIO at the 
         start of the intial alignment procedure.  This is the low value 
         for use with automatic allocation of signalling data links and 
         terminals, where the value of T2 at either end of the link must 
         be different.
         
         For all signalling link transmission speeds, ITU-T specifies a 
         range of 5 to 50 seconds, with a provisional value of 20 
         seconds."
    REFERENCE   
        "ITU-T Rec. Q.704 (1996) : 12.3.4.3
         ITU-T Rec. Q.703 (07/96) 12.3, Timer T2 low.
         ITU-T Rec. Q.703 (1996).  ANSI T1.111.3/2000.  ETSI 300 008-1."
    DEFVAL      { 2000 }
    ::= { slProfileEntry 7 }

slProfileTimerT2H OBJECT-TYPE
    SYNTAX      TimeInterval
    UNITS       "seconds"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "MTP Level 2 T2 timer: high value for automatic allocation of 
         signalling data terminals.
         
         Timeout interval T2h.  This is the time interval that a 
         signalling link will await an SIO after sending an SIO at the 
         start of the initial alignment procedure.  This is the high 
         value for use with automatic allocation of signalling data 
         links and terminals, where the value of T2 at either end of the 
         link must be different.
         
         For all signalling link transmission speeds, ITU-T specifies a 
         range of 70 to 150 seconds, with a provisional value of 100 
         seconds."
    REFERENCE   
        "ITU-T Rec. Q.704 (1996) : 12.3.4.3
         ITU-T Rec. Q.703 (07/96) 12.3, Timer T2 high.
         ITU-T Rec. Q.703 (1996).  ANSI T1.111.3/2000.  ETSI 300 008-1."
    DEFVAL      { 10000 }
    ::= { slProfileEntry 8 }

slProfileTimerT3 OBJECT-TYPE
    SYNTAX      TimeInterval (100..1400)
    UNITS       "seconds"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "MTP Level 2 T3 timer.
         
         Q.703/T1.111.3: Level 2 timer 'Aligned'.
         
         Timeout interval T3.  This is the time interval that a 
         signalling link will await an MSU or FISU after sending an MSU 
         or FISU at the end of the initial alignment procedure.
         
         For all signalling link tranmission rates, ITU-T specifies a 
         range of 1 to 2 seconds, with a provisional value of 1 second."
    REFERENCE   
        "ITU-T Rec. Q.751.1 (1997), ANSI T1.116.1 (2000), l2TimerT3 
         attribute.
         ITU-T Rec. Q.703 (1996).  ANSI T1.111.3/2000.  ETSI 300 008-1.
         ITU-T Rec. Q.703 (07/96) 12.3, Timer T3.
         ITU-T Rec. Q.703, ANSI T1.111.3, T3 timer."
    DEFVAL      { 100 }
    ::= { slProfileEntry 9 }

slProfileTimerT4N OBJECT-TYPE
    SYNTAX      TimeInterval (230..12000)
    UNITS       "seconds"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "MTP Level 2 T4n (normal) timer.
         
         Q.703/T1.111.3: Level 2 timer 'Proving period timer normal'.
         
         Timeout interval T4n.  This is the period for which a 
         signalling link will send continuous SIN during the initial 
         alignement procedure.  This is the interval of the 'normal' 
         proving period.
         
         ITU-T 4800            110s (100-120s) (2^16)
         ITU-T 64              8.2s (7.5-9.5s) (2^16)
         ITU-T 1.536/1.984Mbps 30s (3-70s) (2^16)
         ANSI  64/56           2.3s (+-10%) (2^14)
         ANSI  1.536           30s (+-10%)"
    REFERENCE   
        "ITU-T Rec. Q.751.1 (1997), ANSI T1.116.1 (2000), l2TimerT4N 
         attribute.
         ITU-T Rec. Q.703 (1996).  ANSI T1.111.3/2000.  ETSI 300 008-1.
         ITU-T Rec. Q.703, ANSI T1.111.3, T4n timer.
         ITU-T Rec. Q.703 (07/96) 12.3, Timer T4n(4.8) and T4n(64).
         ITU-T Rec. Q.703 (07/96) Annex A, A.12.3, Timer T4n."
    DEFVAL      { 800 }
    ::= { slProfileEntry 10 }

slProfileTimerT4E OBJECT-TYPE
    SYNTAX      TimeInterval (40..800)
    UNITS       "seconds"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "MTP Level 2 T4e (emergency) timer.
         
         Q.703/T1.111.3: Level 2 timer 'Proving period timer 
         emergency'.
         
         Timeout interval T4e.  This is the period for which a 
         signalling link will send continuous SIE during the initial 
         alignment procedure.  This is the interval of the 'emergency' 
         proving period.
         
         ITU-T 4800            7s (6-8s) (2^12)
         ITU-T 64              500ms (400-600ms) (2^12)
         ITU-T 1.536/1.984Mbps 400-600ms (2^12)
         ANSI  64/56           0.6s (+-10%) (2^12)
         ANSI  1.536           5s (+-10%)"
    REFERENCE   
        "ITU-T Rec. Q.751.1 (1997), ANSI T1.116.1 (2000), l2TimerT4E 
         attribute.
         ITU-T Rec. Q.703 (1996).  ANSI T1.111.3/2000.  ETSI 300 008-1.
         ITU-T Rec. Q.703, ANSI T1.111.3, T4e timer.
         ITU-T Rec. Q.703 (07/96) 12.3, Timer T4e(4.8) and T4e(64).
         ITU-T Rec. Q.703 (07/96) Annex A, A.12.3, Timer T4n."
    DEFVAL      { 50 }
    ::= { slProfileEntry 11 }

slProfileTimerT5 OBJECT-TYPE
    SYNTAX      TimeInterval (8..12)
    UNITS       "seconds"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "MTP Level 2 T5 timer.
         
         Q.703/T1.111.3: Level 2 timer 'Sending SIB'.
         
         Timeout interval T5.  This time interval defines the frequency 
         at which a signalling link will sent SIB when it is 
         experiencing receive congestion.
         
         For all transmission rates, ITU-T specifies a range of 80 to 
         120 milliseconds with a provisional value of 100 milliseconds."
    REFERENCE   
        "ITU-T Rec. Q.751.1 (1997), ANSI T1.116.1 (2000), l2TimerT5 
         attribute.
         ITU-T Rec. Q.703 (1996).  ANSI T1.111.3/2000.  ETSI 300 008-1.
         ITU-T Rec. Q.703 (07/96) 12.3, Timer T5.
         ITU-T Rec. Q.703, ANSI T1.111.3, T5 timer."
    DEFVAL      { 10 }
    ::= { slProfileEntry 12 }

slProfileTimerT6 OBJECT-TYPE
    SYNTAX      TimeInterval (300..1200)
    UNITS       "seconds"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "MTP Level 2 T6 timer.
         
         Q.703/T1.111.3: Level 2 timer 'Remote congestion'.
         
         Timeout interval T6.  This interval is the maximum amount of 
         time that a signalling link will await an acknowledgement for a 
         previously sent signalling unit, when the remote signalling 
         link is busy, before failing the signalling link.
         
         For modems, ITU-T specifies a range of 8 to 12 seconds, with a 
         provisional value of 10 seconds.  For ds0 and e1, ITU-T 
         specifies a range of 3 to 6 seconds, with a provisional value 
         of 4 seconds."
    REFERENCE   
        "ITU-T Rec. Q.751.1 (1997), ANSI T1.116.1 (2000), l2TimerT6 
         attribute.
         ITU-T Rec. Q.703 (1996).  ANSI T1.111.3/2000.  ETSI 300 008-1.
         ITU-T Rec. Q.703 (07/96) 12.3, Timer T6(4.8) and T6(64).
         ITU-T Rec. Q.703, ANSI T1.111.3, T6 timer."
    DEFVAL      { 400 }
    ::= { slProfileEntry 13 }

slProfileTimerT7 OBJECT-TYPE
    SYNTAX      TimeInterval (50..600)
    UNITS       "seconds"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "MTP Level 2 T7 timer.
         
         Q.703/T1.111.3: Level 2 timer 'Excessive delay of 
         acknowledgement'.
         
         Timeout interval T7.  This interval is the maximum amount of 
         time that a signalling link will await an acknowledgement for a 
         previously sent signalling unit, when the remote signalling 
         link is not busy, before failing the signalling link.
         
         For modems, ITU-T specifies a range of 4 to 6 seconds, with a 
         provisional value of 5 seconds.  For ds0 and e1, ITU-T 
         specifies a range of 0.5 to 2 seconds, with a provisional value 
         of 1 second."
    REFERENCE   
        "ITU-T Rec. Q.751.1 (1997), ANSI T1.116.1 (2000), l2TimerT7 
         attribute.
         ITU-T Rec. Q.703 (1996).  ANSI T1.111.3/2000.  ETSI 300 008-1.
         ITU-T Rec. Q.703 (07/96) 12.3, Timer T7(4.8) and T7(64).
         ITU-T Rec. Q.703, ANSI T1.111.3, T7 timer."
    DEFVAL      { 100 }
    ::= { slProfileEntry 14 }

slProfileTbOnset1 OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "MSUs or octets"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "If the number of MSUs in the transmission / retransmission 
         buffer gets bigger than this value, the respective link is 
         congested in congestionLevel 1.  CongestionLevel 1 measures 
         start.
         
         Transmit congestion onset threshold 1.  When the number of 
         octets in the transmission buffers surpasses this threshold, 
         MTP will issue congestion indications to the message originator 
         for congestion level 1."
    REFERENCE   
        "ITU-T Rec. Q.751.1 (1997), ANSI T1.116.1 (2000), 
         transmissionCongestionThresholdOnsetLevel1 attribute.
         ITU-T Rec. Q.703 (1996).  ANSI T1.111.3/2000.  ETSI 300 008-1."
    DEFVAL      { 69632 }
    ::= { slProfileEntry 15 }

slProfileTbAbate1 OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "MSUs or octets"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "If the number of MSUs in the transmission / retransmission 
         buffer gets smaller than this number, Level 2 flow control 
         measures end. The value has to be smaller than the respective 
         congestion onset threshold.
         
         Transmit congestion abate threshold 1.  When the number of 
         MSUs or octets in the transmission buffers falls beneath this 
         threshold, MTP will resume sending MSUs of message priority 0."
    REFERENCE   
        "ITU-T Rec. Q.751.1 (1997), ANSI T1.116.1 (2000), 
         transCongThreshAbatementLevel1 attribute.
         ITU-T Rec. Q.703 (1996).  ANSI T1.111.3/2000.  ETSI 300 008-1."
    DEFVAL      { 34816 }
    ::= { slProfileEntry 16 }

slProfileNumberOfThresholdLevels OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "This attribute equals the number of threshold levels that are 
         used for congestion control."
    REFERENCE   
        "ITU-T Rec. Q.751.1 (1997), ANSI T1.116.1 (2000), 
         numberOfThresholdLevels attribute."
    DEFVAL      { 1 }
    ::= { slProfileEntry 17 }

slProfileCongestionCounting OBJECT-TYPE
    SYNTAX      INTEGER { octet(0), message(1) }
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "This attribute indicates whether congestion counting is message 
         based or octet based."
    REFERENCE   
        "ITU-T Rec. Q.751.1 (1997), ANSI T1.116.1 (2000), 
         congestionCounting attribute."
    DEFVAL      { octet }
    ::= { slProfileEntry 18 }

slProfileCongestionRepBaseObject OBJECT-TYPE
    SYNTAX      INTEGER { routeSet(0), linkSetOfCongestedRouteSet(1), 
