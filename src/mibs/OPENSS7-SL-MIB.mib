    slSaalNniTimerT2                    TimeInterval,
    slSaalNniTimerT3                    TimeInterval,
    slSaalSscopTimerCc                  TimeInterval,
    slSaalSscopTimerIdle                TimeInterval,
    slSaalSscopTimerKeepAlive           TimeInterval,
    slSaalSscopTimerNoResponse          TimeInterval,
    slSaalSscopTimerPoll                TimeInterval,
    slSaalTransRateIntervalLower        Unsigned32,
    slSaalTransRateIntervalUpper        Unsigned32,
