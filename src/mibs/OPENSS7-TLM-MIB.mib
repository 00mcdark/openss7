    ::= { tlmConnectionIVMO 1 }

tlmConnectionIVMOEntry OBJECT-TYPE
    SYNTAX      TlmConnectionIVMOEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Transport connection initial value managed object.  
         There may be multiple instances of the 
         transportConnectionIVMO in a system.  
         A transportConnectionIVMO may be used to supply initial 
         values for the attributes of automatically created 
         transportConnection MOs.
         
         The values supplied in an IVMO may be overridden by 
         values supplied by an implementation-specific manner 
         across the service interface.
         
         Its definition permits it to be created and deleted 
         explicitly by management operation."
    REFERENCE   
        "ITU-T Rec. X.284 (1997), transportConnectionIVMO MO"
    INDEX       { tlmConnectionIVMOId }
    ::= { tlmConnectionIVMOTable 1 }

TlmConnectionIVMOEntry ::= SEQUENCE {
    tlmConnectionIVMOId                             SimpleNameType,
    tlmConnectionIVMOProtocolClasses                ProtocolClasses,
    tlmConnectionIVMOMaxTPDUSize                    Integer32,
-- transportConnectionIVMOClass1-P PACKAGE
    tlmConnectionIVMONetworkExpeditedData           TruthValue,
    tlmConnectionIVMOReceiptConfirmation            TruthValue,
    tlmConnectionIVMOReassignmentTime1              MilliSeconds,
    tlmConnectionIVMOTransportExpeditedService1     TruthValue,
-- transportConnectionIVMOClass2-P PACKAGE
    tlmConnectionIVMOExplicitFlowControl            TruthValue,
    tlmConnectionIVMOExtendedFormat2                TruthValue,
    tlmConnectionIVMOMaximumWindow2                 Integer32,
    tlmConnectionIVMOTransportExpeditedService2     TruthValue,
-- transportConnectionIVMOClass3-P PACKAGE
    tlmConnectionIVMOExtendedFormat3                TruthValue,
    tlmConnectionIVMOReassignmentTime3              MilliSeconds,
    tlmConnectionIVMOMaximumWindow3                 Integer32,
    tlmConnectionIVMOTransportExpeditedService3     TruthValue,
-- transportConnectionIVMOClass4-P PACKAGE
    tlmConnectionIVMOChecksumNonuse                 TruthValue,
    tlmConnectionIVMOExtendedFormat4                TruthValue,
    tlmConnectionIVMOInactivityTime                 MilliSeconds,
    tlmConnectionIVMOMaxTransmissions               Integer32,
    tlmConnectionIVMORetransmissionTime             MilliSeconds,
    tlmConnectionIVMOWindowTimer                    MilliSeconds,
    tlmConnectionIVMOMaximumWindow4                 Integer32,
    tlmConnectionIVMOTransportExpeditedService4     TruthValue,
    tlmConnectionIVMORowStatus                      RowStatus
}

tlmConnectionIVMOId OBJECT-TYPE
    SYNTAX      SimpleNameType
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Name for this instance of initial values applicable to 
         automatic creation of transport connections.  There is 
         always a row named 'Default'."
    REFERENCE   
        "ITU-T Rec. X.284 (1997), transportConnectionIVMOId"
    DEFVAL      { "Default" }
    ::= { tlmConnectionIVMOEntry 1 }

tlmConnectionIVMOProtocolClasses OBJECT-TYPE
    SYNTAX      ProtocolClasses
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "The preferred/alternate set of protocol classes that 
         may be stated at connection establishment.  The default 
         value is implementation-dependent, but must be 
         consistent with the class negotiation rules of ITU-T 
         Rec. X.224 | ISO/IEC 8073."
    REFERENCE   
        "ITU-T Rec. X.284 (1997), protocolClasses"
    DEFVAL      { {class0, class1, class2, class3, class4} }
    ::= { tlmConnectionIVMOEntry 2 }

tlmConnectionIVMOMaxTPDUSize OBJECT-TYPE
    SYNTAX      Integer32 (0 | 128 | 256 | 512 | 1024 | 2048 | 4096 | 8192)
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "The maximum TPDU size negotiated for the connection.  
         Implementation constraints or policies, or relating to 
         the remote NSAP or transport entity, may cause 
         a smaller value to be used as an initial value for 
         negotiation.  During connection establishment, this 
         attribute represents the desired value and not 
         necessarily the value that will ultimatedly be used."
    REFERENCE   
        "ITU-T Rec. X.284 (1997), maxTPDUSize"
    DEFVAL      { 1024 }
    ::= { tlmConnectionIVMOEntry 3 }

-- transportConnectionIVMOClass1-P PACKAGE
tlmConnectionIVMONetworkExpeditedData OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "Enables negotiation/indicates use of Network Expedited 
         Data.  Enables the negotiation of use or non-use of 
         Network Expedited Data (for Class 1 only) during 
         transport connection establishment.  true(1) enables 
         use of Network Expedited Data, false(2) disables it."
    REFERENCE   
        "ITU-T Rec. X.284 (1997), networkExpeditedData"
    DEFVAL      { false }
    ::= { tlmConnectionIVMOEntry 4 }

tlmConnectionIVMOReceiptConfirmation OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "Enables negotiation/indicates use of receipt 
         confirmation.  Enables negotiation of use of network 
         receipt confirmation (Class 1 only) during transport 
         connection establishment.  true(1) enables use, 
         false(2) enables non-use."
    REFERENCE   
        "ITU-T Rec. X.284 (1997), receiptConfirmation"
    DEFVAL      { false }
    ::= { tlmConnectionIVMOEntry 5 }

tlmConnectionIVMOReassignmentTime1 OBJECT-TYPE
    SYNTAX      MilliSeconds
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "The value of the Reassignment Time (as defined in ITU-T 
         Rec. X.224 | ISO/IEC 8073) to be conveyed or 
         established during connection establishment.  This 
         value applies only to protocol Class 1 connections."
    REFERENCE   
        "ITU-T Rec. X.284 (1997), reassignmentTime"
    ::= { tlmConnectionIVMOEntry 6 }

tlmConnectionIVMOTransportExpeditedService1 OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "Indicates whether Transport Expedited Service is 
         provided, true(1), or not, false(1), for protocol Class 
         1 transport connections."
    REFERENCE   
        "ITU-T Rec. X.284 (1997), transportExpeditedService"
    DEFVAL      { true }
    ::= { tlmConnectionIVMOEntry 7 }

-- transportConnectionIVMOClass2-P PACKAGE
tlmConnectionIVMOExplicitFlowControl OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "Enables negotiation/indicates use of explicit Flow 
         Control.  Enables negotiation of non-use of explicit 
         flow control (Class 2 only) during connection 
         establishment.  true(1) enables non-use, false(2) 
         disables it."
    REFERENCE   
        "ITU-T Rec. X.284 (1997), explicitFlowControl"
    ::= { tlmConnectionIVMOEntry 8 }

tlmConnectionIVMOExtendedFormat2 OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "Enables negotiation/indicates use of extended TPDU 
         format.  Enables negotiation of extended TPDU format 
         (for protocol Class 2) during transport connection 
         establishment.  true(1) enables use of extended format, 
         false(2) disables it."
    REFERENCE   
        "ITU-T Rec. X.284 (1997), extendedFormat"
    ::= { tlmConnectionIVMOEntry 9 }

tlmConnectionIVMOMaximumWindow2 OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "The maximum window permitted to be given on the 
         connection at any time.  Buffering or other 
         implementation constraints or policies may cause 
         a smaller value to be used.  This value applies only to 
         protocol Class 2 connections."
    REFERENCE   
        "ITU-T Rec. X.284 (1997), maximumWindow"
    ::= { tlmConnectionIVMOEntry 10 }

tlmConnectionIVMOTransportExpeditedService2 OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "Indicates whether Transport Expedited Service is 
         provided, true(1), or not, false(1), for protocol Class 
         2 transport connections."
    REFERENCE   
        "ITU-T Rec. X.284 (1997), transportExpeditedService"
    DEFVAL      { true }
    ::= { tlmConnectionIVMOEntry 11 }

-- transportConnectionIVMOClass3-P PACKAGE
tlmConnectionIVMOExtendedFormat3 OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "Enables negotiation/indicates use of extended TPDU 
         format.  Enables negotiation of extended TPDU format 
         (for protocol Class 3) during transport connection 
         establishment.  true(1) enables use of extended format, 
         false(2) disables it."
    REFERENCE   
        "ITU-T Rec. X.284 (1997), extendedFormat"
    ::= { tlmConnectionIVMOEntry 12 }

tlmConnectionIVMOReassignmentTime3 OBJECT-TYPE
    SYNTAX      MilliSeconds
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "The value of the Reassignment Time (as defined in ITU-T 
         Rec. X.224 | ISO/IEC 8073) to be conveyed or 
         established during connection establishment.  This 
         value applies only to protocol Class 3 connections."
    REFERENCE   
        "ITU-T Rec. X.284 (1997), reassignmentTime"
    ::= { tlmConnectionIVMOEntry 13 }

tlmConnectionIVMOMaximumWindow3 OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "The maximum window permitted to be given on the 
         connection at any time.  Buffering or other 
         implementation constraints or policies may cause 
         a smaller value to be used.  This value applies only to 
         protocol Class 3 connections."
    REFERENCE   
        "ITU-T Rec. X.284 (1997), maximumWindow"
    ::= { tlmConnectionIVMOEntry 14 }

tlmConnectionIVMOTransportExpeditedService3 OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "Indicates whether Transport Expedited Service is 
         provided, true(1), or not, false(1), for protocol Class 
         3 transport connections."
    REFERENCE   
        "ITU-T Rec. X.284 (1997), transportExpeditedService"
    ::= { tlmConnectionIVMOEntry 15 }

-- transportConnectionIVMOClass4-P PACKAGE
tlmConnectionIVMOChecksumNonuse OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "Enables negotiation/indicates non-use of checksum.  
         Enables negotiation of checksum non-use (Class 4 only) 
         during connection establishment.  true(1) enables 
         non-use, false(2) disables it."
    REFERENCE   
        "ITU-T Rec. X.284 (1997), checksumNonuse"
    ::= { tlmConnectionIVMOEntry 16 }

tlmConnectionIVMOExtendedFormat4 OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "Enables negotiation/indicates use of extended TPDU 
         format.  Enables negotiation of extended TPDU format 
         (for protocol Class 4) during transport connection 
         establishment.  true(1) enables use of extended format, 
         false(2) disables it."
    REFERENCE   
        "ITU-T Rec. X.284 (1997), extendedFormat"
    ::= { tlmConnectionIVMOEntry 17 }

tlmConnectionIVMOInactivityTime OBJECT-TYPE
    SYNTAX      MilliSeconds
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "Value of the inactivity time (as defined in ITU-T Rec. 
         X.224 | ISO/IEC 8073) in use for the connection.  ITU-T 
         Rec. X.284 unit is in seconds.  This value is 
         applicable to Class 4 only."
    REFERENCE   
        "ITU-T Rec. X.284 (1997), inactivityTime"
    DEFVAL      { 400000 } -- 6 minutes, 40 seconds
    ::= { tlmConnectionIVMOEntry 18 }

tlmConnectionIVMOMaxTransmissions OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "The maximum number of transmissions as defined (for 
         Class 4 only) as the parameter N in ITU-T Rec. X.224 
         | ISO/IEC 8073."
    REFERENCE   
        "ITU-T Rec. X.284 (1997), maxTransmissions"
    ::= { tlmConnectionIVMOEntry 19 }

tlmConnectionIVMORetransmissionTime OBJECT-TYPE
    SYNTAX      MilliSeconds
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "Initial or current value for the Local Retransmission 
         Time as defined  (for Class 4 only) in ITU-T Rec. X.224 
         | ISO/IEC 8073.  Another value may be adopted initially 
         based on knowledge concerning the remote system.  The 
         current value may change during the lifetime of the 
         connection, based on observations of traffic on the 
         connection or other information concerning the remote 
         Transport Entity.  The value of this attribute is used 
         in the absence of other information.  ITU-T Rec. X.284 
         unit is in seconds."
    REFERENCE   
        "ITU-T Rec. X.284 (1997), retransmissionTime"
    DEFVAL      { 2000 } -- 2 seconds
    ::= { tlmConnectionIVMOEntry 20 }

tlmConnectionIVMOWindowTimer OBJECT-TYPE
    SYNTAX      MilliSeconds
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "Value of Window Timer as defined in ITU-T Rec. X.224 
         | ISO/IEC 8073.  The value of this attribute is 
         meaningful only for connections using Class 4 of the 
         protocol.  For other connections no constraint is 
         placed on the value to be returned.  ITU-T Rec. X.284 
         unit is in seconds."
    REFERENCE   
        "ITU-T Rec. X.284 (1997), windowTimer"
    DEFVAL      { 120000 }
    ::= { tlmConnectionIVMOEntry 21 }

tlmConnectionIVMOMaximumWindow4 OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "The maximum window permitted to be given on the 
         connection at any time.  Buffering or other 
         implementation constraints or policies may cause 
         a smaller value to be used.  This value applies only to 
         protocol Class 4 connections."
    REFERENCE   
        "ITU-T Rec. X.284 (1997), maximumWindow"
    ::= { tlmConnectionIVMOEntry 22 }

tlmConnectionIVMOTransportExpeditedService4 OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "Indicates whether Transport Expedited Service is 
         provided, true(1), or not, false(1), for protocol Class 
         4 transport connections."
    REFERENCE   
        "ITU-T Rec. X.284 (1997), transportExpeditedService"
    ::= { tlmConnectionIVMOEntry 23 }

tlmConnectionIVMORowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "Provides a mechanism whereby management stations may 
         create and destroy rows in this table.  The default row 
         named 'Default' is always present and cannot be 
         deleted.  Rows must persist across management system 
         restart/reboots."
    REFERENCE   
        "ITU-T Rec. X.284 (1997), 
               transportConnectionIVMO-comodeTPM NAME BINDING"
    ::= { tlmConnectionIVMOEntry 24 }

tlmTSAP OBJECT IDENTIFIER
    ::= { tlmTables 7 }

tlmTSAPTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF TlmTSAPEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "TSAP managed object class."
    REFERENCE   
        "ITU-T Rec. X.284 (1997): tSAP MOC"
    ::= { tlmTSAP 1 }

tlmTSAPEntry OBJECT-TYPE
    SYNTAX      TlmTSAPEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "TSAP managed object: there is one tSAP MO for each TSAP 
         currently recognized by the containing Transport 
         Entity.  Its definition permits it to be created and 
         deleted by Management operation or to be created and 
         deleted automatically as part of system operation.  
         Some implementations may required TSAP MOs to be 
         created explicitly through management before they can 
         be used.  Others may create them automatically when 
         a user entity attaches itself to them (in some 
         implementation-dependent fashion).  In this case, it is 
         recommended that the naming convention be used whereby 
         the name of the Managed Object is the representation in 
         hexadecimal of the Transport Selector of the TSAP, so 
         that it is possible to configure Transport Users 
         without system specific knowledge."
    REFERENCE   
        "ITU-T Rec. X.284 (1997): tSAP MO"
    INDEX       { tlmTSAPSapId }
    ::= { tlmTSAPTable 1 }

TlmTSAPEntry ::= SEQUENCE {
    tlmTSAPSapId            SimpleNameType,
    tlmTSAPSap1Address      Integer32,
    tlmTSAPUserEntityNames  SnmpAdminString,
    tlmTSAPRowStatus        RowStatus
}

tlmTSAPSapId OBJECT-TYPE
    SYNTAX      SimpleNameType
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "If the object is created automatically, it is 
         recommended that the naming convention be used whereby 
         the name of the MO is the representation in Hexadecimal 
         of the Transport Selector of the TSAP.  If the package 
         is created by a management station, the initial value 
         shall be specified during creation."
    REFERENCE   
        "ITU-T Rec. X.723 (1993) | ISO/IEC 10165-5 : 1993, sapId 
               ATTRIBUTE"
    ::= { tlmTSAPEntry 1 }

tlmTSAPSap1Address OBJECT-TYPE
    SYNTAX      Integer32 (0..65535)
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "If the object is created automatically, it is 
         recommended that the naming convention be used whereby 
         the name of the MO is the representation in Hexadecimal 
         of the Transport Selector of the TSAP.  If the package 
         is created by a management station, the initial value 
         shall be specified during creation."
    REFERENCE   
        "ITU-T Rec. X.284 (1997), tSAPAddress ATTRIBUTE"
    ::= { tlmTSAPEntry 2 }

tlmTSAPUserEntityNames OBJECT-TYPE
    SYNTAX      SnmpAdminString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "This should really be a side-table..."
    REFERENCE   
        "ITU-T Rec. X.723 (1993) | ISO/IEC 10165-5 : 1993, 
               userEntityNames ATTRIBUTE"
    ::= { tlmTSAPEntry 3 }

tlmTSAPRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "This Name Binding corresponds to the use of TSAPs that 
         are explicitly created by management.  The value of the 
         tsapID attribute shall be included int he Create 
         operation, otherwise the create operation will fail."
    REFERENCE   
        "ITU-T Rec. X.284 (1997), 
               tSAP-transportEntity-Management NAME BINDING"
    ::= { tlmTSAPEntry 4 }

