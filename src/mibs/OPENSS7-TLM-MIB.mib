--  tlmConnectionExtendedFormat                 TruthValue,
    tlmConnectionInactivityTime                 MilliSeconds,
    tlmConnectionMaxTransmissions               Integer32,
    tlmConnectionRetransmissionTime             MilliSeconds,
--  tlmConnectionTransportExpeditedService      TruthValue,
    tlmConnectionWindowTimer                    MilliSeconds,
-- transportConnectionNCMS-P PACKAGE
    tlmConnectionRelatingNCCMONames             SnmpAdminString
}

