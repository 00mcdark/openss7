    REFERENCE   
        "ITU-T Rec. X.284 (1997), checksumNonuse"
    ::= { tlmConnectionEntry 29 }

--  tlmConnectionExtendedFormat                 TruthValue
tlmConnectionInactivityTime OBJECT-TYPE
    SYNTAX      MilliSeconds
