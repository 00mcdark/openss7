    tlmConnectionPdusReceivedCounter            Counter64,
    tlmConnectionPdusRetransmittedErrorCounter  Counter64,
    tlmConnectionPdusSentCounter                Counter64,
    tlmConnectionCalledNSAPAddress              NSAPAddress,
    tlmConnectionCalledTSelector                TSelector,
    tlmConnectionCallingNSAPAddress             NSAPAddress,
    tlmConnectionCallingTSelector               TSelector,
    tlmConnectionConnectionDirection            ConnectionDirection,
    tlmConnectionLocalReference                 TReference,
    tlmConnectionMaxTPDUSize                    Integer32,
    tlmConnectionNetworkConnectionIDs           NetworkConnectionIDs,
    tlmConnectionProtocolClass                  ProtocolClass,
    tlmConnectionProtocolErrors                 Counter64,
    tlmConnectionRemoteReference                TReference,
    tlmConnectionRespondingNSAPAddress          NSAPAddress,
-- transportConnectionClass1-P PACKAGE
    tlmConnectionNetworkExpeditedData           TruthValue,
    tlmConnectionReassignmentsAfterFailure      Counter64,
    tlmConnectionReassignmentTime               MilliSeconds,
    tlmConnectionReceiptConfirmation            TruthValue,
    tlmConnectionTransportExpeditedService      TruthValue,
-- transportConnectionClass2-P PACKAGE
    tlmConnectionExtendedFormat                 TruthValue,
    tlmConnectionExplicitFlowControl            TruthValue,
--  tlmConnectionTransportExpeditedService      TruthValue,
-- transportConnectionClass3-P PACKAGE
--  tlmConnectionExtendedFormat                 TruthValue,
--  tlmConnectionReassignmentTime               MilliSeconds,
--  tlmConnectionReassignmentsAfterFailure      Counter64,
--  tlmConnectionTransportExpeditedService      TruthValue,
