    ::= { openss7X 284 }

 -- top level organization of the MIB

-- tlmNotifications    OBJECT IDENTIFIER ::= { tlmMIB         0 }
-- tlmObjects          OBJECT IDENTIFIER ::= { tlmMIB         1 }
-- tlmConformance      OBJECT IDENTIFIER ::= { tlmMIB         2 }

-- tlmTables           OBJECT IDENTIFIER ::= { tlmObjects     1 }
-- tlmScalars          OBJECT IDENTIFIER ::= { tlmObjects     2 }
-- tlmNotifs           OBJECT IDENTIFIER ::= { tlmObjects     3 }
-- tlmOids             OBJECT IDENTIFIER ::= { tlmObjects     4 }

-- tlmCompliances      OBJECT IDENTIFIER ::= { tlmConformance 1 }
-- tlmGroups           OBJECT IDENTIFIER ::= { tlmConformance 2 }

ConnectionDirection ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION 
        "Indicates the direction of the connection.  The value 
         incoming(1) means that it was initiated by the remote 
         Transport Entity, the value outgoing(0) means the it 
         was initiated by the local Transport Entity."
    REFERENCE   
        "ITU-T Rec. X.284 (1997): TLM.ConnectionDirectionSyntax"
    SYNTAX      INTEGER { outgoing(0), incoming(1) }

DeletionCause ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION 
        "Reason why the Transport Connection Object is being 
         deleted."
    REFERENCE   
        "ITU-T Rec. X.284 (1997): TLM.DelettionCauseSyntax"
    SYNTAX      INTEGER { protocolError(0), networkServiceProvider(1), 
                    remoteCongestion(3), localCongestion(4) }

NCCOL ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION 
        "Indicates the collision algorithm as defined in Annex 
         B of ITU-T Rec. X.224 | ISO/IEC 8073.  In NCCIV managed 
         object, indicates the collision algorithm to be used.  
         In NCC managed object, indicates the collision 
         algorithm in use."
    REFERENCE   
        "ITU-T Rec. X.284 (1997): TLM.NC-COLSyntax"
    SYNTAX      INTEGER { nc-COL0(0) }

NCPREF ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION 
        "Indicates the preference the initiator has to keep the 
         network connection as defined in Annex B or ITU-T Rec. 
         X.224 | ISO/IEC 8073.  In NCCIV managed object, 
         indicates the preference to be used.  In NCC managed 
         object, indicates the preference in use."
    REFERENCE   
        "ITU-T Rec. X.284 (1997): TLM.NC-PREFSyntax"
    SYNTAX      INTEGER { highest(0), medium(1), lowest(3) }

NCREC ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION 
        "Indicates the recovery optimization option as defined 
         in Annex B of ITU-T Rec. X.224 | ISO/IEC 8073.  In 
         NCCIV managed object, indicates the recovery 
         optimization option to be used.  In NCC managed object, 
         indicates the recover optimization option in use."
    REFERENCE   
        "ITU-T Rec. X.284 (1997): TLM.NC-RECSyntax"
    SYNTAX      INTEGER { pleaseDoNotRecover(0), pleaseRecover(1) }

NCREF ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION 
        "Indicates the nc-reference as defined in Annex B of 
         ITU-T Rec. X.224 | ISO/IEC 8073."
    REFERENCE   
        "ITU-T Rec. X.284 (1997): TLM.NC-REFSyntax"
    SYNTAX      Integer32

NCRight ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION 
        "Indicates the type of right of use as defined in Annex 
         B of ITU-T Rec. X.224 | ISO/IEC 8073.  In NCCIV managed 
         object, indicates the type of right of use to be used.  
         Namely, the value my-side(1) means 'SA', remote-side(2) 
         means 'RA' and both-sides(3) means 'RR'.  In NCC 
         managed object, indicates the type of right of use in 
         use."
    REFERENCE   
        "ITU-T Rec. X.284 (1997): TLM.NC-RightSyntax"
    SYNTAX      INTEGER { my-side(1), remote-side(2), both-sides(3) }

ProtocolClass ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION 
        "The protocol class in used on the connection, as 
         negotiated during connection establishment.  During 
         connection establishment (before the connection reaches 
         the OPEN state) this indicates what is currently 
         preferred and not necessarily the ultimate class that 
         will be used for the connection."
    REFERENCE   
        "ITU-T Rec. X.284 (1997): TLM.ProtocolClassSyntax"
    SYNTAX      INTEGER { class0(0), class1(1), class2(2), class3(3), 
                    class4(4) }

ProtocolClasses ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION 
        "The preferred/alternate set of protocol classes that 
         may be stated at connection establishment.  The default 
         value is implementation dependent, but must be 
         consistent with the class negotiation rules of ITU-T 
         Rec. X.224 | ISO/IEC 8073."
    REFERENCE   
        "ITU-T Rec. X.284 (1997): TLM.ProtocolClassesSyntax"
    SYNTAX      BITS { class0(0), class1(1), class2(2), class3(3), 
                    class4(4) }

PDUHeader ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION 
        "Header of a PDU causing an event.  Returned in the 
         problemData field of a communicationsAlarm 
         notification."
    REFERENCE   
        "ITU-T Rec. X.284 (1997): TLM.PDUHeaderSyntax"
    SYNTAX      OCTET STRING (SIZE(1..255))

ReasonCode ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION 
        "Reason why the PDU is in error as placed in the 
         corresponding parameter of the ER TPDU.  Returned in 
         the problemData field of a communicationsAlarm 
         notification."
    REFERENCE   
        "ITU-T Rec. X.284 (1997): TLM.ReasonCodeSyntax"
    SYNTAX      Integer32

SourceAddress ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION 
        "Source N-Address.  Returned in the problemData field 
         of a communicationsAlarm notification."
    REFERENCE   
        "ITU-T Rec. X.284 (1997): TLM.SourceAddressSyntax"
    SYNTAX      OCTET STRING

SourceOfAllocation ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION 
        "Indicates the transport entity that established the 
         network connection at the first time during the life 
         time of an NC reference."
    REFERENCE   
        "ITU-T Rec. X.284 (1997): TLM.SourceOfAllocationSyntax"
    SYNTAX      INTEGER { local(0), remote(1) }

-- FIXME: this should be imported from OPENSS7-NLM-MIB

NSAPAddress ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION 
        ""
    SYNTAX      OCTET STRING (SIZE(0..20))

TSelector ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION 
        ""
    SYNTAX      OCTET STRING (SIZE(0 | 2))

TReference ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION 
        ""
    SYNTAX      Integer32 (0..65535)

NetworkConnectionIDs ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION 
        ""
    SYNTAX      OCTET STRING (SIZE(0..256))

-- tlmTransportSusbsytem (X.723:subsystem)
-- tlmEntity (X.723:communicationsEntity)
-- tlmClModeTPM (X.723:clProtocolMachine)
-- tlmCoModeTPM (X.723:coProtocolMachine)
-- tlmConnection (X.723:singlePeerConnection)
-- tlmConnectionIVMO (initial values)
-- tlmTSAP (X.723:sap1)
-- tlmNcmsPM
-- tlmNcc
-- tlmNccIVMO (initial values)

-- tlmSubsystem       OBJECT IDENTIFIER ::= { tlmTables  1 }
-- tlmEntity          OBJECT IDENTIFIER ::= { tlmTables  2 }
-- tlmClModeTPM       OBJECT IDENTIFIER ::= { tlmTables  3 }
-- tlmCoModeTPM       OBJECT IDENTIFIER ::= { tlmTables  4 }
-- tlmConnection      OBJECT IDENTIFIER ::= { tlmTables  5 }
-- tlmConnectionIVMO  OBJECT IDENTIFIER ::= { tlmTables  6 }
-- tlmTSAP            OBJECT IDENTIFIER ::= { tlmTables  7 }
-- tlmNcmsPM          OBJECT IDENTIFIER ::= { tlmTables  8 }
-- tlmNcc             OBJECT IDENTIFIER ::= { tlmTables  9 }
-- tlmNccIVMO         OBJECT IDENTIFIER ::= { tlmTables 10 }

tlmNotifications OBJECT IDENTIFIER
    ::= { tlmMIB 0 }

tlmObjects OBJECT IDENTIFIER
    ::= { tlmMIB 1 }

tlmTables OBJECT IDENTIFIER
    ::= { tlmObjects 1 }

tlmSubsystem OBJECT IDENTIFIER
    ::= { tlmTables 1 }

tlmSubsystemTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF TlmSubsystemEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        ""
    REFERENCE   
        "ITU-T Rec. X.284 (1997), transportSubsystem MOC"
    ::= { tlmSubsystem 1 }

tlmSubsystemEntry OBJECT-TYPE
    SYNTAX      TlmSubsystemEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Transport subsystem managed object.  Managed object for 
         transport layer subsystem.  There is exactly one of 
         these MOs within a system.  It exists to provide 
         a container for the layer entity MOs.
         
         The transportSubsystem managed object cannot be created 
         or deleted explicitly by management operation.  It 
         exists inherently in a system; created and deleted as 
         part of system operation."
    REFERENCE   
        "ITU-T Rec. X.284 (1997), transportSubsystem MO"
    INDEX       { tlmSubsystemId }
    ::= { tlmSubsystemTable 1 }

TlmSubsystemEntry ::= SEQUENCE {
    tlmSubsystemId  SnmpAdminString
}

tlmSubsystemId OBJECT-TYPE
    SYNTAX      SnmpAdminString
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        ""
    REFERENCE   
        "ITU-T Rec. X.723 (1993) : ISO/IEC 10165-5 : 1993, 
               subsystemId"
    DEFVAL      { "TransportSubsystem" }
    ::= { tlmSubsystemEntry 1 }

tlmEntity OBJECT IDENTIFIER
    ::= { tlmTables 2 }

tlmEntityTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF TlmEntityEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        ""
    REFERENCE   
        "ITU-T Rec. X.284 (1997), transportEntity MOC"
    ::= { tlmEntity 1 }

tlmEntityEntry OBJECT-TYPE
    SYNTAX      TlmEntityEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "There may be multiple instances of these MOs within 
         a system.  Its definition permits it to be deleted and 
         created explicitly by management operation, or to be 
         created and deleted automatically as part of system 
         operation."
    REFERENCE   
        "ITU-T Rec. X.284 (1997), transportEntity MO"
    INDEX       { tlmEntityId }
    ::= { tlmEntityTable 1 }

TlmEntityEntry ::= SEQUENCE {
    tlmEntityId                         SimpleNameType,
    tlmEntityLocalSapNames              SnmpAdminString,
    tlmEntityOperationalState           OperationalState,
    tlmEntityActualNSAP                 RowPointer,
    tlmEntityChecksumErrorsDetected     Counter64,
    tlmEntityProtocolErrors             Counter64,
    tlmEntityTargetNSAP                 RowPointer,
    tlmEntityUndecodedNSDUs             Counter64,
    tlmEntityRowStatus                  RowStatus
}

tlmEntityId OBJECT-TYPE
    SYNTAX      SimpleNameType
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Provides a simple name for the communications entity as 
         an index into the tlmEntityTable."
    REFERENCE   
        "ITU-T Rec. X.723 (1993) : ISO/IEC 10165-5 : 1993, 
               communicationsEntityId"
    ::= { tlmEntityEntry 1 }

tlmEntityLocalSapNames OBJECT-TYPE
    SYNTAX      SnmpAdminString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Provides a list of local SAP names provided by the 
         transport entity."
    REFERENCE   
        "ITU-T Rec. X.723 (1993) : ISO/IEC 10165-5 : 1993, 
               localSapNames"
    ::= { tlmEntityEntry 2 }

tlmEntityOperationalState OBJECT-TYPE
    SYNTAX      OperationalState
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "Provides the operational state of the transport entity 
         in accordance with X.721 and the OperationalState 
         textual convention of the OPENSS7-SMI-MIB."
    REFERENCE   
        "ITU-T Rec. X.721 (1992) | ISO/IEC 10165-2 : 1992, 
               operationalState"
    ::= { tlmEntityEntry 3 }

tlmEntityActualNSAP OBJECT-TYPE
    SYNTAX      RowPointer
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "The actual MO name(s) of the NSAP(s) in use by this 
         transport entity."
    REFERENCE   
        "ITU-T Rec. X.284 (1997), actualNSAP"
    ::= { tlmEntityEntry 4 }

tlmEntityChecksumErrorsDetected OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "The number of PDUs received by this transport entity 
         with an incorrect checksum."
    REFERENCE   
        "ITU-T Rec. X.284 (1997), checksumErrorsDetected"
    ::= { tlmEntityEntry 5 }

tlmEntityProtocolErrors OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Counter associated to protocol errors."
    REFERENCE   
        "ITU-T Rec. X.284 (1997), protocolErrors"
    ::= { tlmEntityEntry 6 }

tlmEntityTargetNSAP OBJECT-TYPE
    SYNTAX      RowPointer
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "The MO name(s) of the NSAP(s) to be used by this 
         transport entity.  The value of this attribute cannot 
         be changed unless the operational state of the entity 
         is Off.  An implementation may permit it to be set only 
         at creation of the transport entity MO.  An 
         implementation may permit the size of the set to be 
         restricted to 1.  An implementation may permit a null 
         value (empty set) to be specified, in which case some 
         system dependent autoconfiguration takes place."
    REFERENCE   
        "ITU-T Rec. X.284 (1997), targetNSAP"
    ::= { tlmEntityEntry 7 }

tlmEntityUndecodedNSDUs OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Number of (valid) NSDUs that cannot be attributed to 
         any protocol machines."
    REFERENCE   
        "ITU-T Rec. X.284 (1997), undecodedNSDUs"
    ::= { tlmEntityEntry 8 }

tlmEntityRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "Provides a mechanism whereby management stations can 
         create or delete conceptual rows in this table.  An 
         implementation might not permit more than one row to be 
         created.  Other implementations might not permit the 
         last row to be deleted.  Not all implementations need 
         support the addition or deletion of rows in this table.  
         Rows in this table must persist across management 
         system restart/reboot."
    REFERENCE   
        "ITU-T Rec. X.284 (1997), 
               transportEntity-transportSubsystem-Management 
               NAME BINDING"
    ::= { tlmEntityEntry 9 }

tlmClModeTPM OBJECT IDENTIFIER
    ::= { tlmTables 3 }

tlmClModeTPMTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF TlmClModeTPMEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        ""
    REFERENCE   
        "ITU-T Rec. X.284 (1997): clmodeTPM MOC"
    ::= { tlmClModeTPM 1 }

tlmClModeTPMEntry OBJECT-TYPE
    SYNTAX      TlmClModeTPMEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Connectionless-mode transport protocol machine managed 
         object.  There is no more than one of these MOs per 
         Transport Entity.  Its definition permits it to be 
         created and deleted explicitly by management operation, 
         but in some systems it will exist inherently and 
         neither creation nor deletetion by management operation 
         will be possible.  Name bindings are defined for both 
         cases.
         
         When the protocol machine is operable, the 
         operationalState shall have the value enabled(); 
         otherwise, it shall have the value disabled().
         
         Transitions of operationalState shall be reported using 
         the stateChange notification.  An clmodeTPM MO may be 
         created in the enabled() operational state."
    REFERENCE   
        "ITU-T Rec. X.284 (1997): clmodeTPM MOC"
    INDEX       { tlmEntityId }
    ::= { tlmClModeTPMTable 1 }

TlmClModeTPMEntry ::= SEQUENCE {
    tlmClModeTPMClProtocolMachineId     SnmpAdminString,
    tlmClModeTPMOperationalState        OperationalState,
    tlmClModeTPMAdministrativeState     AdministrativeState,
    tlmClModeClChecksumOption           TruthValue,
    tlmClModeTPMTotalRemoteSAPs         Counter64,
    tlmClModeOctetsSentCounter          Counter64,
    tlmClModeOctetsReceivedCounter      Counter64,
    tlmClModePdusSentCounter            Counter64,
    tlmClModePdusReceivedCounter        Counter64,
    tlmClModeUndeliverablePDUsCounter   Counter64,
    tlmClModeRowStatus                  RowStatus
}

tlmClModeTPMClProtocolMachineId OBJECT-TYPE
    SYNTAX      SnmpAdminString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Provides the name of this connectionless protocol 
         machine.  Connectionless transport only has one 
         connectionless protocol machine per transport entity 
         and the name is always 'CLTPM'."
    REFERENCE   
        "ITU-T Rec. X.284 (1997), clProtocolMachineId"
    DEFVAL      { "CLTPM" }
    ::= { tlmClModeTPMEntry 1 }

tlmClModeTPMOperationalState OBJECT-TYPE
    SYNTAX      OperationalState
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "Provides the operational state of the connectionless 
         transport protocol machine."
    REFERENCE   
        "ITU-T Rec. X.721 (1992) | ISO/IEC 10165-2 : 1992, 
               operationalState"
    ::= { tlmClModeTPMEntry 2 }

tlmClModeTPMAdministrativeState OBJECT-TYPE
    SYNTAX      AdministrativeState
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "Provides the administrative state of the connectionless 
         transport protocol machine."
    REFERENCE   
        "ITU-T Rec. X.721 (1992) | ISO/IEC 10165-2 : 1992, 
               administrativeState"
    ::= { tlmClModeTPMEntry 3 }

tlmClModeClChecksumOption OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "Enables use of the checksum option in ITU-T Rec. X.234 
         | ISO/IEC 8602 PDUs (in the absense of over-riding 
         local controls)."
    REFERENCE   
        "ITU-T Rec. X.284 (1997), clChecksumOption ATTRIBUTE"
    ::= { tlmClModeTPMEntry 4 }

tlmClModeTPMTotalRemoteSAPs OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Counts the number of remote (N) SAPs that the 
         containing clProtocolMachine has communicated with.  
         Incremented by 1 every time communication with a remote 
         (N) SAP is achieved."
    REFERENCE   
        "ITU-T Rec. X.723 (1993) : ISO/IEC 10165-5 : 1993, 
               totalRemoteSAPs ATTRIBUTE"
    ::= { tlmClModeTPMEntry 5 }

tlmClModeOctetsSentCounter OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Specifies the total number of user data octets sent by 
         the connectionless transport protocol state machine."
    REFERENCE   
        "ITU-T Rec. X.721 (1992) | ISO/IEC 10165-2 : 1992, 
               octetsSentCounter"
    ::= { tlmClModeTPMEntry 6 }

tlmClModeOctetsReceivedCounter OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Specifies the total number of user data octets received 
         by the connectionless transport protocol state machine."
    REFERENCE   
        "ITU-T Rec. X.721 (1992) | ISO/IEC 10165-2 : 1992, 
               octetsReceivedCounter"
    ::= { tlmClModeTPMEntry 7 }

tlmClModePdusSentCounter OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Specifies the total number of PDUs sent by the 
         connectionless transport protocol state machine."
    REFERENCE   
        "ITU-T Rec. X.721 (1992) | ISO/IEC 10165-2 : 1992, 
               pdusSentCounter"
    ::= { tlmClModeTPMEntry 8 }

tlmClModePdusReceivedCounter OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Specifies the total number of PDUs received by the 
         connectionless transport protocol state machine."
    REFERENCE   
        "ITU-T Rec. X.721 (1992) | ISO/IEC 10165-2 : 1992, 
               pdusReceivedCounter"
    ::= { tlmClModeTPMEntry 9 }

tlmClModeUndeliverablePDUsCounter OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Counter associated with the notification as recommended 
