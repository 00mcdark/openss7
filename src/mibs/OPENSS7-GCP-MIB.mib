OPENSS7-GCP-MIB DEFINITIONS ::= BEGIN

IMPORTS
    InetAddressType, InetAddress
        FROM INET-ADDRESS-MIB
    openss7Modules
        FROM OPENSS7-SMI
    AdministrativeState, AlarmStatus, AvailabilityStatus, 
    OperationalState, ProceduralStatus, SimpleNameType, StandbyStatus, 
    UsageState, attributeValueChangeNotificationPackage, 
    communicationsAlarmPackage, createDeleteNotificationsPackage, 
    tmnObjectInstance, tmnPerceivedSeverity, tmnProbableCause
        FROM OPENSS7-SMI-MIB
