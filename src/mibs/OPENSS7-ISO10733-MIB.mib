cLNSManagedObjectCompliance MODULE-COMPLIANCE
    STATUS      current
    DESCRIPTION
        "Complance for the cLNS managed object and corresponding 
         cLNSTable."
    MODULE
        MANDATORY-GROUPS {
        }
        GROUP cLNS8473Package
        DESCRIPTION
            "Present when the protocol defined in 8473 is used 
             to implement the CLNS."
        GROUP cLNSChecksumPackage
        DESCRIPTION
            "Present when the 8473 Generate Checksum option is 
             implemented."
    MODULE OPENSS7-ISO10589-MIB
        GROUP cLNSISISBasicPackage
        DESCRIPTION
            "The system is an ISO 10589 IS."
        GROUP cLNSISISAuthenticationPackage
        DESCRIPTION
            "The system is an ISO 10589 IS and the 
             authentication procedures are implemented."
        GROUP cLNSISISPartitionRepairPackage
        DESCRIPTION
            "The system is an ISO 10589 Level 2 IS and the 
             partition repair procedures are implemented."
        GROUP cLNSISISLevel2Package
        DESCRIPTION
            "The system is an ISO 10589 Level 2 IS."
        GROUP cLNSISISLevel2AuthenticationPackage
        DESCRIPTION
            "The system is an ISO 10589 Level 2 IS and the 
             authentication procedures are implemented"
    ::= { nlmCompliances 1 }

linkageManagedObjectCompliance MODULE-COMPLIANCE
    STATUS      current
    DESCRIPTION
        "Compliance for the linkage managed object class and 
         corresponding linkageTable."
    MODULE
        MANDATORY-GROUPS {
            nlmLinkagePackage
        }
        GROUP nlmLinkage-ISO9542ISPackage
        DESCRIPTION
                "PRESENT IF support for ISO 9542 operating as an 
                 IS."
        GROUP nlmLinkage-ISO9542ESPackage
        DESCRIPTION
                "PRESENT IF support for ISO 9542 operating as an 
                 ES."
        GROUP nlmLinkageInitialMinimumTimerPackage
        DESCRIPTION
                "PRESENT IF support for the initial minimum 
                 timer attribute of an ISO 8208 or CO Datalink 
                 Service SNDCF for ISO 8473."
        GROUP nlmLinkageReserveTimerPackage
        DESCRIPTION
                "PRESENT IF support for the reserve timer 
                 attribute of an ISO 8208 or CO Datalink Service 
                 SNDCF for ISO 8473."
        GROUP nlmLinkageIdleTimerPackage
        DESCRIPTION
                "PRESENT IF support for the idle timer attribute 
                 of an ISO 8208 or CO Datalink Service SNDCF for 
                 ISO 8473."
        GROUP nlmLinkage-ISO8473-ISO8208SNDCFPackage
        DESCRIPTION
                "PRESENT IF support for an ISO 8208 SNDCF for 
                 ISO 8473."
        GROUP nlmLinkageCODLServicePackage
        DESCRIPTION
                "PRESENT IF support for a CO Datalink SNDCF for 
                 ISO8473/Add.3.."
        GROUP isisLinkageISISBasicPackage
        DESCRIPTION
                "PRESENT IF the system is an ISO 10589 IS."
        GROUP isisLinkageISISAuthenticationPackage
        DESCRIPTION
                "PRESENT IF the authentication procedures are 
                 implemented on an ISO 10589 IS."
        GROUP isisLinkageISISBroadcastPackage
        DESCRIPTION
                "PRESENT IF the linkage is a broadcast circuit 
                 on an ISO 10589 IS."
        GROUP isisLinkageISISDACallEstablishmentMetricIncrementPackage
        DESCRIPTION
                "PRESENT IF the linkage is a DA Circuit and 
                 support is implemented for call establishment 
                 metric increment values greater than zero on an 
                 ISO 10589 IS."
        GROUP isisLinkageISISPtToPtPackage
        DESCRIPTION
                "PRESENT IF the linkage is a point to point 
                 circuit on an ISO 10589 IS."
        GROUP isisLinkageISISStaticPackage
        DESCRIPTION
                "PRESENT IF the linkage is an X.25 static 
                 circuit (IN or OUT) on an ISO 10589 IS."
        GROUP isisLinkageISISLevel2Package
        DESCRIPTION
                "PRESENT IF the system is an ISO 10589 level 2 
                 IS."
        GROUP isisLinkageISISLevel2BroadcastPackage
        DESCRIPTION
                "PRESENT IF the linkage is a broadcast circuit 
                 on an ISO 10589 level 2 IS."


