nlmLinkage OBJECT IDENTIFIER
    ::= { nlmTables 3 }

nlmLinkageTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF NlmLinkageEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        ""
    REFERENCE   
        "ITU-T Rec. X.283 (1997) | ISO/IEC 10733 : 1998, 
               linkage MOC"
    ::= { nlmLinkage 1 }

nlmLinkageEntry OBJECT-TYPE
    SYNTAX      NlmLinkageEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "There is one of these MOs associated with each separate 
         provision of the underlying service to the superior 
         protocol machine.  Its definition permits it to be 
         created and deleted explicitly by management operation, 
         but in some systems it will exist inherently and 
         neither creation nor deletion by management operation 
         will be possible.  Name bindings are defined for both 
         cases.
         
         When the linkage is operable, the operationalState 
         shall have the value enabled(); otherwise, it shall 
         have the value disabled().  Transitions of 
         operationalState shall be reported using the 
         stateChange notification.  A linkage MO may be created 
         in the enabled() operational state."
    REFERENCE   
        "ITU-T Rec. X.283 (1997) | ISO/IEC 10733 : 1998, 
               linkage MO"
    INDEX       { nlmLinkageId }
    ::= { nlmLinkageTable 1 }

NlmLinkageEntry ::= SEQUENCE {
    nlmLinkageId                                        SimpleNameType,
    nlmLinkageOperationalState                          OperationalState,
    nlmLinkageAdministrativeState                       AdministrativeState,
    nlmLinkageSnServiceProvider                         LocalDistinguishedName,
    nlmLinkageSnSAP                                     LocalDistinguishedName,
    nlmLinkageOperationalProtocols                      SupportedProtocols,
-- linkage-ISO9542IS-P PACKAGE
    nlmLinkageISO9542OperationalSubsets                 ISO9542Subsets,
    nlmLinkageHoldingTimerMultiplier                    Integer32,
    nlmLinkageISConfigurationTimer                      MilliSeconds,
    nlmLinkageSuggestedESConfigurationTimer             MilliSeconds,
    nlmLinkageRedirectHoldingTime                       RedirectHoldingTime,
    nlmLinkageESReachabilityChanges                     Counter64,
    nlmLinkageInvalid9542PDUs                           Counter64,
-- linkage-ISO9542ES-P PACKAGE
--  nlmLinkageISO9542OperationalSubsets                 ISO9542Subsets,
--  nlmLinkageHoldingTimerMultiplier                    Integer32,
    nlmLinkageManualISSNPAAddress                       ManualISSNPAAddress,
    nlmLinkageDefaultESConfigTimer                      MilliSeconds,
    nlmLinkageActiveESConfigTimer                       MilliSeconds,
    nlmLinkageISReachabilityChanges                     Counter64,
--  nlmLinkageInvalid9542PDUs                           Counter64,
-- linkage-ISO9542Checksum-P PACKAGE
    nlmLinkageEnableChecksum                            TruthValue,
-- linkageInitialMinimumTimer-P PACKAGE
    nlmLinkageInitialMinimumTimer                       MilliSeconds,
-- linkageReserveTimer-P PACKAGE
    nlmLinkageReserveTimer                              MilliSeconds,
-- linkageIdleTimer-P PACKAGE
    nlmLinkageIdleTimer                                 MilliSeconds,
-- linkage-ISO8473-ISO8208SNDCF-P PACKAGE
    nlmLinkageCallsPlaced                               Counter64,
    nlmLinkageCallsFailed                               Counter64,
-- linkageCODLService-P PACKAGE
--  nlmLinkageCallsPlaced                               Counter64,
--  nlmLinkageCallsFailed                               Counter64,
-- "ISO/IEC 10589":linkageISISBasic-P PACKAGE
    nlmLinkageType                                      CircuitType,
    nlmLinkageISISHelloTimer                            MilliSeconds,
    nlmLinkageL1DefaultMetric                           HopMetric,
