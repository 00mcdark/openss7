    nlmPermanentVirtualCircuitDCEChargingDirection      TruthValue,
    nlmPermanentVirtualCircuitDCEOperationalState       OperationalState,
    nlmPermanentVirtualCircuitDCERemoteDTEAddress       DTEAddress,
    nlmPermanentVirtualCircuitDCERemoteLogicalChannel   LogicalChannelId,
    nlmPermanentVirtualCircuitDCERowStatus              RowStatus
}

