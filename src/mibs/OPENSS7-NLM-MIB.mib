    nlmVirtualCallDTEOriginallyCalledAddress    DTEAddress,
    nlmVirtualCallDTERedirectReason             RedirectReason,
    nlmVirtualCallDTERemoteDTEAddress           DTEAddress,
    nlmVirtualCallDTEReverseCharging            TruthValue
}

