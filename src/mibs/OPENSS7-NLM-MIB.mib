    nlmCLNSversion                                  SnmpAdminString,
    nlmCLNSiSType                                   ISType,
    nlmCLNSsystemId                                 SystemId,
    nlmCLNSmaximumPathSplits                        Integer32,
    nlmCLNSminimumLSPTransmissionInterval           MilliSeconds,
    nlmCLNSmaximumLSPGenerationInterval             MilliSeconds,
