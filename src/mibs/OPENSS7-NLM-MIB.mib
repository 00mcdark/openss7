    nlmNSAPSapId                SimpleNameType,
    nlmNSAPSap2Address          NAddresses,
    nlmNSAPUserEntityNames      LocalDistinguishedName,
    nlmNSAPProviderEntityNames  LocalDistinguishedNames,
    nlmNSAPRowStatus            RowStatus
}

nlmNSAPSapId OBJECT-TYPE
