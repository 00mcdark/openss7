nlmSubsystem OBJECT IDENTIFIER
    ::= { nlmTables 1 }

nlmSubsystemTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF NlmSubsystemEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        ""
    REFERENCE   
        "ITU-T Rec. X.283 (1997) | ISO/IEC 10733 : 1998, 
               networkSubsystem MOC"
    ::= { nlmSubsystem 1 }

nlmSubsystemEntry OBJECT-TYPE
    SYNTAX      NlmSubsystemEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Managed object for network layer subsystem.  There is 
         exactly one of these MOs within a system.  It exists to 
         provide a container for the network entity MOs, nSAP 
         MOs and the 'SNPA' MOs as described in clause.
         
         The networkSubsystem managed object cannot be created 
         or deleted explicitly by management operation.  It 
         exists inherently in a system; created and deleted as 
         part of system operation."
    REFERENCE   
        "ITU-T Rec. X.283 (1997) | ISO/IEC 10733 : 1998, 
               networkSubsystem MO"
    INDEX       { nlmSubsystemId }
    ::= { nlmSubsystemTable 1 }

NlmSubsystemEntry ::= SEQUENCE {
    nlmSubsystemId  SnmpAdminString
}

nlmSubsystemId OBJECT-TYPE
    SYNTAX      SnmpAdminString
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        ""
    REFERENCE   
        "ITU-T Rec. X.723 (1993) : ISO/IEC 10165-5 : 1993, 
               subsystemId"
    DEFVAL      { "NetworkSubsystem" }
    ::= { nlmSubsystemEntry 1 }

