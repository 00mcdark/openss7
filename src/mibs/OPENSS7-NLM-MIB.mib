nlmEntity OBJECT IDENTIFIER
    ::= { nlmTables 2 }

nlmEntityTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF NlmEntityEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        ""
    REFERENCE   
        "ITU-T Rec. X.283 (1997) | ISO/IEC 10733 : 1998, 
               networkEntity MOC"
    ::= { nlmEntity 1 }

nlmEntityEntry OBJECT-TYPE
    SYNTAX      NlmEntityEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "The network entity managed object.  There may be 
         multiple instances of these MOs within a system.  Its 
         definition permits it to be deleted and created by 
         management operation, or to created and deleted 
         automatically as part of system operation."
    REFERENCE   
        "ITU-T Rec. X.283 (1997) | ISO/IEC 10733 : 1998, 
               networkEntity MO"
    INDEX       { nlmEntityId }
    ::= { nlmEntityTable 1 }

NlmEntityEntry ::= SEQUENCE {
    nlmEntityId                     SimpleNameType,
    nlmEntityLocalSapNames          SnmpAdminString,
    nlmEntityOperationalState       OperationalState,
    nlmEntityNetworkEntityTitles    NAddresses,
    nlmEntitySystemTypes            SystemTypes,
    nlmEntityRowStatus              RowStatus
}

nlmEntityId OBJECT-TYPE
    SYNTAX      SimpleNameType
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Provides a simple name type for the network entity that 
         acts as an index into the network entity table."
    REFERENCE   
        "ITU-T Rec. X.723 (1993_ | ISO/IEC 10165-5 : 1993, 
               communicationsEntityId"
    ::= { nlmEntityEntry 1 }

nlmEntityLocalSapNames OBJECT-TYPE
    SYNTAX      SnmpAdminString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "The local SAP names are represented as a list of 
         strings, separated by commas.  Under OpenSS7 OSI stack, 
         the local SAP names identify the filesystem path to the 
         STREAMS characer device that provides the network user 
         interface to the local SAP.  For example: /dev/clnp."
    REFERENCE   
        "ITU-T Rec. X.723 (1993) | ISO/IEC 10165-5 : 1993, 
               localSapNames"
    ::= { nlmEntityEntry 2 }

nlmEntityOperationalState OBJECT-TYPE
    SYNTAX      OperationalState
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Provides the operational state of the network entity in 
         accordance with the OperationalState textual 
         convention of the OPENSS7-SMI-MIB and ITU-T Rec. X.721 
         (1992) | ISO/IEC 10165-2 : 1992."
    REFERENCE   
        "ITU-T Rec. X.721 (1992) | ISO/IEC 10165-2 : 1992, 
               operationalState"
    ::= { nlmEntityEntry 3 }

nlmEntityNetworkEntityTitles OBJECT-TYPE
    SYNTAX      NAddresses
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "Provides a list of network entity titles corresponding 
         to the network entity.  Network entity titles are 
         represented as NSAP addresses expressed in zero to 40 
         (event length) hexadecimal characters.  Multiple 
         addresses are separated by commas."
    REFERENCE   
        "ITU-T Rec. X.283 (1997) | ISO/IEC 10733 : 1998, 
               networkEntityTitles ATTRIBUTE"
    ::= { nlmEntityEntry 4 }

nlmEntitySystemTypes OBJECT-TYPE
    SYNTAX      SystemTypes
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "The set of system roles supported by this network 
         entity.  This may be end system, intermeidate system, 
         or both.  The actual role in which a particular 
         instance of the protocol machine is operating is 
         determined by the operationalSystemType attribute of 
         the cLNS or cONS MO."
    REFERENCE   
        "ITU-T Rec. X.283 (1997) | ISO/IEC 10733 : 1998, 
               systemTypes ATTRIBUTE"
    ::= { nlmEntityEntry 5 }

nlmEntityRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "Provides a mechanism whereby management stations may 
         create or delete network entities.  Rows in this table 
         must persist across management system restart/reboot."
    REFERENCE   
        "ITU-T Rec. X.283 (1997) | ISO/IEC 10733 : 1998, 
               networkEntity--networkSubsystem-Management 
               NAME BINDING"
    ::= { nlmEntityEntry 6 }

