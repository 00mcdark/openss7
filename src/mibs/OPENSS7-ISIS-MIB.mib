isisAdjacency OBJECT IDENTIFIER
    ::= { isisTables 1 }

isisAdjacencyTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IsisAdjacencyEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        ""
    REFERENCE   
        "ISO/IEC 10589: 2001, adjacency MANAGED OBJECT CLASS"
    ::= { isisAdjacency 1 }

isisAdjacencyEntry OBJECT-TYPE
    SYNTAX      IsisAdjacencyEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "11.2.6 Adjacency managed object
         
         Created either through the adjacency-linkage Name 
         binding for adjacencies instantiated by protocol 
         operation, or the adjacency-linkage-management name 
         binding for adjacencies created via explicit system 
         management operation."
    REFERENCE   
        "ISO/IEC 10589: 2001, adjacency MANAGED OBJECT"
    INDEX       { nlmLinkageId, isisAdjacencyId }
    ::= { isisAdjacencyTable 1 }

IsisAdjacencyEntry ::= SEQUENCE {
    isisAdjacencyId                         SimpleNameType,
    isisAdjacencyState                      AdjacencyState,
    isisAdjacencyNeighbourSNPAAddress       SNPAAddress,
    isisAdjacencyNeighbourSystemType        NeighbourSystemType,
    isisAdjacencyNeighbourSystemIds         SystemIds,
-- iSAdjacency-P PACKAGE
    isisAdjacencyUsage                      AdjacencyUsage,
    isisAdjacencyAreaAddressesOfNeighbour   AreaAddresses,
    isisAdjacencyHoldingTimer               MilliSeconds,
-- broadcastISAdjacency-P PACKAGE
    isisAdjacencyPriorityOfNeighbour        IntermediateSystemPriority,
-- adjacency-linkage-management NAME BINDING
    isisAdjacencyRowStatus                  RowStatus
}

isisAdjacencyId OBJECT-TYPE
    SYNTAX      SimpleNameType
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "A string which is the Identifier for the Adjacency and 
         which is unique amongst the set of Adjacencies 
         maintained for this linkage. If this is an adjacency 
         created by system management, it is set by the System 
         Manager when the Adjacency is created, otherwise it is 
         generated by the implementation such that it is unique. 
         The set of identifiers containing the leading string 
         'Auto' are reserved for Automatic Adjacencies. An 
         attempt by system management to create an adjacency 
         with such an identifier will cause a reserved name 
         violation!"
    REFERENCE   
        "ISO/IEC 10589: 2001, adjacencyId ATTRIBUTE"
    ::= { isisAdjacencyEntry 1 }

isisAdjacencyState OBJECT-TYPE
    SYNTAX      AdjacencyState
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "The state of the adjacency."
    REFERENCE   
        "ISO/IEC 10589: 2001, adjacencyState ATTRIBUTE"
    ::= { isisAdjacencyEntry 2 }

isisAdjacencyNeighbourSNPAAddress OBJECT-TYPE
    SYNTAX      SNPAAddress
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "The SNPA address of the neighbour system.
         
         Value is supplied either by the protocol machine if the 
         MO is automatically created, or supplied via the CREATE 
         Operation. The value cannot be changed thereafter.
         
         This attribute shall only permit the REPLACE operation 
         to be performed on it while the MO is in the Disabled 
         Operational State. An attempt to perform a REPLACE 
         operation while the MO is in the Enabled Operation 
         State shall fail with the generation of the 
         constraintViolation specific error."
    REFERENCE   
        "ISO/IEC 10589: 2001, neighbourSNPAAddress ATTRIBUTE"
    ::= { isisAdjacencyEntry 3 }

isisAdjacencyNeighbourSystemType OBJECT-TYPE
    SYNTAX      NeighbourSystemType
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "The type of the neighbour system one of: Unknown, End 
         system, Intermediate system, L1 Intermediate system, L2 
         Intermediate system."
    REFERENCE   
        "ISO/IEC 10589: 2001, neighbourSystemType ATTRIBUTE"
    ::= { isisAdjacencyEntry 4 }

isisAdjacencyNeighbourSystemIds OBJECT-TYPE
    SYNTAX      SystemIds
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "For Intermediate system neighbours: Contains the single 
         SystemId of the neighbouring Intermediate system 
         obtained from the Source ID field of the neighbour's 
         IIH PDU. For End system neighbours: Contains the set of 
         system ID(s) of a neighbour End system.
         
         Value is supplied either by the protocol machine if the 
         MO is automatically created, or supplied via the CREATE 
         Operation. The value cannot be changed thereafter."
    REFERENCE   
        "ISO/IEC 10589: 2001, neighbourSystemIds ATTRIBUTE"
    ::= { isisAdjacencyEntry 5 }

-- iSAdjacency-P PACKAGE
isisAdjacencyUsage OBJECT-TYPE
    SYNTAX      AdjacencyUsage
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "The usage of the Adjacency. An Adjacency of type Level 
         1 will be used for Level 1 traffic only. An adjacency 
         of type Level 2 will be used for Level 2 traffic only. 
         An adjacency of type Level 1 and 2 will be used for 
         both Level 1 and Level 2 traffic. There may be two 
         adjacencies (of types Level 1 and Level 2 between the 
         same pair of Intermediate Systems."
    REFERENCE   
        "ISO/IEC 10589: 2001, adjacencyUsage ATTRIBUTE"
    ::= { isisAdjacencyEntry 6 }

isisAdjacencyAreaAddressesOfNeighbour OBJECT-TYPE
    SYNTAX      AreaAddresses
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "This contains the Area Addresses of a neighbour 
         Intermediate System from the IIH PDU."
    REFERENCE   
        "ISO/IEC 10589: 2001, areaAddressesOfNeighbour ATTRIBUTE"
    ::= { isisAdjacencyEntry 7 }

isisAdjacencyHoldingTimer OBJECT-TYPE
    SYNTAX      MilliSeconds
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Holding time for this adjacency updated from the IIH 
         PDUs."
    REFERENCE   
        "ISO/IEC 10589: 2001, holdingTimer ATTRIBUTE"
    ::= { isisAdjacencyEntry 8 }

-- broadcastISAdjacency-P PACKAGE
isisAdjacencyPriorityOfNeighbour OBJECT-TYPE
    SYNTAX      IntermediateSystemPriority
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Priority of neighbour on this adjacency for becoming 
         LAN Level 1 Designated Intermediate System if 
         adjacencyType is L1 Intermediate System or LAN Level 
         2 Designated Intermediate System if adjacencyType is L2 
         Intermediate System."
    REFERENCE   
        "ISO/IEC 10589: 2001, priorityOfNeighbour ATTRIBUTE"
    ::= { isisAdjacencyEntry 9 }

-- adjacency-linkage-management NAME BINDING
isisAdjacencyRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "This Name Binding is used for adjacencies created 
         automatically by operation of the ISO/IEC 10589 
         protocol machine.
         
         This Name Binding is used for adjacencies created by 
         system management."
    REFERENCE   
        "ISO/IEC 10589: 2001, adjacency-linkage-management NAME 
             BINDING"
    ::= { isisAdjacencyEntry 10 }


