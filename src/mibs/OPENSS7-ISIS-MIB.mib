    ::= { isisVirtualAdjacencyTable 1 }

IsisVirtualAdjacencyEntry ::= SEQUENCE {
    isisVirtualAdjacencyNetworkEntityTitle  NAddress,
    isisVirtualAdjacencyMetric              PathMetric,
    isisVirtualAdjacencyRowStatus           RowStatus
}

isisVirtualAdjacencyNetworkEntityTitle OBJECT-TYPE
