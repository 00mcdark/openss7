    REFERENCE   
        "ITU-T Rec. X.282 (1999) | ISO/IEC 10742 : 1994, 
               mlpFramesReceived ATTRIBUTE"
    DEFVAL      { 0 }
    ::= { dlmLAPBDLEEntry 10 }

dlmLAPBDLEmlpFramesSent OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "Counter. Total number of mlp frames Sent.
         
         This is a non-wrapping 64-bit counter that can be reset 
