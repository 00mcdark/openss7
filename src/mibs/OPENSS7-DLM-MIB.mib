        "ITU-T Rec. X.282 (1999) | ISO/IEC 10742 : 1994, 
               timesMT1Expired ATTRIBUTE"
    DEFVAL      { 0 }
    ::= { dlmLAPBDLEEntry 8 }

dlmLAPBDLEiFramesReassignments OBJECT-TYPE
