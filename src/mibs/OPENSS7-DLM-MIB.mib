    REFERENCE   
        "ITU-T Rec. X.282 (1999) | ISO/IEC 10742 : 1994, 
               mlpFramesSent ATTRIBUTE"
    DEFVAL      { 0 }
    ::= { dlmLAPBDLEEntry 11 }

dlmLAPBDLEmlpFramesOutsideWindowGuard OBJECT-TYPE
