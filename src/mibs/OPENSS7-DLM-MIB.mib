-- dlmTables               OBJECT IDENTIFIER ::= { dlmObjects       1 }
-- dlmScalars              OBJECT IDENTIFIER ::= { dlmObjects       1 }
-- dlmNotifs               OBJECT IDENTIFIER ::= { dlmObjects       1 }
-- dlmOids                 OBJECT IDENTIFIER ::= { dlmObjects       1 }

-- dlmCompliances          OBJECT IDENTIFIER ::= { dlmConformance   1 }
-- dlmGroups               OBJECT IDENTIFIER ::= { dlmConformance   2 }

-- dlmSubsystem            OBJECT IDENTIFIER ::= { dlmTables        1 }
-- dlmEntity               OBJECT IDENTIFIER ::= { dlmTables        2 }
-- dlmDLSAP                OBJECT IDENTIFIER ::= { dlmTables        3 }
-- dlmLAPBDLE              OBJECT IDENTIFIER ::= { dlmTables        4 }
-- dlmSLPPM                OBJECT IDENTIFIER ::= { dlmTables        5 }
-- dlmSLPConnection        OBJECT IDENTIFIER ::= { dlmTables        6 }
-- dlmSLPConnectionIVMO    OBJECT IDENTIFIER ::= { dlmTables        7 }
-- dlmMACDLE               OBJECT IDENTIFIER ::= { dlmTables        8 }
-- dlmMAC                  OBJECT IDENTIFIER ::= { dlmTables        9 }
-- dlmLLCDLE               OBJECT IDENTIFIER ::= { dlmTables       10 }
-- dlmLLCCLPM              OBJECT IDENTIFIER ::= { dlmTables       11 }
-- dlmLLCCOPM              OBJECT IDENTIFIER ::= { dlmTables       12 }

DLSap ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION 
        ""
    REFERENCE   
        "ITU-T Rec. X.723 (1993) : ISO/IEC 10165-5 : 1993, 
               DLM.Sap1Address"
    SYNTAX      Integer32 (0..255)

WindowSize ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION 
        "The MLP window size.  When a modulo 8 window is in 
         effect, the syntax has the range (1..7).  When a modulo 
         128 window is in effect, the syntax has the range 
         (1..127)."
    REFERENCE   
        "ITU-T Rec. X.723 (1993) : ISO/IEC 10165-5 : 1993, 
               DLM.WindowSize"
    SYNTAX      Integer32 (1..127)

GuardRegion ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION 
        "Send or receive LP window in the guard region."
    REFERENCE   
        "ITU-T Rec. X.723 (1993) : ISO/IEC 10165-5 : 1993, 
               DLM.MX"
    SYNTAX      Integer32 (0..4095)

dlmNotifications OBJECT IDENTIFIER
    ::= { dlmMIB 0 }

dlmObjects OBJECT IDENTIFIER
    ::= { dlmMIB 1 }

dlmTables OBJECT IDENTIFIER
    ::= { dlmObjects 1 }

-- ==========================================

dlmSubsystem OBJECT IDENTIFIER
    ::= { dlmTables 1 }

dlmSubsystemTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF DlmSubsystemEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        ""
    REFERENCE   
        "ITU-T Rec. x.282 (1999) | ISO/IEC 10742 : 1994, 
               datalinkSubsystem MANAGED OBJECT CLASS"
    ::= { dlmSubsystem 1 }

dlmSubsystemEntry OBJECT-TYPE
    SYNTAX      DlmSubsystemEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "5.3 The Data Link Subsystem managed object
         
         There shall be exactly one of these managed objects 
         within a system.  It exists to provide a container for 
         all managed objects in a system, that relate to the 
         operation of the Datalink layer.
         
         The datalinkSubsystem managed object cannot be created 
         or deleted explicitly by management operation. It 
         exists inherently in a system; created and deleted as 
         part of system operation."
    REFERENCE   
        "ITU-T Rec. x.282 (1999) | ISO/IEC 10742 : 1994, 
               datalinkSubsystem MANAGED OBJECT"
    INDEX       { dlmSubsystemId }
    ::= { dlmSubsystemTable 1 }

DlmSubsystemEntry ::= SEQUENCE {
    dlmSubsystemId  SnmpAdminString
}

dlmSubsystemId OBJECT-TYPE
    SYNTAX      SnmpAdminString
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        ""
    REFERENCE   
        "ITU-T Rec. X.723 (1993) : ISO/IEC 10165-5 : 1993, 
               subsystemId"
    DEFVAL      { "DatalinkSubsystem" }
    ::= { dlmSubsystemEntry 1 }

-- ==========================================

dlmEntity OBJECT IDENTIFIER
    ::= { dlmTables 2 }

dlmEntityTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF DlmEntityEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        ""
    REFERENCE   
        "ITU-T Rec. X.282 (1999) | ISO/IEC 10742 : 1994, 
               datalinkEntity MANAGED OBJECT CLASS"
    ::= { dlmEntity 1 }

dlmEntityEntry OBJECT-TYPE
    SYNTAX      DlmEntityEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "5.4 The Data Link Entity managed object
         
         The generic DLE MO from which protocol- and 
         media-specific DLE MOs may be derived."
    REFERENCE   
        "ITU-T Rec. X.282 (1999) | ISO/IEC 10742 : 1994, 
               datalinkEntity MANAGED OBJECT"
    INDEX       { dlmEntityId }
    ::= { dlmEntityTable 1 }

DlmEntityEntry ::= SEQUENCE {
    dlmEntityId                     SimpleNameType,
    dlmLocalSapNames                SnmpAdminString,
    dlmEntityOperationalState       OperationalState,
    dlmEntityProviderEntityNames    OCTET STRING,
    dlmEntityRowStatus              RowStatus
}

dlmEntityId OBJECT-TYPE
    SYNTAX      SimpleNameType
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Provides a simple name type for the datalink entity 
         that acts as an index into the datalink entity table."
    REFERENCE   
        "ITU-T Rec. X.723 (1993_ | ISO/IEC 10165-5 : 1993, 
               communicationsEntityId"
    ::= { dlmEntityEntry 1 }

dlmLocalSapNames OBJECT-TYPE
    SYNTAX      SnmpAdminString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "The local SAP names are represented as a list of 
         strings, separated by commas.  Under OpenSS7 OSI stack, 
         the local SAP names identify the filesystem path to the 
         STREAMS characer device that provides the datalink user 
         interface to the local SAP.  For example: /dev/dl1."
    REFERENCE   
        "ITU-T Rec. X.723 (1993) | ISO/IEC 10165-5 : 1993, 
               localSapNames"
    ::= { dlmEntityEntry 2 }

dlmEntityOperationalState OBJECT-TYPE
    SYNTAX      OperationalState
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Provides the operational state of the datalink entity in 
         accordance with the OperationalState textual 
         convention of the OPENSS7-SMI-MIB and ITU-T Rec. X.721 
         (1992) | ISO/IEC 10165-2 : 1992."
    REFERENCE   
        "ITU-T Rec. X.721 (1992) | ISO/IEC 10165-2 : 1992, 
               operationalState"
    ::= { dlmEntityEntry 3 }

dlmEntityProviderEntityNames OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "The distinguished names of provider entity managed 
         objects. The managed objects that represent the 
         entities to be used to provide services to this 
         entity."
    REFERENCE   
        "ITU-T Rec. X.282 (1999) | ISO/IEC 10742 : 1994, 
               providerEntityNames ATTRIBUTE"
    ::= { dlmEntityEntry 4 }

dlmEntityRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "The name binding which applies when a datalinkEntity 
         managed object (or an instance of a subclass of the 
         datalinkEntity MO class) can be created by management 
         as a subordinate object of the datalinkSubsystem 
         managed object (or subclass), and deleted by 
         management."
    REFERENCE   
        "ITU-T Rec. X.282 (1999) | ISO/IEC 10742 : 1994, 
               datalinkEntity-datalinkSubsystem-Management NAME 
               BINDING"
    ::= { dlmEntityEntry 5 }

-- ==========================================

dlmDLSAP OBJECT IDENTIFIER
    ::= { dlmTables 3 }

dlmDLSAPTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF DlmDLSAPEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        ""
    REFERENCE   
        "ITU-T Rec. X.282 (1999) | ISO/IEC 10742 : 1994, 
               dLSAP MANAGED OBJECT CLASS"
    ::= { dlmDLSAP 1 }

dlmDLSAPEntry OBJECT-TYPE
    SYNTAX      DlmDLSAPEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "5.5 The Data Link Service Access Point managed object
         
         The dLSAP managed object class is used to represent 
         a service access point at which services are provided 
         by a DLE to the user entity.  Instances of the dLSAP 
         managed object class are contained within instances of 
         subclasses derived from the datalinkEntity managed 
         object class. Constraints on the number of contained 
         dLSAP MOs and any specific semantics of the attributes 
         are given as part of the definition of the containing 
         DLE managed object class.
         
         A dLSAP managed object may be created and deleted 
         explicitly by management operation or created and 
         deleted automatically as part of system operation, e.g. 
         when a user of the Data Link layer service requests and 
         is granted use of the service. The mechanism by which 
         this happens is a local matter and not subject to OSI 
         standardization."
    REFERENCE   
        "ITU-T Rec. X.282 (1999) | ISO/IEC 10742 : 1994, 
               dLSAP MANAGED OBJECT"
    INDEX       { dlmEntityId, dlmDLSAPSapId }
    ::= { dlmDLSAPTable 1 }

DlmDLSAPEntry ::= SEQUENCE {
    dlmDLSAPSapId               SimpleNameType,
    dlmDLSAPSap1Address         DLSap,
    dlmDLSAPUserEntityNames     LocalDistinguishedNames,
    dlmDLSAPRowStatus           RowStatus
}

dlmDLSAPSapId OBJECT-TYPE
    SYNTAX      SimpleNameType
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Provides a simple name for this DLSAP for a datalink 
         entity."
    REFERENCE   
        "ITU-T Rec. X.723 (1993) | ISO/IEC 10165-5 : 1993, 
               sapId ATTRIBUTE"
    ::= { dlmDLSAPEntry 1 }

dlmDLSAPSap1Address OBJECT-TYPE
    SYNTAX      DLSap
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "Provides the DLSAP LSAP on the datalink entity.  This 
         object cannot be changed while the corresponding row 
         status is active(1)."
    REFERENCE   
        "ITU-T Rec. X.723 (1993) | ISO/IEC 10165-5 : 1993, 
               sap1Address ATTRIBUTE"
    ::= { dlmDLSAPEntry 2 }

dlmDLSAPUserEntityNames OBJECT-TYPE
    SYNTAX      LocalDistinguishedNames
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "The user entity names utilizing this DLSAP.  Note that 
         at most one network entity can be associated with 
         a given DLSAP.  When no network entity is associated 
         with this DLSAP, this object will be a null string."
    REFERENCE   
        "ITU-T Rec. X.723 (1993) | ISO/IEC 10165-5 : 1993, 
               userEntityNames ATTRIBUTE"
    DEFVAL      { "" }
    ::= { dlmDLSAPEntry 3 }

dlmDLSAPRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "Provides a mechanism whereby management stations may 
         create or delete DLSAPs.  Rows created by management 
         action in this table must persist across management 
         system restart/reboot."
    REFERENCE   
        "ITU-T Rec. X.723 (1993) | ISO/IEC 10165-5 : 1993, 
               sap1-communicationsEntity NAME BINDING"
    ::= { dlmDLSAPEntry 4 }

-- ==========================================

dlmLAPBDLE OBJECT IDENTIFIER
    ::= { dlmTables 4 }

dlmLAPBDLETable OBJECT-TYPE
    SYNTAX      SEQUENCE OF DlmLAPBDLEEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        ""
    REFERENCE   
        "ITU-T Rec. X.282 (1999) | ISO/IEC 10742 : 1994, 
               lAPBDLE MANAGED OBJECT CLASS"
    ::= { dlmLAPBDLE 1 }

dlmLAPBDLEEntry OBJECT-TYPE
    SYNTAX      DlmLAPBDLEEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "5.6 The LAPB Data Link Entity managed object
         
         There may be multiple instances of the lAPBDLE managed 
         object in a system. Systems not supporting the LAPB 
         Data Link procedures defined in ISO/IEC 7776 are not 
         required to support the lAPBDLE and contained managed 
         objects.
         
         The lAPBDLE managed object has a conditional mlp 
         Package. There may be multiple instances of lAPBDLE MO 
         in a system, both with and without the mlp Package. For 
         those instances in which the package is absent, the mlp 
         procedures do not apply and the lAPBDLE MO may contain 
         at most one sLPPM MO. For those instances in which the 
         package is present, the mlp procedures do apply and the 
         lAPBDLE MO may contain multiple instances of sLPPM MO.
         
         A lAPBDLE MO may contain at most one dLSAP MO. The 
         value of the sap1Address attribute is of no 
         significance. The cardinality of the userEntityNames 
         attribute is limited to one.
         
         A lAPBDLE MO may be created and deleted explicitly by 
         management operation or created and deleted 
         automatically as part of system operation.
         
         When the lAPBDLE MO is operable, the operationalState 
         shall have the value 'enabled'; otherwise it shall have 
         the value 'disabled'.  Transitions of operationalState 
         shall be reported using the stateChange notification."
    REFERENCE   
        "ITU-T Rec. X.282 (1999) | ISO/IEC 10742 : 1994, 
               lAPBDLE MANAGED OBJECT"
    AUGMENTS    { dlmEntityEntry }
    ::= { dlmLAPBDLETable 1 }

DlmLAPBDLEEntry ::= SEQUENCE {
-- mLP-P PACKAGE
    dlmLAPBDLEmT1Timer                          MilliSeconds,
    dlmLAPBDLEmT3Timer                          MilliSeconds,
    dlmLAPBDLEmW                                WindowSize,
    dlmLAPBDLEmXSend                            GuardRegion,
    dlmLAPBDLEmXReceive                         GuardRegion,
-- mT2-P PACKAGE
    dlmLAPBDLEmT2Timer                          MilliSeconds,
-- mLP-Counters-P PACKAGE
    dlmLAPBDLEReceivedMlpResets                 Counter64,
    dlmLAPBDLETimesMT1Expired                   Counter64,
    dlmLAPBDLEiFramesReassignments              Counter64,
    dlmLAPBDLEmlpFramesReceived                 Counter64,
    dlmLAPBDLEmlpFramesSent                     Counter64,
    dlmLAPBDLEmlpFramesOutsideWindowGuard       Counter64,
    dlmLAPBDLEReceivedMlpFramesInGuardRegion    Counter64
}

-- mLP-P PACKAGE
dlmLAPBDLEmT1Timer OBJECT-TYPE
    SYNTAX      MilliSeconds
    UNITS       "ms"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "Value of the ISO/IEC 7776 parameter MT1 (lost frame 
         timer). Unit is seconds."
    REFERENCE   
        "ITU-T Rec. X.282 (1999) | ISO/IEC 10742 : 1994, 
               mT1Timer ATTRIBUTE"
    ::= { dlmLAPBDLEEntry 1 }

dlmLAPBDLEmT3Timer OBJECT-TYPE
    SYNTAX      MilliSeconds
    UNITS       "ms"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "Value of the ISO/IEC 7776 parameter MT3 (mlp reset 
         confirmation timer). Unit is seconds."
    REFERENCE   
        "ITU-T Rec. X.282 (1999) | ISO/IEC 10742 : 1994, 
               mT3Timer ATTRIBUTE"
    ::= { dlmLAPBDLEEntry 2 }

dlmLAPBDLEmW OBJECT-TYPE
    SYNTAX      WindowSize
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "MLP window size."
    REFERENCE   
        "ITU-T Rec. X.282 (1999) | ISO/IEC 10742 : 1994, 
               mW ATTRIBUTE"
    ::= { dlmLAPBDLEEntry 3 }

dlmLAPBDLEmXSend OBJECT-TYPE
    SYNTAX      GuardRegion
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "Receive MLP window in the Guard Region."
    REFERENCE   
        "ITU-T Rec. X.282 (1999) | ISO/IEC 10742 : 1994, 
               mX ATTRIBUTE"
    ::= { dlmLAPBDLEEntry 4 }

dlmLAPBDLEmXReceive OBJECT-TYPE
    SYNTAX      GuardRegion
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "Receive MLP window in the Guard Region."
    REFERENCE   
        "ITU-T Rec. X.282 (1999) | ISO/IEC 10742 : 1994, 
               mX ATTRIBUTE"
    ::= { dlmLAPBDLEEntry 5 }

-- mT2-P PACKAGE
dlmLAPBDLEmT2Timer OBJECT-TYPE
    SYNTAX      MilliSeconds
    UNITS       "ms"
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "Value of the ISO/IEC 7776 parameter MT2 (group busy 
         timer). Unit is seconds."
    REFERENCE   
        "ITU-T Rec. X.282 (1999) | ISO/IEC 10742 : 1994, 
               mT2Timer ATTRIBUTE"
    ::= { dlmLAPBDLEEntry 6 }

-- mLP-Counters-P PACKAGE
dlmLAPBDLEReceivedMlpResets OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION 
        "Counter. Number of received MLP Resets.
         
         This is a non-wrapping 64-bit counter that can be reset 
         by writing a zero (0) to the object."
    REFERENCE   
        "ITU-T Rec. X.282 (1999) | ISO/IEC 10742 : 1994, 
               receivedMlpResets ATTRIBUTE"
    DEFVAL      { 0 }
    ::= { dlmLAPBDLEEntry 7 }

dlmLAPBDLETimesMT1Expired OBJECT-TYPE
