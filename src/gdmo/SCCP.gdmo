	SUBORDINATE OBJECT CLASS
		sccp AND SUBCLASSES;
	NAMED BY
	SUPERIOR OBJECT CLASS
		"ITU-T Recommendation Q.751.1 (1995)":managedSwitchingElement AND SUBCLASSES;
	WITH ATTRIBUTE
		"ITU-T Rec. X.723 (1993)":communicationsEntityId;
	CREATE;
	DELETE;
REGISTERED AS { sccpNameBinding 3 };
