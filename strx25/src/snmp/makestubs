MIBS=+OPENSS7-X25-PLP-MIB:OPENSS7-SMI-MIB
MIBDIRS=../mibs:./mibs:/usr/share/snmp/mibs
MIBFILES=../mibs/x25-1.mib:./mibs/OPENSS7-SMI-MIB.mib

export MIBS
export MIBDIRS
export MIBFILES

all: simple storage openss7

check: x25MIB.tree

x25MIB.tree: ../mibs/x25-1.mib
	snmptranslate -Tp OPENSS7-X25-PLP-MIB::x25PLPMIB 2>&1 | tee $@

simple: x25MIB_simple.c x25MIB_simple.h

x25MIB_simple.c x25MIB_simple.h: mib2c mib2c.conf ../mibs/x25-1.mib
	./mib2c -c ./mib2c.conf -f x25MIB_simple x25PLPMIB
	indent -nhnl x25MIB_simple.[ch]

storage: x25MIB_storage.c x25MIB_storage.h

x25MIB_storage.c x25MIB_storage.h: mib2c mib2c.storage.conf ../mibs/x25-1.mib
	./mib2c -c ./mib2c.storage.conf -f x25MIB_storage x25PLPMIB
	indent -nhnl x25MIB_storage.[ch]

openss7: x25MIB_openss7.c x25MIB_openss7.h

x25MIB_openss7.c x25MIB_openss7.h: mib2c mib2c.openss7.conf ../mibs/x25-1.mib
	./mib2c -c ./mib2c.openss7.conf -f x25MIB_openss7 x25PLPMIB
	indent -nhnl x25MIB_openss7.[ch]

clean:
	rm -f x25MIB_simple.[ch]
	rm -f x25MIB_storage.[ch]
	rm -f x25MIB_openss7.[ch]
	rm -f *Table.[ch]
	rm -f x25MIB_simple.[ch]~
	rm -f x25MIB_storage.[ch]~
	rm -f x25MIB_openss7.[ch]~
	rm -f *Table.[ch]~
	rm -f x25MIB.tree

.PHONY: all clean check openss7 basic storage
