MIBS=
MIBDIRS=./mibs:/usr/share/snmp/mibs
MIBFILES=

export MIBS
export MIBDIRS
export MIBFILES

all: basic storage openss7

check: \
	x400pMIB.tree \
	mxMIB.tree \
	phyMIB.tree \
	ds1.tree \
	ds1EXT.tree

x400pMIB.tree: MIBS+=OPENSS7-X400P-MX-MIB
x400pMIB.tree: mibs/OPENSS7-X400P-MX-MIB.mib
	snmptranslate -Tp OPENSS7-X400P-MX-MIB::x400pMIB >x400pMIB.tree

mxMIB.tree: MIBS+=OPENSS7-MX-MIB:PerfHist-TC-MIB
mxMIB.tree: mibs/OPENSS7-MX-MIB.mib
	snmptranslate -Tp OPENSS7-MX-MIB::mxMIB >mxMIB.tree

phyMIB.tree: MIBS+=OPENSS7-PHY-MIB
phyMIB.tree: mibs/OPENSS7-PHY-MIB.mib
	snmptranslate -Tp OPENSS7-PHY-MIB::phyMIB >phyMIB.tree

ds1.tree: MIBS+=DS1-MIB:PerfHist-TC-MIB
ds1.tree: mibs/RFC4805-MIB.txt
	snmptranslate -Tp DS1-MIB::ds1>ds1.tree

ds1EXT.tree: MIBS+=DS1-EXT-MIB:PerfHist-TC-MIB
ds1EXT.tree: mibs/RFC4805-EXT-MIB.mib
	snmptranslate -Tp DS1-EXT-MIB::ds1EXT >ds1EXT.tree

basic: \
	x400pMIB_simple.c x400pMIB_simple.h \
	mxMIB_simple.c mxMIB_simple.h \
	phyMIB_simple.c phyMIB_simple.h \
	ds1_simple.c ds1_simple.h \
	ds1EXT_simple.c ds1EXT_simple.h

x400pMIB_simple.c x400pMIB_simple.h: MIBS=+OPENSS7-X400P-MX-MIB
x400pMIB_simple.c x400pMIB_simple.h: mib2c mib2c.conf mibs/OPENSS7-X400P-MX-MIB.mib
	./mib2c -c ./mib2c.conf -f x400pMIB_simple x400pMIB
	indent -nhnl x400pMIB_simple.[ch]

mxMIB_simple.c mxMIB_simple.h: MIBS=+OPENSS7-MX-MIB:PerfHist-TC-MIB
mxMIB_simple.c mxMIB_simple.h: mib2c mib2c.conf mibs/OPENSS7-MX-MIB.mib
	./mib2c -c ./mib2c.conf -f mxMIB_simple mxMIB
	indent -nhnl mxMIB_simple.[ch]

phyMIB_simple.c phyMIB_simple.h: MIBS=+OPENSS7-PHY-MIB
phyMIB_simple.c phyMIB_simple.h: mib2c mib2c.conf mibs/OPENSS7-PHY-MIB.mib
	./mib2c -c ./mib2c.conf -f phyMIB_simple phyMIB
	indent -nhnl phyMIB_simple.[ch]

ds1_simple.c ds1_simple.h: MIBS+=DS1-MIB:PerfHist-TC-MIB
ds1_simple.c ds1_simple.h: mib2c mib2c.conf mibs/RFC4805-MIB.txt
	./mib2c -c ./mib2c.conf -f ds1_simple ds1
	indent -nhnl ds1_simple.[ch]

ds1EXT_simple.c ds1EXT_simple.h: MIBS+=DS1-EXT-MIB:PerfHist-TC-MIB
ds1EXT_simple.c ds1EXT_simple.h: mib2c mib2c.conf mibs/RFC4805-EXT-MIB.mib
	./mib2c -c ./mib2c.conf -f ds1EXT_simple ds1EXT
	indent -nhnl ds1EXT_simple.[ch]

storage: \
	x400pMIB_storage.c x400pMIB_storage.h \
	mxMIB_storage.c mxMIB_storage.h \
	phyMIB_storage.c phyMIB_storage.h \
	ds1_storage.c ds1_storage.h \
	ds1EXT_storage.c ds1EXT_storage.h

x400pMIB_storage.c x400pMIB_storage.h: MIBS=+OPENSS7-X400P-MX-MIB
x400pMIB_storage.c x400pMIB_storage.h: mib2c mib2c.storage.conf mibs/OPENSS7-X400P-MX-MIB.mib
	./mib2c -c ./mib2c.storage.conf -f x400pMIB_storage x400pMIB
	indent -nhnl x400pMIB_storage.[ch]

mxMIB_storage.c mxMIB_storage.h: MIBS=+OPENSS7-MX-MIB:PerfHist-TC-MIB
mxMIB_storage.c mxMIB_storage.h: mib2c mib2c.storage.conf mibs/OPENSS7-MX-MIB.mib
	./mib2c -c ./mib2c.storage.conf -f mxMIB_storage mxMIB
	indent -nhnl mxMIB_storage.[ch]

phyMIB_storage.c phyMIB_storage.h: MIBS=+OPENSS7-PHY-MIB
phyMIB_storage.c phyMIB_storage.h: mib2c mib2c.storage.conf mibs/OPENSS7-PHY-MIB.mib
	./mib2c -c ./mib2c.storage.conf -f phyMIB_storage phyMIB
	indent -nhnl phyMIB_storage.[ch]

ds1_storage.c ds1_storage.h: MIBS+=DS1-MIB:PerfHist-TC-MIB
ds1_storage.c ds1_storage.h: mib2c mib2c.storage.conf mibs/RFC4805-MIB.txt
	./mib2c -c ./mib2c.storage.conf -f ds1_storage ds1
	indent -nhnl ds1_storage.[ch]

ds1EXT_storage.c ds1EXT_storage.h: MIBS+=DS1-EXT-MIB:PerfHist-TC-MIB
ds1EXT_storage.c ds1EXT_storage.h: mib2c mib2c.storage.conf mibs/RFC4805-EXT-MIB.mib
	./mib2c -c ./mib2c.storage.conf -f ds1EXT_storage ds1EXT
	indent -nhnl ds1EXT_storage.[ch]

openss7: \
	x400pMIB_openss7.c x400pMIB_openss7.h \
	mxMIB_openss7.c mxMIB_openss7.h \
	phyMIB_openss7.c phyMIB_openss7.h \
	ds1_openss7.c ds1_openss7.h \
	ds1EXT_openss7.c ds1EXT_openss7.h

x400pMIB_openss7.c x400pMIB_openss7.h: MIBS=+OPENSS7-X400P-MX-MIB
x400pMIB_openss7.c x400pMIB_openss7.h: mib2c mib2c.openss7.conf mibs/OPENSS7-X400P-MX-MIB.mib
	./mib2c -c ./mib2c.openss7.conf -f x400pMIB_openss7 x400pMIB
	indent -nhnl x400pMIB_openss7.[ch]

mxMIB_openss7.c mxMIB_openss7.h: MIBS=+OPENSS7-MX-MIB:PerfHist-TC-MIB
mxMIB_openss7.c mxMIB_openss7.h: mib2c mib2c.openss7.conf mibs/OPENSS7-MX-MIB.mib
	./mib2c -c ./mib2c.openss7.conf -f mxMIB_openss7 mxMIB
	indent -nhnl mxMIB_openss7.[ch]

phyMIB_openss7.c phyMIB_openss7.h: MIBS=+OPENSS7-PHY-MIB
phyMIB_openss7.c phyMIB_openss7.h: mib2c mib2c.openss7.conf mibs/OPENSS7-PHY-MIB.mib
	./mib2c -c ./mib2c.openss7.conf -f phyMIB_openss7 phyMIB
	indent -nhnl phyMIB_openss7.[ch]

ds1_openss7.c ds1_openss7.h: MIBS+=DS1-MIB:PerfHist-TC-MIB
ds1_openss7.c ds1_openss7.h: mib2c mib2c.openss7.conf mibs/RFC4805-MIB.txt
	./mib2c -c ./mib2c.openss7.conf -f ds1_openss7 ds1
	indent -nhnl ds1_openss7.[ch]

ds1EXT_openss7.c ds1EXT_openss7.h: MIBS+=DS1-EXT-MIB:PerfHist-TC-MIB
ds1EXT_openss7.c ds1EXT_openss7.h: mib2c mib2c.openss7.conf mibs/RFC4805-EXT-MIB.mib
	./mib2c -c ./mib2c.openss7.conf -f ds1EXT_openss7 ds1EXT
	indent -nhnl ds1EXT_openss7.[ch]

tables:
	./mib2c -c ./mib2c.storage.conf x400pSyncTable
	./mib2c -c ./mib2c.storage.conf x400pCardTable
	./mib2c -c ./mib2c.storage.conf x400pSpanTable
	./mib2c -c ./mib2c.storage.conf x400pChanTable
	indent -nhnl str*Table.[ch]

clean:
	rm -f x400pMIB.tree
	rm -f x400pMIB_simple.[ch]
	rm -f x400pMIB_storage.[ch]
	rm -f x400pMIB_openss7.[ch]
	rm -f x400pMIB_simple.[ch]~
	rm -f x400pMIB_storage.[ch]~
	rm -f x400pMIB_openss7.[ch]~
	rm -f mxMIB.tree
	rm -f mxMIB_simple.[ch]
	rm -f mxMIB_storage.[ch]
	rm -f mxMIB_openss7.[ch]
	rm -f mxMIB_simple.[ch]~
	rm -f mxMIB_storage.[ch]~
	rm -f mxMIB_openss7.[ch]~
	rm -f phyMIB.tree
	rm -f phyMIB_simple.[ch]
	rm -f phyMIB_storage.[ch]
	rm -f phyMIB_openss7.[ch]
	rm -f phyMIB_simple.[ch]~
	rm -f phyMIB_storage.[ch]~
	rm -f phyMIB_openss7.[ch]~
	rm -f ds1.tree
	rm -f ds1_simple.[ch]
	rm -f ds1_storage.[ch]
	rm -f ds1_openss7.[ch]
	rm -f ds1_simple.[ch]~
	rm -f ds1_storage.[ch]~
	rm -f ds1_openss7.[ch]~
	rm -f ds1EXT.tree
	rm -f ds1EXT_simple.[ch]
	rm -f ds1EXT_storage.[ch]
	rm -f ds1EXT_openss7.[ch]
	rm -f ds1EXT_simple.[ch]~
	rm -f ds1EXT_storage.[ch]~
	rm -f ds1EXT_openss7.[ch]~
