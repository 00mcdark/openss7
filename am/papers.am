AM_V_null = `$(ECHO_Q) ' >/dev/null'`

.tex.dvi:
	$(AM_V_GEN)eval "$(LATEX) `echo $< | sed -e 's,.*/,,'` $(AM_V_null) || :"
	$(AM_V_at)eval "$(BIBTEX) `echo $< | sed -e 's,.*/,,;s,\.tex,,'` $(AM_V_null)"
	$(AM_V_at)eval "$(LATEX) `echo $< | sed -e 's,.*/,,'` $(AM_V_null)"
	$(AM_V_at)eval "$(BIBTEX) `echo $< | sed -e 's,.*/,,;s,\.tex,,'` $(AM_V_null)"
	$(AM_V_at)eval "$(LATEX) `echo $< | sed -e 's,.*/,,'` $(AM_V_null)"

.tex.ps:
	$(AM_V_GEN)eval "$(PSLATEX) `echo $< | sed -e 's,.*/,,'` $(AM_V_null) || :"
	$(AM_V_at)eval "$(BIBTEX) `echo $< | sed -e 's,.*/,,;s,\.tex,,'` $(AM_V_null)"
	$(AM_V_at)eval "$(PSLATEX) `echo $< | sed -e 's,.*/,,'` $(AM_V_null)"
	$(AM_V_at)eval "$(BIBTEX) `echo $< | sed -e 's,.*/,,;s,\.tex,,'` $(AM_V_null)"
	$(AM_V_at)eval "$(PSLATEX) `echo $< | sed -e 's,.*/,,'` $(AM_V_null)"
	$(AM_V_at)$(DVI2PS)`$(ECHO_Q) ' -q'` -t letter -f -o $@ `echo $< | sed -e 's,.*/,,;s,\.tex,\.dvi,'`

.tex.pdf:
	$(AM_V_GEN)eval "$(PDFLATEX) `echo $< | sed -e 's,.*/,,'` $(AM_V_null) || :"
	$(AM_V_at)eval "$(BIBTEX) `echo $< | sed -e 's,.*/,,;s,\.tex,,'` $(AM_V_null)"
	$(AM_V_at)eval "$(PDFLATEX) `echo $< | sed -e 's,.*/,,'` $(AM_V_null)"
	$(AM_V_at)eval "$(BIBTEX) `echo $< | sed -e 's,.*/,,;s,\.tex,,'` $(AM_V_null)"
	$(AM_V_at)eval "$(PDFLATEX) `echo $< | sed -e 's,.*/,,'` $(AM_V_null)"

.tex.html:
	$(AM_V_GEN)eval "$(LATEX2HTML) -split 0 -noexternal_images -noascii_mode -font_size 12pt \
		-nonavigation -numbered_footnotes -nosubdir -rooted -noinfo \
		-norooted -antialias_text -antialias -white -image_type png \
		-scalable_fonts`$(ECHO_Q) ' -verbosity 0'` `echo $< | sed -e 's,.*/,,'` $(AM_V_null)"

