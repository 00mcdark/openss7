	if test ! $$b -ef $(srcdir)/$$b; then \
		$(ECHO) "rm -f -- '$$b'"; \
		rm -f -- "$$b"; \
	fi
	@d=images; \
	$(ECHO) "rm -f -- '$$d.aux' '$$d.bbl' '$$d.blg' '$$d.glo' '$$d.log' '$$d.pl' '$$d.tex' img*.png"; \
	rm -f -- "$$d.aux" "$$d.bbl" "$$d.blg" "$$d.glo" "$$d.log" "$$d.pl" "$$d.tex" img*.png

