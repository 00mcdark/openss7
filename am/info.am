txts-am:
	@$(ECHO_V) "D: Making $@ in `pwd`"
	@txts="$(FIG_TXTS) $(PNG_TXTS) $(PS_TXTS)" ; for i in $$txts ; do \
		if test ! -f $$i -o $(srcdir)/$$i -nt $$i ; then \
			$(ECHO_Q) "  GEN   " $$i ; \
			if test -f $(srcdir)/$$i ; then \
				$(ECHO_V) "cp -pf $(srcdir)/$$i $$i" ; \
				cp -pf $(srcdir)/$$i $$i ; \
			else \
				$(ECHO_V) "touch $$i" ; \
				touch $$i ; \
			fi ; \
		fi ; \
	done

