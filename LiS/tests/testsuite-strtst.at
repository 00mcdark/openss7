m4_include([package.m4])

AT_INIT([strtst])
AT_TESTED([strtst])

AT_BANNER([Open/close test [[MT]]:])
AT_SETUP([strtst open])
AT_KEYWORDS([streams open])
AT_CHECK([
    if test `id -u` -ne 0 ; then exit 77 ; fi
    if test $at_verbose = echo ; then options="-vvv" ; fi
    strtst open
    ], [], [ignore], [], [], [])
AT_CLEANUP

