m4_divert_push([DEFAULTS])
test_pipe() {
    test_prog test-pipe "$@"
}
test_pipe_root() {
    test_prog_root test-pipe "$@"
}
m4_divert_pop([DEFAULTS])

AT_TESTED([test-pipe])

AT_BANNER([PIPE Test])

