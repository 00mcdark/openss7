#ifndef __SYS_@PACKAGE_UCNAME@_VERSION_H__
#define __SYS_@PACKAGE_UCNAME@_VERSION_H__

#ident "@(#) $RCSfile: version.h.in,v $ $Name:  $($Revision: 0.9.2.3 $) Copyright (c) 2001-2008 OpenSS7 Corporation."

/*
 * @configure_input@
 *
 * This file contains the autoconf generated version number definition for the
 * installed package.  It will be installed in in the standard package header
 * location /usr/include/@PACKAGE@/sys/@PACKAGE@/version.h and can be included by
 * header files installed in /usr/include/@PACKAGE@/ with an include line
 * something like:
 *
 * #include "sys/@PACKAGE@/version.h".
 */

#define @PACKAGE_UCNAME@_VERSION "@PACKAGE_RPMEPOCH@:@VERSION@-@PACKAGE_RPMRELEASE@"

#endif				/* __SYS_@PACKAGE_UCNAME@_VERSION_H__ */
