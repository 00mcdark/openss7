.TH INSTALL_DRIVER 9 "@PACKAGE_DATE@" "@PACKAGE@-@VERSION@" "@PACKAGE_TITLE@ DDI/DKI"
.\"
.\"
.SH NAME
.B install_driver
\- install a device driver
.\"
.\"
.SH SYNOPSIS
.PP
.B #define _SUN_SOURCE
.br
.B #include <sys/ddi.h>
.br
.B #include <sys/hpuxddi.h>
.HP
int \fBinstall_driver\fR(drv_info_t *\fIdinfo\fR, drv_ops_t *\fIdops\fR);
.\"
.\"
.SH ARGUMENTS
.PP
.TP
.I dinfo
A pointer to a
.B drv_info
structure describing the device.
.TP
.I dops
A pointer to a
.B drv_ops
structure describing the driver operations.
.\"
.\"
.SH DESCRIPTION
.PP
.BR install_driver ()
.\"
.\"
.PP
The
.B drv_info
structure is as follows:
.RS
.nf
\fC\s-1
typedef struct drv_info {
        char *  di_name;
        char *  di_class;
        int     di_flags;
        int     di_block_major;
        int     di_char_major;
        void *  di_cdio;
        void *  di_gio_private;
        void *  di_cdio_private;
} drv_info_t;
\s+1\fP
.fi
.TP \w'\ di_cdio_private\ 'u
.I di_name
.PD 0
.TP
.I di_class
.TP
.I di_flags
.TP
.I di_block_major
.TP
.I di_char_major
.TP
.I di_cdio
.TP
.I di_gio_private
.TP
.I di_cdio_private
.PD
.RE
.\"
.\"
.PP
The
.B drv_ops
structure is as follows:
.RS
.nf
\fC\s-1
typedef struct drv_ops {
        (*d_open)();
        (*d_close)();
        (*d_strategy)();
        (*d_dump)();
        (*d_psize)();
        (*d_mount)();
        (*d_read)();
        (*d_write)();
        (*d_ioctl)();
        (*d_select)();
        (*d_option1)();
        (*d_res1)();
        (*d_res2)();
        (*d_res3)();
        (*d_res4)();
        int d_flags;
} drv_ops_t;
\s+1\fP
.fi
.TP \w'\ d_strategy\ 'u
.I d_open
.PD 0
.TP
.I d_close
.TP
.I d_strategy
.TP
.I d_dump
.TP
.I d_psize
.TP
.I d_mount
.TP
.I d_read
.TP
.I d_write
.TP
.I d_ioctl
.TP
.I d_select
.TP
.I d_option1
.TP
.I d_res1
.TP
.I d_res2
.TP
.I d_res3
.TP
.I d_res4
.TP
.I d_flags
.PD
.RE
.\"
.\"
.SH RETURN
.PP
When
.BR install_driver ()
succeeds, it returns 
.BR DDI_SUCCESS ;
when it fails, it returns
.BR DDI_FAILURE .
.\"
.\"
.SH ERRORS
.PP
.BR install_driver ()
fails when 
