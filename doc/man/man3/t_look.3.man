.BR t_rcvrel (3),
.BR t_rcvreldata (3),
.BR t_rcvv (3),
.BR t_rcvvudata (3),
.BR t_removeleaf (3),
.BR t_snd (3),
.BR t_snddis (3),
.BR t_sndrel (3),
.BR t_sndreldata (3),
.BR t_sndudata (3),
.BR t_sndv (3),
.BR t_sndvudata (3),
.BR t_unbind (3).
.\"
.\"
.SH "BUGS"
.PP
.BR t_look ()
has no known bugs.
.BR t_look ()
has been tested for the described behavior using the
.BR test-xnet (8)
and
.BR test-xnet_thr (8)
conformance test suites.
.\"
.\"
