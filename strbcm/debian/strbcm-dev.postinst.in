@mscript_header@

function configure_dev_postinst() { :
    if test -x '/usr/sbin/@STRMAKENODES@' ; then
	# ensure that devices are added
	echo "@PACKAGE@-${subpkg}(@VERSION@-@PACKAGE_DEBRELEASE@): creating devices."
	/usr/sbin/@STRMAKENODES@
    fi
}

subpkg=dev

@mscript_postinst@

