X25SVCConfigEntry ::=
	SEQUENCE {
		x25SVCConfigId
			INTEGER,
		x25SVCConfigChannel
			LogicalChannelNumber,
		x25SVCConfigDirection
			INTEGER,
		x25SVCConfigRemoteDTEAddress
			DTEAddress,
		x25SVCConfigThroughputClass
			Throughput,
		x25SVCConfigRedirectReason
			RedirectReason,
		x25SVCConfigOriginallyCalledAddress
			DTEAddress,
		x25SVCConfigCallingAddressExtension
			NetworkAddress,
