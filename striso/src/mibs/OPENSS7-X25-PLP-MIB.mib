x25SVCConfigDirection OBJECT-TYPE
	SYNTAX		INTEGER { incoming(0), outgoing(1) }
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
		"The direction (incoming(0) or outgoing(1)) of the call."
	REFERENCE	"ISO/IEC 10733 : direction."
	::= { x25SVCConfigEntry 3 }

x25SVCConfigRemoteDTEAddress OBJECT-TYPE
	SYNTAX		DTEAddress
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
		"The DTE Address of the remote DTE.  In the case of an 
		 outgoing call, this is the remote DTE address from the 
		 called address of the transmitted call request packet.  
		 In the case of an incoming call, it is the calling 
		 address from the received call request packet."
	REFERENCE	"ISO/IEC 10733 remoteDTEAddress."
	::= { x25SVCConfigEntry 4 }

x25SVCConfigThroughputClass OBJECT-TYPE
	SYNTAX		Throughput
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
		"The actual through class in used for the call.  For 
		 SVCs this is the result of negotiation."
	REFERENCE	"ISO/IEC 10733 : throughputClass."
	::= { x25SVCConfigEntry 5 }

x25SVCConfigRedirectReason OBJECT-TYPE
	SYNTAX		RedirectReason
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
		"The reason for call redirect.  The zero value indicates 
		 that the call was not redirected."
	REFERENCE	"ISO/IEC 10733 : redirectReason."
	::= { x25SVCConfigEntry 6 }

x25SVCConfigOriginallyCalledAddress OBJECT-TYPE
	SYNTAX		DTEAddress
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
		"The originally called address."
	REFERENCE	"ISO/IEC 10733 : originallyCalledAddress."
	::= { x25SVCConfigEntry 7 }

x25SVCConfigCallingAddressExtension OBJECT-TYPE
	SYNTAX		NetworkAddress
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
		"The contents of the calling address extension field.

		 In the OSI context, this will always be an NSAP address 
		 but in other uses it may not.  In any case, it may be 
		 null, for example, when used by ISO 8473."
	REFERENCE	""
	::= { x25SVCConfigEntry 8 }

x25SVCConfigCalledAddressExtension OBJECT-TYPE
	SYNTAX		NetworkAddress
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
		"The contents of the called address extension field.

		 In the OSI context, this will always be an NSAP address 
		 but in other uses it may not.  In any case, it may be 
		 null, for example, when used by ISO 8473."
	REFERENCE	""
	::= { x25SVCConfigEntry 9 }

