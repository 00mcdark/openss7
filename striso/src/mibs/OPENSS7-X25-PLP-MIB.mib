OPENSS7-X25-PLP-MIB DEFINITIONS ::= BEGIN

IMPORTS
	MODULE-IDENTITY, OBJECT-TYPE, Counter32, Gauge32, Integer32,
	Unsigned32, enterprises
		FROM SNMPv2-SMI
	TEXTUAL-CONVENTION, DisplayString, MODULE-COMPLIANCE,
	OBJECT-GROUP, TimeInterval, TruthValue, RowStatus, RowPointer
		FROM SNMPv2-TC
	AdministativeState, OperationalState, UsageState, 
	ProceduralStatus, AvailabilityStatus, AlarmStatus
		FROM OPENSS7-TMN-TC

openss7		OBJECT IDENTIFIER ::= { enterprises 29591 }
openss7MIBs	OBJECT IDENTIFIER ::= { openss7 1 }
openss7Modules	OBJECT IDENTIFIER ::= { openss7MIBs 1 }

x25-PLP-MIB MODULE-IDENTITY
	LAST-UPDATED
		"20071125221024Z"
	ORGANIZATION
		"The OpenSS7 Project"
	CONTACT-INFO
		"Brian Bidulock
		 OpenSS7 Corporation
		 1469 Jefferys Crescent
		 Edmonton, Alberta T6L 6T1
		 CANADA
		 Tel: +1 780 490 1141
		 Fax: +1 780 490 1241
		 Mob: +1 780 953 0557
		 mailto:bidulock@openss7.org"
	DESCRIPTION
		"This MIB provides configuration and status information 
		 for an X.25 PLP subsystem, where the X.25 PLP is in 
		 accordance with ITU-T Rec.  X.25 | ISO/IEC 8208 and 
		 management information from ITU-T Rec. X.283 | ISO/IEC 
		 10733."
	::= { openss7Modules 3 }

 -- top level organization of the MIB

x25PLPNotifications	 OBJECT IDENTIFIER ::= { x25-PLP-MIB	 0 }
x25PLPObjects		 OBJECT IDENTIFIER ::= { x25-PLP-MIB	 1 }
x25PLPConform		 OBJECT IDENTIFIER ::= { x25-PLP-MIB	 2 }

x25PLPTables		 OBJECT IDENTIFIER ::= { x25PLPObjects	 1 }
x25PLPScalars		 OBJECT IDENTIFIER ::= { x25PLPObjects	 2 }
x25PLPNotifs		 OBJECT IDENTIFIER ::= { x25PLPObjects	 3 }
x25PLPOids		 OBJECT IDENTIFIER ::= { x25PLPObjects	 4 }

x25PLPComplicances	 OBJECT IDENTIFIER ::= { x25PLPConform	 1 }
x25PLPGroups		 OBJECT IDENTIFIER ::= { x25PLPConform	 2 }

x25PLEConfig		 OBJECT IDENTIFIER ::= { x25PLPTables	 1 }
x25PLEProfile		 OBJECT IDENTIFIER ::= { x25PLPTables	 2 }
x25PLEState		 OBJECT IDENTIFIER ::= { x25PLPTables	 3 }
x25PLEStats		 OBJECT IDENTIFIER ::= { x25PLPTables	 4 }

x25VCConfig		 OBJECT IDENTIFIER ::= { x25PLPTables	 5 }
x25VCProfile		 OBJECT IDENTIFIER ::= { x25PLPTables	 6 }
x25VCStats		 OBJECT IDENTIFIER ::= { x25PLPTables	 7 }

x25PVCConfig		 OBJECT IDENTIFIER ::= { x25PLPTables	 8 }
x25SVCConfig		 OBJECT IDENTIFIER ::= { x25PLPTables	 9 }


