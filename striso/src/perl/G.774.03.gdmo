	WITH ATTRIBUTE SYNTAX SDHProtASN1.Boolean;
	MATCHES FOR EQUALITY;
	BEHAVIOUR
		revertiveBeh;
REGISTERED AS { g774-03Attribute 14 };


revertiveBeh BEHAVIOUR
	DEFINED AS
		"This attribute indicates whether the protection scheme is revertive or not. If 
		the value of this attribute is TRUE, the traffic is returned to the protected 
		protectionUnit instance that initiated the switch after the fault clears and the 
		waitToRestoreTime interval (if any) has expired. If the value of this attribute 
		is FALSE, then after the fault has cleared, traffic does not revert to the 
		protectionUnit that initiated the switch.";
