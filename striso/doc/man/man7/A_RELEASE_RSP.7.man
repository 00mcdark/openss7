.SH NAME
.B A_RELEASE_RSP
\- 
.\"
.\"
.SH SYNOPSIS
.PP
.B #include <xap.h>
.HP
.B int ap_snd(
.br
.BI "int " fd ,
.br
.BI "unsigned long " sptype ,
.br
