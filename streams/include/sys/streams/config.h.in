/*
 * @configure_input@
 *
 * This file needs to include any autoconf definitions that are used by
 * installed header files.  This file will be installed in
 * /usr/include/streams/sys/streams/config.h to be included by header files
 * installed in /usr/include/streams/ with an include line something like:
 *
 * #include "sys/streams/config.h".
 */

#if defined __i386__ || defined __x86_64__ || defined __k8__
