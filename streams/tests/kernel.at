AT_SETUP([depmod test])
AT_KEYWORDS([depmod])
AT_CHECK([
    if test `id -u` -ne 0 ; then
	exit 77
    fi
    UNAME_MACHINE=${at_target_cpu} \
	${at_DEPMOD} -ae \
	-b  ${at_DESTDIR}${at_krootdir}/ \
	-C ${at_DESTDIR}${at_krootdir}/etc/modules.conf \
	-F ${at_DESTDIR}${at_ksysmap} \
	${at_kversion}
    ], [], [ignore], [], [], [])
AT_CLEANUP

AT_SETUP([insmod test])
AT_KEYWORDS([insmod])
AT_CHECK([
    if test -z "$KERNEL_MODULES" ; then
	exit 77
    fi
    kmodules=`echo " $KERNEL_MODULES " | sed -e 's| lib[[^ ]]*_a-| |g;s|\.o | |g'`
    echo "$kmodules"
    for mod in $kmodules ; do
	echo $mod
    done
    if test `id -u` -ne 0 ; then
	exit 77
    fi
    if test ! -x /sbin/insmod ; then
	exit 77
    fi
    ], [], [ignore], [], [], [])
AT_CLEANUP

