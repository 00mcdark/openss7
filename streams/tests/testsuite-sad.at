m4_divert_push([DEFAULTS])
test_sad() {
    test_prog test-sad "$@"
}
test_sad_root() {
    test_prog_root test-sad "$@"
}
m4_divert_pop([DEFAULTS])

AT_TESTED([test-sad])

AT_BANNER([SAD Test])

AT_SETUP([sad test 0.1])
AT_KEYWORDS([sad])
AT_CHECK([test_sad 0.1], [], [ignore], [], [], [])
AT_CLEANUP

