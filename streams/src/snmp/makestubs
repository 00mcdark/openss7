MIBS=+OPENSS7-STREAMS-MIB
MIBDIRS=./mibs:/usr/share/snmp/mibs
MIBFILES=./mibs/OPENSS7-STREAMS-MIB.mib

export MIBS
export MIBDIRS
export MIBFILES

all: openss7

check: strMIB.tree

strMIB.tree: mibs/OPENSS7-STREAMS-MIB.mib
	snmptranslate -Tp OPENSS7-STREAMS-MIB::strMIB 2>&1 | tee $@

basic: strMIB_simple.c strMIB_simple.h

strMIB_simple.c strMIB_simple.h: mib2c mib2c.conf mibs/OPENSS7-STREAMS-MIB.mib
	./mib2c -c ./mib2c.conf -f strMIB_simple strMIB
	indent -nhnl strMIB_simple.[ch]

storage: strMIB_storage.c strMIB_storage.h

strMIB_storage.c strMIB_storage.h: mib2c mib2c.storage.conf mibs/OPENSS7-STREAMS-MIB.mib
	./mib2c -c ./mib2c.storage.conf -f strMIB_storage strMIB
	indent -nhnl strMIB_storage.[ch]

openss7: strMIB_openss7.c strMIB_openss7.h

strMIB_openss7.c strMIB_openss7.h: mib2c mib2c.openss7.conf mibs/OPENSS7-STREAMS-MIB.mib
	./mib2c -c ./mib2c.openss7.conf -f strMIB_openss7 strMIB
	indent -nhnl strMIB_openss7.[ch]

tables:
	./mib2c -c ./mib2c.storage.conf strModTable
	./mib2c -c ./mib2c.storage.conf strModInfoTable
	./mib2c -c ./mib2c.storage.conf strModStatTable
	./mib2c -c ./mib2c.storage.conf strStatsTable
	./mib2c -c ./mib2c.storage.conf strApshTable
	indent -nhnl str*Table.[ch]

clean:
	rm -f strMIB_simple.[ch]
	rm -f strMIB_storage.[ch]
	rm -f strMIB_openss7.[ch]
	rm -f strMIB_simple.[ch]~
	rm -f strMIB_storage.[ch]~
	rm -f strMIB_openss7.[ch]~
	rm -f strMIB.tree
